  "raw_meta_yaml": "context:\n  name: msmtp\n  version: \"1.8.32\"\n\nrecipe:\n  name: ${{ name }}\n  version: ${{ version }}\n\nsource:\n  url: https://marlam.de/msmtp/releases/${{ name }}-${{ version }}.tar.xz\n  sha256: 20cd58b58dd007acf7b937fa1a1e21f3afb3e9ef5bbcfb8b4f5650deadc64db4\n\nbuild:\n  number: 0\n  skip: win\n\noutputs:\n  - package:\n      name: ${{ name }}\n    build:\n      script:\n        - autoreconf -ivf\n        - ./configure --prefix=$PREFIX --datarootdir=$PREFIX --with-tls=gnutls --with-libidn --with-libgsasl\n        - make -j${CPU_COUNT}\n        - make check || (cat tests/test-suite.log && exit 1)\n        - make install\n    requirements:\n      build:\n        - ${{ compiler('c') }}\n        - ${{ stdlib('c') }}\n        - automake\n        - make\n        - pkg-config\n        - gettext\n        - texinfo\n        - if: osx\n          then:\n            - xz-tools\n      host:\n        - gnutls\n        - libgsasl >=2.1\n        - if: osx\n          then:\n            - libintl-devel\n      run:\n        - ${{ pin_compatible('libgsasl', upper_bound='x') }}\n    tests:\n      - package_contents:\n          bin:\n            - msmtp\n            - msmtpd\n          files:\n            - man/man1/msmtp.1\n      - script:\n          - msmtp --help\n          - msmtpd --help\n\n  - package:\n      name: ${{ name }}-sendmail\n    build:\n      script:\n        - ln -s msmtp $PREFIX/bin/sendmail\n        - ln -s msmtp.1 $PREFIX/man/man1/sendmail.1\n    requirements:\n      host:\n        - msmtp\n      run:\n        - msmtp\n    tests:\n      - package_contents:\n          bin:\n            - sendmail\n          files:\n            - man/man1/sendmail.1\n    about:\n      summary: Use msmtp as a sendmail alternative\n      description: |\n        Provides an alternative to the 'sendmail' tool. When sendmail is called\n        it will call 'msmtp' instead.\n      license: GPL-3.0-or-later\n\nabout:\n  summary: msmtp is an SMTP client\n  description: |\n    In the default mode, it transmits a mail to an SMTP server which takes care\n    of further delivery.\n\n    To use this program with your mail user agent (MUA), create a configuration\n    file with your mail account(s) and tell your MUA to call msmtp instead of\n    /usr/sbin/sendmail.\n  license: GPL-3.0-or-later\n  license_file: COPYING\n  homepage: https://marlam.de/msmtp/\n  repository: https://git.marlam.de/gitweb/?p=msmtp.git\n\nextra:\n  feedstock-name: msmtp\n  recipe-maintainers:\n    - blmaier\n",
