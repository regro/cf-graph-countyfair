    "about": {
      "description": "Harvest, normalise, and index climate / earth-system metadata from POSIX,\nS3/MinIO, and OpenStack Swift using configurable DRS dialects (CMIP6, CMIP5,\nCORDEX, …). Output to a temporary catalogue (JSONLines) and then index into\nsystems such as Solr or MongoDB. Configuration is TOML with inheritance,\ntemplating, and computed rules.",
      "home": "https://github.com/freva-org/metadata-crawler",
      "license": "BSD-3-Clause",
      "license_family": "BSD-3-Clause",
      "license_file": "LICENSE",
      "summary": "Crawl, extract and push climate metadata for indexing."
    },
    "build": {
      "number": "0",
      "script": {
        "content": [
          "python -m pip install . -vv --no-deps --no-build-isolation",
          "cargo-bundle-licenses --format yaml --output ./THIRDPARTY.yml"
        ],
        "env": {
          "CARGO_PROFILE_RELEASE_LTO": "fat",
          "CARGO_PROFILE_RELEASE_STRIP": "symbols"
        }
      }
    },
    "extra": {
      "recipe-maintainers": [
        "antarcticrainforest",
        "mo-dkrz"
      ]
    },
