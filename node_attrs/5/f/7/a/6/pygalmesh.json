    "about": {
      "description": "pygalmesh is a Python frontend to CGAL’s 3D mesh generation capabilities. pygalmesh makes it easy to create high-quality 3D volume meshes, periodic volume meshes, and surface meshes.\n",
      "dev_url": "https://github.com/nschloe/pygalmesh",
      "doc_url": "https://github.com/nschloe/pygalmesh",
      "home": "https://github.com/nschloe/pygalmesh/",
      "license": "GPL-3.0-or-later",
      "license_family": "GPL",
      "license_file": "LICENSE",
      "summary": "A Python frontend to CGAL’s 3D mesh generators."
    },
    "build": {
      "entry_points": [
        "pygalmesh = pygalmesh._cli:cli"
      ],
      "number": "1"
    },
    "extra": {
      "recipe-maintainers": [
        "nschloe",
        "flothesof"
      ]
    },
    "package": {
      "name": "pygalmesh",
      "version": "0.10.7"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "cxx_compiler_stub"
      ],
      "host": [
        "python",
        "pip",
        "eigen",
