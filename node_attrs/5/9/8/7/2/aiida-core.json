=2.0\n        - tabulate <0.10.0,>=0.8.0\n        - tqdm ~=4.45\n        - typing-extensions ~=4.0\n        - upf_to_json ~=0.9.2\n        - wrapt ~=1.11\n        - chardet ~=5.2.0\n      run_constrained:\n        - {{ pin_subpackage(aiidaservice) }}\n    test:\n      requires:\n        - python {{ python_min }}\n        - pip\n      imports:\n        - aiida\n        - aiida.common\n        - aiida.common.hashing\n        - aiida.manage\n        - aiida.restapi\n        - aiida.schedulers\n        - aiida.transports\n        - aiida.tools\n        - aiida.tools.dbexporters\n        - aiida.tools.dbimporters\n      commands:\n        - verdi --help\n        - verdi --version\n        - pip check\n\n  - name: {{ aiidaservice }}\n    build:\n      noarch: python\n    requirements:\n      run:\n        - postgresql >=9.6\n        - rabbitmq-server >=3.7,<3.8.15\n    test:\n      commands:\n        - postgres --help\n        - rabbitmqctl help\n\n  - name: {{ aiida_atomic_tools }}\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python {{ python_min }}\n      run:\n        - python >={{ python_min }}\n        - {{ pin_subpackage(aiidacore, exact=True) }}\n        - ase ~=3.18\n        - matplotlib-base ~=3.3,>=3.3.4\n        - pycifrw ~=4.4\n        - pymatgen >=2022.1.20\n        - pymysql ~=0.9.3\n        - seekpath ~=1.9,>=1.9.3\n        - spglib >=1.14,<3.0\n    test:\n      requires:\n        - python {{ python_min }}\n        - pip\n      commands:\n        - pip check\n\n  - name: {{ aiida_notebook }}\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python {{ python_min }}\n      run:\n        - python >={{ python_min }}\n        - {{ pin_subpackage(aiidacore, exact=True) }}\n        - jupyter_client ~=8.0\n        - jupyter ~=1.0\n        - notebook ~=6.1,>=6.1.5\n    test:\n      requires:\n        - python {{ python_min }}\n        - pip\n      commands:\n        - pip check\n\n  - name: {{ aiida_rest }}\