  "linux_64_meta_yaml": {
    "about": {
      "description": "HEALPix is a Hierarchical, Equal Area, and iso-Latitude Pixelation of\nthe sphere designed to support efficiently (1) local operations on the\npixel set, (2) a hierarchical tree structure for multi-resolution\napplications, and (3) the global Fast Spherical Harmonic transform.\nHEALPix based mathematical software meets the challenges presented\nby high resolution and large volume data sets, such as the WMAP and\nPlanck CMB mission products.\n",
      "dev_url": "https://sourceforge.net/projects/healpix/",
      "doc_url": "http://healpix.sf.net/documentation.php",
      "home": "https://sourceforge.net/projects/healpix/",
      "license": "GPL-2.0-or-later",
      "license_family": "GPL",
      "license_file": "COPYING",
      "summary": "Software for pixelization, hierarchical indexation, synthesis, analysis, and visualization of data on the sphere."
    },
    "build": {
      "error_overdepending": true,
      "error_overlinking": true,
      "ignore_run_exports": null,
