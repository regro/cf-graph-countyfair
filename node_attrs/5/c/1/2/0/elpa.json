  "feedstock_name": "elpa",
  "hash_type": "sha256",
  "linux_64_meta_yaml": {
    "about": {
      "description": "The computation of selected or all eigenvalues and eigenvectors of a symmetric\n(Hermitian) matrix has high relevance for various scientific disciplines.\nFor the calculation of a significant part of the eigensystem typically direct\neigensolvers are used. For large problems, the eigensystem calculations with\nexisting solvers can become the computational bottleneck.\n",
      "dev_url": "https://gitlab.mpcdf.mpg.de/elpa/elpa",
      "doc_url": "https://elpa.mpcdf.mpg.de/documentation/doxygen/ELPA_DOXYGEN_PAGES/ELPA-2025.06.001/html/index.html",
      "home": "https://elpa.mpcdf.mpg.de/",
      "license": "LGPL-3.0-only",
      "license_file": [
        "LICENSE",
        "COPYING/COPYING",
        "COPYING/gpl.txt",
        "COPYING/lgpl.txt"
      ],
      "summary": "Eigenvalue Solvers for Petaflop-Applications"
    },
    "build": {
      "number": "1",
      "run_exports": [
        "elpa * mpi_mpich_*",
        "elpa",
        "elpa * mpi_openmpi_*"
      ],
      "string": "mpi_openmpi_h1234567_1"
    },
    "extra": {
      "recipe-maintainers": [
        "jan-janssen",
        "awvwgk",
        "yuzie007"
      ]
    },
    "package": {
      "name": "elpa",
      "version": "2025.06.001"
    },
    "requirements": {
      "build": [
        "gnuconfig",
        "c_compiler_stub",
        "cxx_compiler_stub",
        "fortran_compiler_stub",
        "c_stdlib_stub",
        "mpich",
        "scalapack",
        "libblas",
        "liblapack",
        "make",
        "perl",
        "libgomp",
        "openmpi"
      ],
      "host": [
        "mpich",
        "scalapack",
        "libblas",
        "liblapack",
        "openmpi"
      ],
      "run": [
        "mpich",
        "scalapack",
        "openmpi"
      ]
    },
    "schema_version": 0,
    "source": {
      "patches": [
        "fdep-perl.patch",
        "pkgconf.patch",
        "check_thread_affinity.patch",
        "includedir-suffix.patch"
      ],
      "sha256": "feeb1fea1ab4a8670b8d3240765ef0ada828062ef7ec9b735eecba2848515c94",
      "url": "https://elpa.mpcdf.mpg.de/software/tarball-archive/Releases/2025.06.001/elpa-2025.06.001.tar.gz"
    },
    "test": {
      "commands": [
        "test -f $PREFIX/lib/libelpa${SHLIB_EXT}",
        "test -f $PREFIX/lib/libelpa_openmp${SHLIB_EXT}",
        "pkg-config elpa --exact-version 2025.06.001",
        "pkg-config elpa_openmp --exact-version 2025.06.001",
        "elpa2_print_kernels",
        "elpa2_print_kernels_openmp",
        "test -f $PREFIX/lib/libelpa_onenode${SHLIB_EXT}",
        "test -f $PREFIX/lib/libelpa_onenode_openmp${SHLIB_EXT}",
        "pkg-config elpa_onenode --exact-version 2025.06.001",
        "pkg-config elpa_onenode_openmp --exact-version 2025.06.001",
        "elpa2_print_kernels_onenode",
        "elpa2_print_kernels_onenode_openmp"
      ],
      "requires": [
        "pkg-config"
      ]
    }
  },
