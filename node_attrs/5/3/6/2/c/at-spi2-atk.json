  "linux_64_meta_yaml": {
    "about": {
      "description": "This package includes libatk-bridge, a library that bridges ATK to the new\nD-Bus based AT-SPI, as well as a corresponding module for gtk+ 2.x. Gtk+ 3.x\nnow links against libatk-bridge directly rather than requiring it to be loaded\nas a module.\n\nThese libraries depend on the at-spi2-core code that contains the daemon for\nregistering applications, D-Bus helper libraries and the AT-SPI D-Bus specifications.\n",
      "dev_url": "https://gitlab.gnome.org/GNOME/at-spi2-atk",
      "home": "http://www.gtk.org/",
      "license": "LGPL-2.1-or-later",
      "license_family": "LGPL",
      "license_file": "COPYING",
      "summary": "bridge for AT-SPI and ATK accessibility technologies"
    },
    "build": {
      "number": "3",
      "run_exports": [
        "at-spi2-atk"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "ryanvolz",
