  "linux_64_meta_yaml": {
    "about": {
      "description": "PyKafka is a cluster-aware Kafka>=0.8.2 client for Python. It includes\nPython implementations of Kafka producers and consumers, which are\noptionally backed by a C extension built on librdkafka, and runs under\nPython 2.7+, Python 3.4+, and PyPy.\n\nPyKafka’s primary goal is to provide a similar level of abstraction to\nthe JVM Kafka client using idioms familiar to Python programmers and\nexposing the most Pythonic API possible.\n",
      "dev_url": "https://github.com/parsely/pykafka",
      "doc_url": "http://pykafka.readthedocs.org/",
      "home": "http://github.com/parsely/pykafka",
      "license": "Apache-2.0",
      "license_family": "Apache",
      "license_file": "LICENSE",
      "summary": "Apache Kafka client for Python; high-level & low-level consumer/producer, with great performance"
    },
    "build": {
      "number": "1010",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "mrocklin",
        "stuarteberg"
      ]
    },
    "package": {
      "name": "pykafka",
      "version": "2.8.0"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub"
      ],
      "host": [
        "python",
