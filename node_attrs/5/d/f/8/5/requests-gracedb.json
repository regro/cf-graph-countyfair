    "about": {
      "description": "requests-gracedb provides a generic REST API client for GraceDB and similar\nLIGO/Virgo API services. It uses the powerful Requests package for reliable\nand high-throughput HTTP connection pooling.\n",
      "dev_url": "https://git.ligo.org/emfollow/requests-gracedb",
      "doc_url": "https://requests-gracedb.readthedocs.io/",
      "home": "https://git.ligo.org/emfollow/requests-gracedb",
      "license": "GPL-2.0-or-later",
      "license_family": "GPL",
      "license_file": "LICENSE.md",
      "summary": "generic connection pooling HTTP client for GraceDB and similar services"
    },
    "build": {
      "noarch": "python",
      "number": "0",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "duncanmmacleod",
        "lpsinger"
      ]
    },
    "package": {
      "name": "requests-gracedb",
      "version": "0.2.2"
    },
    "requirements": {
      "host": [
        "pip",
        "python 3.9",
        "setuptools >=77",
        "setuptools-scm >=8"
      ],
      "run": [
        "cryptography >=42",
        "igwn-auth-utils >=1.3.0",
        "pyjwt",
        "python >=3.9",
        "requests",
        "safe-netrc"
      ]
    },
    "schema_version": 0,
