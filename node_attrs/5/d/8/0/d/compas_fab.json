  "linux_64_meta_yaml": {
    "about": {
      "description": "Robotic fabrication package for the COMPAS Framework facilitating the\nplanning and execution of robotic fabrication processes. It provides\ninterfaces to existing software libraries and tools available in the\nfield of robotics (e.g. OMPL, ROS, PyBullet) and makes them accessible\nfrom within the parametric design environment. The package builds upon\nCOMPAS (https://compas.dev), an open-source Python-based framework for\ncollaboration and research in architecture, engineering and digital fabrication.\n",
      "dev_url": "https://github.com/compas-dev/compas_fab",
      "doc_url": "https://gramaziokohler.github.io/compas_fab",
      "home": "https://github.com/compas-dev/compas_fab",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "Robotic fabrication package for the COMPAS Framework"
    },
    "build": {
      "number": "0",
      "script": "PYTHON -m pip install . --no-deps --ignore-installed --no-cache-dir -vvv"
    },
    "extra": {
      "recipe-maintainers": [
        "gonzalocasas",
        "beverlylytle",
        "romanarust"
      ]
    },
    "package": {
      "name": "compas_fab",
      "version": "1.1.2"
    },
