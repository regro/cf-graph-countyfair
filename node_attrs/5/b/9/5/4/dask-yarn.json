    "about": {
      "description": "Dask-Yarn deploys Dask on YARN clusters, such as are found in traditional\nHadoop installations. Dask-Yarn provides an easy interface to quickly\nstart, stop, and scale Dask clusters natively from Python.\n",
      "dev_url": "https://github.com/dask/dask-yarn",
      "doc_url": "https://dask-yarn.readthedocs.io/",
      "home": "https://github.com/dask/dask-yarn",
      "license": "BSD-3-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE.txt",
      "summary": "Deploy dask clusters on Apache YARN"
    },
    "build": {
      "entry_points": [
        "dask-yarn=dask_yarn.cli:main"
      ],
      "number": "2",
      "script": "PYTHON -m pip install . --no-deps -vv"
    },
