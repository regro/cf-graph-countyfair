  "linux_64_meta_yaml": {
    "about": {
      "description": "Cygrid allows to resample a number of spectra (or data points) to a\nregular grid - a data cube - using any valid astronomical FITS/WCS\nprojection. The method is a based on serialized convolution with finite\ngridding kernels. Currently, only Gaussian (radial-symmetric or\nelliptical) kernels are provided (which has the drawback of slight\ndegradation of the effective resolution). The algorithm has very small\nmemory footprint, allows easy parallelization, and is very fast.\n",
      "dev_url": "https://github.com/bwinkel/cygrid",
      "home": "https://github.com/bwinkel/cygrid",
      "license": "GPL-3.0",
      "license_family": "GPL",
      "license_file": "licenses/README.rst",
      "summary": "Cygrid is a cython-powered convolution-based gridding module for astronomy"
    },
    "build": {
      "number": "0",
      "script": "PYTHON -m pip install . --no-deps -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "bwinkel",
        "DanielLenz"
      ]
    },
    "package": {
      "name": "cygrid",
      "version": "2.0.4"
    },
    "requirements": {
      "build": [
        "cxx_compiler_stub"
      ],
      "host": [
        "python",
        "pip",
        "setuptools",
        "setuptools_scm",
        "extension-helpers",
        "astropy",
        "cython",
        "numpy"
      ],
      "run": [
        "python",
        "astropy",
        "pytest",
        "pytest-astropy",
        "pytest-doctestplus",
        "numpy"
      ]
    },
    "schema_version": 0,
