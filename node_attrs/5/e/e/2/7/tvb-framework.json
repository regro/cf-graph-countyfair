        "scikit-learn",
        "scipy <1.14",
        "simplejson",
        "six",
        "sqlalchemy",
        "tvb-library",
        "tvb-gdist",
        "tvb-storage",
        "werkzeug",
        "matplotlib",
        "psycopg2",
        "docutils",
        "gevent",
        "bctpy"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "d69d29548c24cdcaeff7a25122dad95f3ff5a5e40a924558b2835448d14b174b",
      "url": "https://pypi.io/packages/source/t/tvb-framework/tvb_framework-2.9.1.tar.gz"
    },
    "test": {
      "commands": [
        "pip install requests-toolbelt pynrrd simpleitk wget",
        "pip install bctpy siibra==1.0a11 xarray",
        "python -m wget -o tvb_data.zip https://zenodo.org/record/10128131/files/tvb_data.zip?download=1",
        "python -m zipfile -e tvb_data.zip tvb_data",
        "cd tvb_data",
        "python setup.py develop",
        "pytest -o mock_use_standalone_module=true --pyargs tvb.tests.framework"
      ],
      "imports": [
        "tvb.core.adapters"
      ],
      "requires": [
        "pip",
        "pytest",
        "pytest-benchmark",
        "pytest-mock",
        "beautifulsoup4",
        "mock"
      ]
    }
  },
