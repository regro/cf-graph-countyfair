    "about": {
      "description": "PyEMMA (EMMA = Emma's Markov Model Algorithms) is an open source Python/C package\nfor analysis of extensive molecular dynamics simulations.\nIn particular, it includes algorithms for estimation, validation and analysis of:\n  * Clustering and Featurization\n  * Markov state models (MSMs)\n  * Hidden Markov models (HMMs)\n  * Multi-ensemble Markov models (MEMMs)\n  * Time-lagged independent component analysis (TICA)\n  * Transition Path Theory (TPT)\n",
      "dev_url": "https://github.com/markovmodel/pyemma",
      "doc_url": "http://emma-project.org",
      "home": "http://github.com/markovmodel/pyemma",
      "license": "LGPL-3.0-or-later",
      "license_file": "LICENSE.txt",
      "summary": "PyEMMA (EMMA = Emma's Markov Model Algorithms) is an open source Python/C package for analysis of extensive molecular dynamics simulations"
    },
    "build": {
      "number": "1",
      "script": "PYTHON -m pip install . --no-deps --ignore-installed --no-cache-dir -vvv"
    },
    "extra": {
      "recipe-maintainers": [
        "franknoe",
        "marscher",
        "clonker"
      ]
    },
