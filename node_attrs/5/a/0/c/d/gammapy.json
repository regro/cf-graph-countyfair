    "about": {
      "description": "Gammapy is an open-source Python package for gamma-ray astronomy built on Numpy and Astropy.\nIt is a prototype for the Cherenkov Telescope Array (CTA) science tools,\nand can also be used to analyse data from existing gamma-ray telescopes.\n",
      "dev_url": "https://github.com/gammapy/gammapy",
      "doc_url": "https://docs.gammapy.org/",
      "home": "https://gammapy.org/",
      "license": "BSD-3-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE.rst",
      "summary": "A Python package for gamma-ray astronomy"
    },
    "build": {
      "entry_points": [
        "gammapy = gammapy.scripts.main:cli"
      ],
      "number": "0",
      "script": "PYTHON -m pip install . --no-deps -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "mwcraig",
        "bsipocz",
        "adonath",
        "registerrier"
      ]
    },
    "package": {
      "name": "gammapy",
      "version": "2.0"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
