  "linux_64_meta_yaml": {
    "about": {
      "description": "mlpack is an intuitive, fast, and flexible C++ machine learning library\nwith bindings to other languages. It is meant to be a machine learning analog\nto LAPACK, and aims to implement a wide array of machine learning methods and\nfunctions as a \"swiss army knife\" for machine learning researchers. In addition\nto its powerful C++ interface, mlpack also provides command-line programs and\nPython bindings.\n",
      "dev_url": "https://github.com/mlpack/mlpack",
      "doc_url": "http://www.mlpack.org/docs.html",
      "home": "http://www.mlpack.org",
      "license": "BSD-3-Clause",
      "license_file": "LICENSE.txt",
      "summary": "mlpack: a fast, header-only machine learning library"
    },
    "build": {
      "number": "1",
      "script_env": null
    },
    "extra": {
      "recipe-maintainers": [
        "marcelotrevisani",
        "zoq",
        "rcurtin",
        "coatless"
      ]
    },
