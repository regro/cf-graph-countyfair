  "raw_meta_yaml": "# yaml-language-server: $schema=https://raw.githubusercontent.com/prefix-dev/recipe-format/main/schema.json\nschema_version: 1\n\ncontext:\n  version: \"0.17.4\"\n  python_check_max: \"3.13\"\n\nrecipe:\n  name: typer-split\n  version: ${{ version }}\n\nsource:\n  - url: https://pypi.org/packages/source/t/typer-slim/typer_slim-${{ version }}.tar.gz\n    sha256: c25430a74e45546df3e76b53bdfd0a1d86569d319f9c387e76e1b25a4dd72a0c\n    target_directory: typer-slim\n  - url: https://pypi.org/packages/source/t/typer-cli/typer_cli-${{ version }}.tar.gz\n    sha256: ee414bed75fdcfa3ef0d3b4eef9b269a01507770d9d2965c4c14e166626729fa\n    target_directory: typer-cli\n    patches:\n      - 0000-typer-cli-no-entry-point.patch\n  - url: https://pypi.org/packages/source/t/typer/typer-${{ version }}.tar.gz\n    sha256: b77dc07d849312fd2bb5e7f20a7af8985c7ec360c45b051ed5412f64d8dc1580\n    target_directory: typer\n\nbuild:\n  number: 0\n  noarch: python\n\noutputs:\n  - package:\n      name: typer-slim\n    build:\n      noarch: python\n      script:\n        interpreter: python\n        file: build_typer.py\n    requirements:\n      host:\n        - pdm-backend\n        - pip\n        - python ${{ python_min }}.*\n        - python-build\n      run:\n        - python >=${{ python_min }}\n        - click >=8.0.0\n        - typing_extensions >=3.7.4.3\n      run_constraints:\n        - typer ${{ version }}.*\n        - rich >=10.11.0\n        - shellingham >=1.3.0\n    tests:\n      - python:\n          imports: typer\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n\n  - package:\n      name: typer-cli\n    build:\n      noarch: python\n      script:\n        interpreter: python\n        file: build_typer.py\n    requirements:\n      host:\n        - pdm-backend\n        - pip\n        - python ${{ python_min }}.*\n        - python-build\n      run:\n        - ${{ pin_subpackage(\"typer\", exact=True) }}\n        - python >=${{ python_min }}\n    tests:\n      - python:\n          imports: typer\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n      - requirements:\n          run:\n            - python ${{ python_min }}.*\n        script:\n          - typer --version\n          - typer --help\n\n  - package:\n      name: typer\n    build:\n      noarch: python\n      script:\n        interpreter: python\n        file: build_typer.py\n      python:\n        entry_points:\n          - typer = typer.cli:main\n    requirements:\n      host:\n        - pdm-backend\n        - pip\n        - python ${{ python_min }}.*\n        - python-build\n      run:\n        - ${{ pin_subpackage(\"typer-slim-standard\", exact=True) }}\n        - python >=${{ python_min }}\n    tests:\n      - python:\n          imports: typer\n          pip_check: true\n          python_version: ${{ python_min }}.*\n      - files:\n          source:\n            - typer/tests/\n            - typer/docs_src/\n        requirements:\n          run:\n            - coverage\n            - pytest\n            - python ${{ python_min }}.*\n        script:\n          - typer --version\n          - typer --help\n          - cd typer\n          - coverage run --source=typer --branch -m pytest -vv --color=yes --tb=long -k \"not ((multiple_values and main) or completion or invalid_score)\"\n          - coverage report --show-missing --skip-covered --fail-under=66\n\n  - package:\n      name: typer-slim-standard\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - ${{ pin_subpackage(\"typer-slim\", exact=True) }}\n        # see `typer-slim` for pins in `run_constrained`\n        - rich\n        - shellingham\n    tests:\n      - python:\n          imports: typer\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\nabout:\n  license: MIT\n  license_file: typer-slim/LICEN