  "linux_64_meta_yaml": {
    "about": {
      "description": "The Open Force Field Toolkit, built by the Open Force Field Initiative, is a Python\ntoolkit for the development and application of modern molecular mechanics force fields\nbased on direct chemical perception and rigorous statistical parameterization methods.\n",
      "dev_url": "https://github.com/openforcefield/openff-toolkit",
      "doc_url": "https://docs.openforcefield.org/projects/toolkit/en/stable/",
      "home": "https://openforcefield.org/",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "A modern, extensible library for molecular mechanics force field science from the Open Force Field Initiative"
    },
    "build": {
      "number": "1"
    },
    "extra": {
      "recipe-maintainers": [
        "jaimergp",
        "j-wags",
        "mattwthompson"
      ]
    },
    "outputs": [
      {
        "build": {
          "noarch": "python"
        },
        "name": "openff-toolkit-base",
        "requirements": {
          "host": [
            "python 3.11",
            "pip",
            "setuptools",
            "versioningit"
          ],
          "run": [
            "python >=3.11,<3.14a0",
            "packaging",
            "numpy",
            "openff-forcefields",
            "openff-amber-ff-ports >=0.0.4",
            "openff-units",
            "openff-utilities",
            "networkx >=2.5",
            "xmltodict",
            "bson",
            "python-constraint",
            "cachetools",
            "typing_extensions"
          ],
          "run_constrained": [
            "openforcefield ==9999999999",
            "pydantic <3.0.0a0",
            "qcportal >=0.50.0"
          ]
        },
        "script": "build_base.sh",
        "test": {
          "imports": [
            "openff.toolkit"
          ],
          "requires": [
            "python 3.11"
          ]
        }
      },
      {
        "build": {
          "noarch": "python"
        },
        "name": "openff-toolkit",
        "requirements": {
          "host": [
            "python 3.11"
          ],
          "run": [
            "python >=3.11,<3.14a0",
            "openmm >=7.6",
            "openff-interchange ~=0.4.0",
            "rdkit !=2024.03.6",
            "ambertools >=22",
            "mdtraj",
            "openff-toolkit-base",
            "notebook",
            "ipywidgets",
            "openff-nagl >=0.4"
          ]
        },
        "test": {
          "commands": [
            "python test_openff_toolkit.py"
          ],
          "files": [
            "test_openff_toolkit.py"
          ],
          "imports": [
            "openff.toolkit",
            "rdkit"
          ],
          "requires": [
            "python 3.11",
            "qcportal"
          ]
        }
      },
      {
        "build": {
          "noarch": "python"
        },
        "name": "openff-toolkit-examples",
        "requirements": {
          "run": [
            "python >=3.11,<3.13",
            "nglview",
            "jupyterlab",
            "qcelemental",
            "qcportal >=0.50.0",
            "qcengine",
            "openmmforcefields",
            "pdbfixer",
            "openff-toolkit"
          ]
        },
        "script": "move-examples.sh",
        "test": {
          "commands": [
            "python test_examples_copied.py"
          ],
          "files": [
            "test_examples_copied.py"
          ],
          "requires": [
            "python 3.11"
          ],
          "source_files": [
            "examples"
          ]
        }
      }
    ],
    "package": {
