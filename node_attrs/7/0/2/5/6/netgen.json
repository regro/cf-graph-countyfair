      ]
    },
    "package": {
      "name": "netgen",
      "version": "6.2.2006"
    },
    "requirements": {
      "build": [
        "cxx_compiler_stub",
        "ninja",
        "cmake"
      ],
      "host": [
        "occt",
        "python",
        "pybind11",
        "zlib"
      ],
      "run": [
        "occt",
        "python",
        "zlib"
      ]
    },
