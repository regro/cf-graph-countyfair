  "linux_64_meta_yaml": {
    "about": {
      "description": "The mesoscale hydrologic model (mHM) is a spatially explicit distributed\nhydrologic model that uses grid cells as a primary hydrologic unit\nand accounts for the following processes: canopy interception,\nsnow accumulation and melting, soil moisture dynamics, infiltration and surface runoff,\nevapotranspiration, subsurface storage and discharge generation,\ndeep percolation and baseflow and discharge attenuation and flood routing.\n",
      "dev_url": "https://git.ufz.de/mhm/mhm",
      "doc_url": "https://mhm.pages.ufz.de/mhm",
      "home": "https://www.ufz.de/mhm",
      "license": "LGPL-3.0-or-later",
      "license_family": "LGPL",
      "license_file": [
        "COPYING",
        "COPYING.LESSER",
        "LICENSE.md",
        "AUTHORS"
      ],
      "summary": "The mesoscale Hydrological Model - mHM"
    },
    "build": {
      "entry_points": [
        "mhm = mhm.cli:mhm",
        "mhm-download = mhm.download:cli"
      ],
      "number": "1",
      "script": [
        "PYTHON -m pip install . --no-deps --no-build-isolation -vv"
      ],
      "script_env": [
        "SETUPTOOLS_SCM_PRETEND_VERSION=5.13.2",
        "MHM_BUILD_PARALLEL=1",
        "CMAKE_BUILD_PARALLEL_LEVEL=4"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "MuellerSeb",
        "StephanThober",
        "mhm-admin"
      ]
    },
    "package": {
      "name": "mhm",
      "version": "5.13.2"
    },
    "requirements": {
      "build": [
        "fortran_compiler_stub",
