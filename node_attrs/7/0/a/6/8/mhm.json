{
  "archived": false,
  "branch": "main",
  "conda-forge.yml": {
    "build_platform": {
      "linux_aarch64": "linux_64",
      "linux_ppc64le": "linux_64",
      "osx_arm64": "osx_64"
    },
    "conda_build": {
      "pkg_format": "2"
    },
    "conda_forge_output_validation": true,
    "github": {
      "branch_name": "main",
      "tooling_branch_name": "main"
    },
    "provider": {
      "linux_aarch64": "default",
      "linux_ppc64le": "default"
    },
    "test": "native_and_emulated"
  },
  "feedstock_name": "mhm",
  "hash_type": "sha256",
  "linux_64_meta_yaml": {
    "about": {
      "description": "The mesoscale hydrologic model (mHM) is a spatially explicit distributed\nhydrologic model that uses grid cells as a primary hydrologic unit\nand accounts for the following processes: canopy interception,\nsnow accumulation and melting, soil moisture dynamics, infiltration and surface runoff,\nevapotranspiration, subsurface storage and discharge generation,\ndeep percolation and baseflow and discharge attenuation and flood routing.\n",
      "dev_url": "https://git.ufz.de/mhm/mhm",
      "doc_url": "https://mhm.pages.ufz.de/mhm",
      "home": "https://www.ufz.de/mhm",
      "license": "LGPL-3.0-or-later",
      "license_family": "LGPL",
      "license_file": [
        "COPYING",
        "COPYING.LESSER",
        "LICENSE.md",
        "AUTHORS"
      ],
      "summary": "The mesoscale Hydrological Model - mHM"
    },
    "build": {
      "number": "5",
      "script": [
        "PYTHON -m pip install . --no-deps --no-build-isolation -vv"
      ],
      "script_env": [
        "SETUPTOOLS_SCM_PRETEND_VERSION=5.13.1",
        "MHM_BUILD_PARALLEL=1",
        "CMAKE_BUILD_PARALLEL_LEVEL=4",
        "SKBUILD_CMAKE_ARGS=\"-GNinja\""
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "MuellerSeb",
        "StephanThober",
        "mhm-admin"
      ]
    },
    "package": {
      "name": "mhm",
      "version": "5.13.1"
    },
    "requirements": {
      "build": [
        "fortran_compiler_stub",
        "c_compiler_stub",
        "c_stdlib_stub",
        "cxx_compiler_stub",
        "cmake",
        "ninja",
        "fypp",
        "libgomp"
      ],
      "host": [
        "pip",
        "python",
        "scikit-build-core >=0.5, <0.6",
        "setuptools_scm >=7, <8",
        "numpy",
        "fypp",
        "netcdf-fortran"
      ],
      "run": [
        "python",
        "netcdf-fortran"
      ]
    },
    "schema_version": 0,
    "source": {
      "patches": [
        "no_find_numpy.patch"
      ],
      "sha256": "acd61f906a6c66b0bee71a22d14b5574ad6aa952b626d7b77e65384928823a24",
      "url": "https://pypi.org/packages/source/m/mhm/mhm-5.13.1.tar.gz"
    },
    "test": {
      "commands": [
        "mhm -h",
        "mhm -V",
        "mhm-download -h",
        "mhm-download -V",
        "mhm-download -v -d 1 -p example_domain",
        "mhm example_domain"
      ]
    }
  },
