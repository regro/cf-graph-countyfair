  "linux_64_meta_yaml": {
    "about": {
      "description": "Python module for reading and writing Apache ORC\nfile format. It uses the Apache ORC's Core C++ API\nunder the hood, and provides a similar interface as\nthe csv module in the Python standard library.\nSupports only Python 3.6 or newer and ORC 1.6.\n",
      "dev_url": "https://github.com/noirello/pyorc",
      "doc_url": "https://pyorc.readthedocs.io/",
      "home": "https://github.com/noirello/pyorc",
      "license": "Apache-2.0",
      "license_file": [
        "LICENSE",
        "vendor-licenses/"
      ],
      "summary": "Python module for Apache ORC file format"
    },
    "build": {
      "number": "5",
      "script": "PYORC_SKIP_ORC_BUILD=1 PYORC_LIB_VERSION=\"2.2.0\" PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "noirello"
      ]
    },
    "package": {
      "name": "pyorc",
      "version": "0.10.0"
    },
    "requirements": {
      "build": [
        "cxx_compiler_stub",
        "c_stdlib_stub"
      ],
      "host": [
        "python",
        "pip",
        "setuptools",
        "pybind11",
        "orc"
      ],
      "run": [
        "python",
        "tzdata"
      ]
    },
    "schema_version": 0,
    "source": {
      "patches": [
        "patches/0001-point-to-our-own-libraries-includes.patch",
        "patches/0002-remove-TZDIR-work-arounds-that-aren-t-needed-for-orc.patch"
