  "linux_64_meta_yaml": {
    "about": {
      "description": "GraKeL is a library that provides implementations of several\nwell-established graph kernels. The library unifies these kernels\ninto a common framework. Furthermore, it provides implementations of\nsome frameworks that work on top of graph kernels. Specifically,\nGraKeL contains 15 kernels and 2 frameworks. The library is compatible\nwith the scikit-learn pipeline allowing easy and fast integration\ninside machine learning algorithms.\n",
      "dev_url": "https://github.com/ysig/GraKeL",
      "doc_url": "https://ysig.github.io/GraKeL/",
      "home": "https://ysig.github.io/GraKeL/",
      "license": "BSD-3-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE",
      "summary": "A scikit-learn compatible library for graph kernels"
    },
    "build": {
      "number": "4",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "jan-janssen"
      ]
    },
    "package": {
      "name": "grakel",
      "version": "0.1.10"
    },
    "requirements": {
