  "linux_64_meta_yaml": {
    "about": {
      "description": "Hangar is based off the belief that too much time is spent collecting, managing,\nand creating home-brewed version control systems for data. At it's core Hangar\nis designed to solve many of the same problems faced by traditional code version\ncontrol system (ie. ``Git``), just adapted for numerical data.\n",
      "dev_url": "https://github.com/tensorwerk/hangar-py",
      "doc_url": "https://hangar-py.readthedocs.io/",
      "home": "https://github.com/tensorwerk/hangar-py",
      "license": "Apache-2.0",
      "license_family": "APACHE",
      "license_file": "LICENSE",
      "summary": "Hangar is version control for tensor data. Commit, branch, merge, revert, and collaborate in the data-defined software era."
    },
    "build": {
      "entry_points": [
        "hangar = hangar.cli:main"
      ],
      "number": "1",
      "script": "PYTHON -m pip install . --no-deps -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "rlizzo",
        "lantiga"
      ]
    },
    "package": {
      "name": "hangar",
      "version": "0.5.2"
    },
    "requirements": {
      "build": [
        "c_compiler_stub"
      ],
      "host": [
        "pip",
        "python",
        "setuptools",
        "wheel",
        "cython >=0.27"
      ],
      "run": [
        "click",
        "grpcio",
        "h5py >=2.9",
        "hdf5plugin >=2.0",
        "numpy",
        "protobuf",
        "python",
        "python-blosc",
        "python-lmdb >=0.92",
        "python-xxhash",
        "tqdm",
        "wrapt"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "d452f37b9ba063bbb1b888b8d251d41dd36cd830b4e710ee7aecf0d527ed90f5",
      "url": "https://pypi.io/packages/source/h/hangar/hangar-0.5.2.tar.gz"
    },
    "test": {
      "commands": [
        "hangar --help",
        "hangar --version"
      ],
      "imports": [
        "hangar",
        "hangar.diagnostics",
        "hangar.records",
        "hangar.remote",
        "hangar.backends",
        "hangar.columns"
      ]
    }
  },
