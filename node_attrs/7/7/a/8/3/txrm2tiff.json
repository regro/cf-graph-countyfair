  "linux_64_meta_yaml": {
    "about": {
      "description": "Converts txrm/xrm files to OME tif/tiff files.\ntxrm2tiff was created for users of beamline B24 of Diamond Light\nSource by Thomas Fish. This has been adapted from B24's the automatic\nprocessing pipeline. Parts of this code were originally written by\nKevin Savage, with further additions and amendments by Peter Chang,\nVictoria Beilsten-Edmands, and Thomas Fish.\n",
      "dev_url": "https://github.com/DiamondLightSource/txrm2tiff",
      "doc_url": "https://github.com/DiamondLightSource/txrm2tiff/blob/master/README.md",
      "home": "https://github.com/DiamondLightSource/txrm2tiff",
      "license": "BSD-3-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE",
      "summary": "Converts txrm/xrm files to OME tif/tiff files"
    },
    "build": {
      "entry_points": [
        "txrm2tiff = txrm2tiff.__main__:main"
      ],
      "number": "1",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "thomasmfish",
        "PeterC-DLS"
      ]
    },
    "package": {
      "name": "txrm2tiff",
      "version": "2.2.0"
    },
    "requirements": {
      "host": [
        "python",
        "pip",
        "setuptools"
      ],
      "run": [
        "python",
        "numpy >=1.24.4",
        "scipy >=1.3.3",
        "tifffile >=2022.7.28",
        "omexml-dls >=1.1.0",
        "olefile >=0.46",
        "pillow >=11"
      ]
    },
