  "raw_meta_yaml": "# yaml-language-server: $schema=https://raw.githubusercontent.com/prefix-dev/recipe-format/main/schema.json\nschema_version: 1\n\ncontext:\n  version: \"0.40.4\"\n  maturin_min: \"1.0\"\n  maturin_max: \"2.0\"\n  python_check_max: \"3.14\"\n  abi3_ext: ${{ \"pyd\" if win else \"abi3.so\" }}\n  unix_sp_dir: |-\n    $(python -c \"print(__import__('site').getsitepackages()[-1])\")/jsonschema_rs\n  win_sp_dir: |-\n    Lib\\site-packages\\jsonschema_rs\n  sp_dir: |-\n    ${{ \"%PREFIX%\\\\\" ~ win_sp_dir if win else unix_sp_dir }}\n\npackage:\n  name: jsonschema-rs\n  version: ${{ version }}\n\nsource:\n  url: https://pypi.org/packages/source/j/jsonschema-rs/jsonschema_rs-${{ version }}.tar.gz\n  sha256: 5df98ebea2f853b09ef018aee81b01e73525fd78455cb2a8d1e65961cecd8229\n\nbuild:\n  number: 0\n  skip: is_abi3 and not is_python_min\n  script:\n    - cargo-bundle-licenses --format yaml --output THIRDPARTY.yml\n    - ${{ PYTHON }} -m pip install . -vv --no-deps --no-build-isolation --disable-pip-version-check\n  python:\n    version_independent: ${{ is_abi3 }}\n\nrequirements:\n  build:\n    - ${{ compiler(\"rust\") }}\n    - ${{ stdlib(\"c\") }}\n    - cargo-bundle-licenses\n    - if: build_platform != target_platform\n      then:\n        - cross-python_${{ target_platform }}\n        - maturin >=${{ maturin_min }},<${{ maturin_max }}\n        - python\n        - if: is_abi3\n          then:\n          - python-abi3\n  host:\n    - maturin >=${{ maturin_min }},<${{ maturin_max }}\n    - pip\n    - python\n    - if: is_abi3\n      then:\n        - python-abi3\n  run:\n    - python\n\ntests:\n  - if: is_abi3\n    then:\n      - python:\n          imports: jsonschema_rs\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n    else:\n      - python:\n          imports: jsonschema_rs\n          pip_check: true\n  - files:\n      source:\n        - crates/jsonschema-py/tests-py/\n   