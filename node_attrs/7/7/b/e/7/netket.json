    "about": {
      "description": "NetKet is an open-source project delivering cutting-edge methods\nfor the study of many-body quantum systems with artificial neural\nnetworks and machine learning techniques. It is a Python library\nbuilt on C++ primitives.\n",
      "dev_url": "https://github.com/netket/netket",
      "doc_url": "https://www.netket.org/documentation.html",
      "home": "https://www.netket.org",
      "license": "Apache-2.0",
      "license_family": "APACHE",
      "license_file": "LICENSE",
      "summary": "Machine Learning for Many-Body Quantum Systems"
    },
    "build": {
      "noarch": "python",
      "number": "0",
      "script": [
        "PYTHON -m pip install --no-deps --ignore-installed --no-cache-dir -vvv ."
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "PhilipVinc",
        "gcarleo"
      ]
    },
    "package": {
      "name": "netket",
      "version": "3.11.4"
    },
    "requirements": {
      "host": [
        "python >=3.8",
        "pip",
        "hatchling >=1.10",
        "hatch-vcs >=0.3"
      ],
      "run": [
        "python >=3.9",
        "numpy >=1.20",
        "scipy >=1.5.2",
        "python-igraph >=0.9",
        "jax >=0.4.3",
        "flax >=0.7.1",
        "optax >=0.1.3",
        "mpi4py >=3.0.1",
        "mpi4jax >=0.3.1",
        "numba4jax >=0.0.9",
        "numba >=0.52.0, <0.58",
        "orjson >=3.4",
        "tqdm >=4.56.2",
        "beartype >=0.12",
        "rich >=10.0",
        "typing-extensions >=4.0"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "a68514dc1e41235b3e4ca403c8f05bcf058864ec67d1a8c842977d11b152f6e3",
      "url": "https://pypi.io/packages/source/n/netket/netket-3.11.4.tar.gz"
    },
    "test": {
      "commands": [
        "",
        "pip install python-igraph",
        "pip check",
        "export OMPI_MCA_plm=isolated",
        "export OMPI_MCA_btl_vader_single_copy_mechanism=none",
        "export OMPI_MCA_rmaps_base_oversubscribe=yes",
        "export OMPI_ALLOW_RUN_AS_ROOT=1",
        "export OMPI_ALLOW_RUN_AS_ROOT_CONFIRM=1",
        "python -c \"import netket; print(netket.__version__)\""
      ],
      "requires": [
        "pip"
      ]
    }
  },
