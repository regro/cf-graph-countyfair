      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE.txt",
      "summary": "Mark an environment as externally managed, prevents pip from installing packages"
    },
    "build": {
      "number": "1",
      "script": [
        "cp  ${RECIPE_DIR}/EXTERNALLY-MANAGED ${STDLIB_DIR}/EXTERNALLY-MANAGED"
      ]
