python_check_max }}.*\n    about:\n      summary: Light-weight and flexible ASGI API Framework (with [annotated-types])\n\n  - package:\n      name: litestar-with-attrs\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - ${{ pin_subpackage(\"litestar\", exact=True) }}\n        - attrs\n    tests:\n      - python:\n          imports: litestar.contrib.attrs\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n    about:\n      summary: Light-weight and flexible ASGI API Framework (with [attrs])\n\n  - package:\n      name: litestar-with-brotli\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - ${{ pin_subpackage(\"litestar\", exact=True) }}\n        - brotli\n    tests:\n      - python:\n          imports: litestar\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n    about:\n      summary: Light-weight and flexible ASGI API Framework (with [brotli])\n\n  - package:\n      name: litestar-with-cli\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - ${{ pin_subpackage(\"litestar\", exact=True) }}\n        - jsbeautifier\n        - uvicorn\n        - uvicorn-standard\n    tests:\n      - python:\n          imports: litestar.cli.main\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n      - requirements:\n          run:\n            - python ${{ python_min }}.*\n        script:\n          - litestar --help\n    about:\n      summary: Light-weight and flexible ASGI API Framework (with [cli])\n\n  - package:\n      name: litestar-with-cryptography\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - ${{ pin_subpackage(\"litestar\", exact=True) }}\n        - cryptography\n    tests:\n      - python:\n          imports: litestar.middleware.session.client_side\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n    about:\n      summary: Light-weight and flexible ASGI API Framework (with [cryptography])\n\n  - package:\n      name: litestar-with-jinja\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - ${{ pin_subpackage(\"litestar\", exact=True) }}\n        - jinja2 >=3.1.2\n    tests:\n      - python:\n          imports: litestar.contrib.jinja\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n    about:\n      summary: Light-weight and flexible ASGI API Framework (with [jinja])\n\n  - package:\n      name: litestar-with-jwt\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - ${{ pin_subpackage(\"litestar\", exact=True) }}\n        - cryptography\n        - pyjwt >=2.9.0\n    tests:\n      - python:\n          imports: litestar.contrib.jwt.jwt_token\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n    about:\n      summary: Light-weight and flexible ASGI API Framework (with [jwt])\n\n  - package:\n      name: litestar-with-mako\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - ${{ pin_subpackage(\"litestar\", exact=True) }}\n        - mako >=1.2.4\n    tests:\n      - python:\n          imports: litestar.contrib.mako\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n    about:\n      summary: Light-weight and flexible ASGI API Framework (with [mako])\n\n  - package:\n      name: litestar-with-minijinja\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - ${{ pin_subpackage(\"litestar\", exact=True) }}\n        - minijinja >=1.0.0\n    tests:\n      - python:\n          imports: litestar.