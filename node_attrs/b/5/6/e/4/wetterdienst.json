  "raw_meta_yaml": "context:\n  name: wetterdienst\n  version: \"0.118.0\"\n  python_min_exporter: \"3.11\"\n\nrecipe:\n  name: ${{ name|lower }}-split\n  version: ${{ version }}\n\nsource:\n  url: https://pypi.org/packages/source/${{ name[0] }}/${{ name }}/${{ name }}-${{ version }}.tar.gz\n  sha256: b87f8f8bcc96f9278bb38cc06ad9b11751980b38753bf3a7b652c5133c4ebe58\n\n\nbuild:\n  number: 1\n\noutputs:\n  - package:\n      name: ${{ name }}\n    build:\n      noarch: python\n      script:\n        - ${{ PYTHON }} -m pip install . --no-deps --ignore-installed --no-cache-dir -vvv\n      python:\n        entry_points:\n          - wetterdienst = wetterdienst.ui.cli:cli\n          - wddump = wetterdienst.provider.dwd.radar.cli:wddump\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n        - uv-build >=0.9.21,<0.10.0\n        - pip\n      run:\n        - python >=${{ python_min }},<4.0\n        - aiohttp >=3.13.2,<4\n        # for python < 3.11 but we want to stay noarch\n        - backports-datetime-fromisoformat >=2\n        - certifi >=2024.2.2\n        - click >=8,<9\n        - cloup >=3.0.1,<4\n        - diskcache >=5.4,<6\n\n        - fsspec >=2023.1\n        # fsspec[http]\n        - aiohttp !=4.0.0a0,!=4.0.0a1\n\n        - lxml >=4.9.1,<7\n        - measurement >=3.2,<4\n        - platformdirs >=4,<5\n\n        - polars >=1.15.0,<2\n        # polars[calamine]\n        - fastexcel >=0.9\n\n        - portion >=2.4.2,<3\n        - pyarrow >=22.0.0\n        - pydantic >=2.7.3,<3\n        - pydantic-extra-types >=2.10.1,<3\n        - pydantic-settings >=2.7.0,<3\n        - python-dateutil >=2,<3\n        - rapidfuzz >=3.1,<4\n        - stamina >=23,<26\n        - tabulate >=0.8,<1\n        - tqdm >=4,<5\n        # for all python, not just py < 3.11 to stay noarch: python\n        - typing-extensions >=4.15.0\n        - python-tzdata >=2024.2\n        - tzfpy >=0.15.2,<2\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n      - script:\n          - wetterdienst --help\n\n  - package:\n      name: ${{ name }}-with-bufr\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        # upstream has this constraint only on pdbufr but we cannot\n        # do that on conda-forge and stay noarch: python\n        - python >=${{ python_min }},<3.14\n        - ${{ pin_subpackage(name, exact=True) }}\n        - pdbufr >=0.10.2,<1\n        - pybufrkit >=0.2,<1\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  # sqlalchemy-cratedb not on conda-forge\n  # - package:\n  #     name: ${{ name }}-with-cratedb\n  #   build:\n  #     noarch: python\n  #   requirements:\n  #     host:\n  #       - python ${{ python_min }}.*\n  #     run:\n  #       - python >=${{ python_min }}\n  #       - ${{ pin_subpackage(name, exact=True) }}\n  #       - pandas >=2,<3\n  #       - sqlalchemy-cratedb <1\n  #   tests:\n  #     - python:\n  #         imports:\n  #           - wetterdienst\n  #         pip_check: true\n  #         python_version: ${{ python_min }}.*\n\n  - package:\n      name: ${{ name }}-with-duckdb\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - duckdb >=0.9,<2\n        - pandas >=2,<3\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  - package:\n      name: ${{ name }}-with-eccodes\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }},<3.14\n        - ${{ pin_subpackage(name, exact=True) }}\n        - python-eccodes >=1.5.2,<3\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  - package:\n      name: ${{ name }}-with-excel\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - xlsxwriter >=3.0.9,<4\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  # this has been dropped upstream but is still needed\n  # here for python 3.10 support\n  - package:\n      name: ${{ name }}-with-export-without-zarr\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - pandas >=2,<3\n        - sqlalchemy >=2,<3\n        - xarray >=2023.1,<2026\n        - xlsxwriter >=3.0.9,<4\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  # this is only available for python >= 3.11, where the\n  # require zarr version is available\n  - package:\n      name: ${{ name }}-with-export\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min_exporter }}.*\n      run:\n        - python >=${{ python_min_exporter }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - ${{ name }}-with-export-without-zarr\n        - zarr >=3.1,<4\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min_exporter }}.*\n\n  # - package:\n  #     name: ${{ name }}-with-influxdb\n  #   build:\n  #     noarch: python\n  #   requirements:\n  #     host:\n  #       - python ${{ python_min }}.*\n  #     run:\n  #       - python >=${{ python_min }}\n  #       - ${{ pin_subpackage(name, exact=True) }}\n  #       - influxdb >=5.3,<6\n  #       - influxdb-client >=1.18,<2\n  #       - influxdb3-python >=0.3,<1\n  #   tests:\n  #     - python:\n  #         imports:\n  #           - wetterdienst\n  #         pip_check: true\n  #         python_version: ${{ python_min }}.*\n\n  - package:\n      name: ${{ name }}-with-interpolation\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - scipy >=1.14.1,<2\n        - shapely >=2,<3\n        - utm >=0.7,<1\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  - package:\n      name: ${{ name }}-with-matplotlib\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - matplotlib-base >=3.3,<4\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  - package:\n      name: ${{ name }}-with-mysql\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - mysqlclient >=2,<3\n        - pandas >=2,<3\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  - package:\n      name: ${{ name }}-with-pdf\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - pypdf >=3.12.1,<7\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  - package:\n      name: ${{ name }}-with-plotting\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - python-kaleido >=0.2.2\n        - plotly >=5.24.1\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  - package:\n      name: ${{ name }}-with-postgresql\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - psycopg2-binary >=2,<3\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  - package:\n      name: ${{ name }}-with-radar\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - h5py >=3.1,<4\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  - package:\n      name: ${{ name }}-with-radarplus\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - pybufrkit >=0.2,<1\n        - wradlib >=2.0.1,<3\n        - xradar >=0.2,<1\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  - package:\n      name: ${{ name }}-with-restapi\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - fastapi >=0.95.1,<1\n        - httpx >=0.24,<1\n        - uvicorn-standard >=0.14,<1\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\n  - package:\n      name: ${{ name }}-with-sql\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - ${{ pin_subpackage(name, exact=True) }}\n        - duckdb >=0.9,<2\n    tests:\n      - python:\n          imports:\n            - wetterdienst\n          pip_check: true\n          python_version: ${{ python_min }}.*\n\nabout:\n  homepage: https://github.com/earthobservations/wetterdienst\n  license: MIT\n  license_file: LICENSE.md\n  summary: Open weather data for humans\n  documentation: https://wetterdienst.readthedocs.io/\n  repository: https://github.com/earthobservations/wetterdienst\n\nextra:\n  feedstock-name: wetterdienst\n  recipe-maintainers:\n    - xylar\n    - amotl\n    - gutzbenj\n    - kmuehlbauer\n",
