  "linux_64_meta_yaml": {
    "about": {
      "dev_url": "https://bitbucket.org/jairhul/pyg4ometry/",
      "doc_url": "http://www.pp.rhul.ac.uk/bdsim/pyg4ometry/",
      "home": "https://bitbucket.org/jairhul/pyg4ometry/",
      "license": "GPL-3.0-only",
      "license_file": [
        "pyg4ometry/LICENCE.txt",
        "pyg4ometry/COPYING.txt"
      ],
      "summary": "Python code for rapid creation and conversion of radiation transport Monte Carlo (Geant4 and Fluka) geometries"
    },
    "build": {
      "number": "0",
      "script": [
        "cd pyg4ometry",
        "find pyg4ometry/ -name '*.g4' -print -exec java -jar \"$SRC_DIR/antlr-4.7.2-complete.jar\" {} -Dlanguage=Python3 -visitor \\;",
        "sed -E -i.bak 's@\"antlr4-python3-runtime==.+\"@\"antlr4-python3-runtime==4.7.2\"@g' setup.py",
        "grep antlr4-python3-runtime==4.7.2 setup.py",
        "PYTHON -m pip install . -vv"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "chrisburr"
      ]
    },
    "package": {
      "name": "pyg4ometry",
      "version": "1.0.2"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "cxx_compiler_stub",
        "make",
        "sed",
        "openjdk"
      ],
      "host": [
        "python",
        "pip",
        "cgal",
        "vtk",
        "pybind11",
        "cython",
        "mpfr",
        "gmp"
      ],
      "run": [
        "python",
        "antlr-python-runtime =4.7.2",
        "matplotlib-base",
        "pandas",
        "networkx",
        "numpy",
        "vtk",
        "configparser",
        "sympy >=1.7",
        "scipy"
      ]
    },
