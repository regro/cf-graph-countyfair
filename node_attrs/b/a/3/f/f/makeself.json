  "linux_64_meta_yaml": {
    "about": {
      "description": "makeself.sh is a small shell script that generates a self-extractable\ncompressed tar archive from a directory. The resulting file appears as a\nshell script (many of those have a .run suffix), and can be launched as is.\nThe archive will then uncompress itself to a temporary directory and an\noptional arbitrary command will be executed (for example an installation\nscript). This is pretty similar to archives generated with WinZip\nSelf-Extractor in the Windows world. Makeself archives also include\nchecksums for integrity self-validation (CRC and/or MD5/SHA256 checksums).",
      "dev_url": "https://github.com/megastep/makeself",
      "doc_url": "https://github.com/megastep/makeself",
      "home": "https://github.com/megastep/makeself",
      "license": "GPL-2.0-or-later",
      "license_family": "GPL-2.0-or-later",
      "license_file": "COPYING",
      "summary": "Make self-extractable archives on Unix"
    },
    "build": {
      "number": "0"
    },
    "extra": {
      "recipe-maintainers": [
        "saraedum",
        "hobu"
      ]
    },
    "outputs": [
      {
        "build": null,
        "name": "makeself",
        "requirements": {
          "build": [],
          "host": [],
          "run": [
            "tar",
            "findutils",
            "gzip",
