    "provider": {
      "linux_aarch64": "default",
      "linux_ppc64le": "default",
      "win": "azure"
    },
    "test": "native_and_emulated"
  },
  "feedstock_name": "glad",
  "hash_type": "sha256",
  "linux_64_meta_yaml": {
    "about": {
      "home": "https://github.com/Dav1dde/glad",
      "license": "MIT",
      "license_file": "LICENSE",
      "summary": "GL/GLES/EGL/GLX/WGL Loader-Generator based on the official specs."
    },
    "build": {
      "number": "1"
    },
    "extra": {
      "recipe-maintainers": [
        "cNoNim",
        "traversaro"
      ]
    },
    "outputs": [
      {
        "build": {
          "entry_points": [
            "glad = glad.__main__:main"
          ],
          "noarch": "python",
          "script": "python -m pip install . --no-deps -vv"
        },
        "name": "glad2",
        "requirements": {
          "host": [
            "pip",
            "python 3.9",
            "setuptools"
          ],
          "run": [
            "python >=3.9",
            "jinja2"
          ]
        },
        "test": {
          "commands": [
            "glad --help",
            "pip check"
          ],
          "imports": [
            "glad"
          ],
          "requires": [
            "python 3.9",
            "pip"
          ]
        }
      },
      {
        "about": {
          "description": "Example of use:\n```cmake\nfind_package(Glad CONFIG REQUIRED)\nglad_add_library(glad_gl_core_mx_33 REPRODUCIBLE MX API gl:core=3.3)\n```\nand link `glad_gl_core_mx_33` to target\n",
          "home": "https://github.com/Dav1dde/glad",
          "license": "MIT",
          "license_file": "LICENSE",
          "summary": "CMake package for glad2."
        },
        "build": {
          "script": [
            "${RECIPE_DIR}/install_cmake.sh"
          ],
          "skip": false
        },
        "name": "glad2-cmake",
        "requirements": {
          "run": [
            "glad2"
          ]
        },
        "test": {
          "commands": [
            "cmake -G Ninja -B b"
          ],
          "files": [
            "CMakeLists.txt"
          ],
          "requires": [
            "cmake",
            "ninja"
          ]
        }
      }
    ],
    "package": {
