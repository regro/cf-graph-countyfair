      "summary": "Python binding to Ammonia HTML sanitizer Rust crate"
    },
    "build": {
      "number": "0",
      "script": {
        "file": "build-nh3"
      }
    },
    "extra": {
      "recipe-maintainers": [
        "bollwyvl"
      ]
    },
    "outputs": [
      {
        "build": null,
        "name": "nh3",
        "requirements": {
          "build": [
            "rust_compiler_stub",
            "c_stdlib_stub",
            "cargo-bundle-licenses",
            "c_compiler_stub"
          ],
          "host": [
            "maturin >=1.0,<2.0",
            "pip",
            "python",
            "python-abi3"
          ],
          "run": [
            "python"
          ]
        },
        "tests": [
          {
            "python": {
              "imports": [
                "nh3"
              ],
              "python_version": [
                "3.10.*",
                "3.13.*"
              ]
            }
          },
          {
            "files": {
              "source": [
                "tests/"
              ]
            },
            "requirements": {
              "run": [
                "pytest-cov",
                "python 3.10.*"
              ]
            },
            "script": [
              "coverage run --source=nh3 --branch -m pytest -vv --tb=long --color=yes",
              "coverage report --show-missing --skip-covered --fail-under=83"
            ]
          }
        ],
        "version": "0.3.1"
      }
    ],
