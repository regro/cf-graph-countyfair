  "linux_64_meta_yaml": {
    "about": {
      "description": "GSD (General Simulation Data) is a file format specification\nand a library to read and write it. The package also contains a python module\nthat reads and writes hoomd schema gsd files with an easy to use syntax.",
      "dev_url": "https://github.com/glotzerlab/%7B%7B%20name%20%7D%7D",
      "doc_url": "https://gsd.readthedocs.io/",
      "home": "https://github.com/glotzerlab/gsd",
      "license": "BSD-2-Clause",
      "license_family": "BSD-2-Clause",
      "license_file": "LICENSE",
      "summary": "General simulation data"
    },
    "build": {
      "number": "0",
      "script": "${{ PYTHON }} -m pip install . -vv --no-deps"
    },
    "extra": {
      "recipe-maintainers": [
        "joaander",
        "tcmoore3",
        "SchoeniPhlippsn",
        "DomFijan",
        "janbridley",
        "tfteague",
        "sumitavaumich"
      ]
    },
    "outputs": [
      {
        "build": null,
        "name": "gsd",
        "requirements": {
          "build": [
            "c_compiler_stub",
            "c_stdlib_stub"
          ],
          "host": [
            "cython >=3.1",
            "python 3.11.*",
            "python-abi3 3.11.*",
            "numpy >=2",
            "pip",
            "setuptools >=77"
          ],
          "run": [
            "python"
          ]
        },
        "tests": [
          {
            "requirements": {
              "run": [
                "pytest",
                "python 3.11.*"
              ]
            },
            "script": [
              "cd /",
