  "raw_meta_yaml": "schema_version: 1\n\ncontext:\n  name: fandango\n  version: 16.0.3\n  python_min: 3.10\n\npackage:\n  name: ${{ name|lower }}\n  version: ${{ version }}\n\nsource:\n  url: https://pypi.org/packages/source/${{ name[0] }}/${{ name }}/fandango-${{ version }}.tar.gz\n  sha256: 73b59d213db51665869e19677f00547fc389f00ceb6a9d805d8dd9603e516653\n\nbuild:\n  number: 1\n  noarch: python\n  script: ${{ PYTHON }} -m pip install . -vv --no-deps --no-build-isolation\n  python:\n    entry_points:\n      - fandango = fandango.scripts.fandango_cli:main\n      - tango2csv = fandango.scripts.tango2csv:main\n      - tango2json = fandango.scripts.tango2json:main\n      - tango_host = fandango.scripts.tango_host:main\n      - tango_monitor = fandango.scripts.tango_monitor:main\n      - tango_servers = fandango.scripts.tango_servers:main\n      - CopyCatDS = fandango.interface.CopyCatDS:main      \n      - DynamicDS = fandango.device.DynamicDS:main\n      - FolderDS = fandango.device.FolderDS:main\n      - WorkerDS = fandango.device.WorkerDS:main\n\nrequirements:\n  host:\n    - python ${{ python_min }}.*\n    - pip\n    - setuptools\n  run:\n    - python >=${{ python_min }}\n\ntests:\n  - python:\n      imports:\n        - fandango\n      pip_check: true\n      python_version: ${{ python_min }}.*\n  - requirements:\n      run:\n        - python ${{ python_min }}.*\n        - pip\n    script:\n      - fandango --help\n\nabout:\n  summary: Simplify the configuration of big Tango control systems\n  license: LGPL-3.0-or-later\n  license_file: LICENSE\n  homepage: https://gitlab.com/tango-controls/fandango\n  documentation: https://pythonhosted.org/fandango/\n\nextra:\n  recipe-maintainers:\n    - sergirubio\n    - ovallcorba\n",
