  "raw_meta_yaml": "# yaml-language-server: $schema=https://raw.githubusercontent.com/prefix-dev/recipe-format/main/schema.json\nschema_version: 1\n\ncontext:\n  version: \"0.15.3\"\n  python_check_max: \"3.13\"\n\nrecipe:\n  name: ariadne-codegen\n  version: ${{ version }}\n\nsource:\n  - url: https://pypi.org/packages/source/a/ariadne-codegen/ariadne_codegen-${{ version }}.tar.gz\n    sha256: 30d8b876168411fb05eb52398ab319ce994b6d0b3fe6ae17056dc8a9f14aea6e\n    target_directory: dist\n  - url: https://github.com/mirumee/ariadne-codegen/archive/refs/tags/${{ version }}.tar.gz\n    sha256: 4eb0e02f0f9eab2ad839145b6964bee34df08be90676134f0ef7d24c7168d1e8\n    target_directory: src\n\nbuild:\n  number: 0\n  noarch: python\n\noutputs:\n  - package:\n      name: ariadne-codegen\n    build:\n      noarch: python\n      script: cd dist && ${{ PYTHON }} -m pip install . -vv --no-deps --no-build-isolation\n      python:\n        entry_points:\n          - ariadne-codegen = ariadne_codegen.main:main\n    requirements:\n      host:\n        - hatchling\n        - pip\n        - python ${{ python_min }}.*\n      run:\n        - python >=${{ python_min }}\n        - autoflake\n        - black\n        - click >=8.1,<9.0\n        - graphql-core >=3.2.0,<3.3\n        - httpx >=0.23,<1.0\n        - isort\n        - pydantic >=2.0.0,<3.0.0\n        - toml >=0.10,<1.0\n      run_constraints:\n        - websockets >=14.2\n    tests:\n      - python:\n          imports: ariadne_codegen\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n      - requirements:\n          run:\n            - python ${{ python_min }}.*\n        script:\n          - ariadne-codegen --help\n\n  - package:\n      name: ariadne-codegen-with-websockets\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - ${{ pin_subpackage(\"ariadne-codegen\", exact=True) }}\n        - websockets\n    tests:\n      - python:\n          imports: ariadne_codegen\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n      - requirements:\n          run:\n            - python ${{ python_min }}.*\n        script:\n          - ariadne-codegen --help\n\n  - package:\n      name: ariadne-codegen-with-opentelemetry\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - ${{ pin_subpackage(\"ariadne-codegen\", exact=True) }}\n        - opentelemetry-api\n    tests:\n      - python:\n          imports: ariadne_codegen\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n      - requirements:\n          run:\n            - python ${{ python_min }}.*\n        script:\n          - ariadne-codegen --help\n\n  - package:\n      name: ariadne-codegen-with-all\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - ${{ pin_subpackage(\"ariadne-codegen-with-opentelemetry\", exact=True) }}\n        - ${{ pin_subpackage(\"ariadne-codegen-with-websockets\", exact=True) }}\n    tests:\n      - python:\n          imports: ariadne_codegen\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n      - files:\n          recipe:\n            - run_test_all.py\n          source:\n            - src/tests/\n        requirements:\n          run:\n            - ariadne\n            - coverage\n            - freezegun\n            - pip\n            - pytest\n            - pytest-asyncio\n            - pytest-httpx\n            - pytest-mock\n            - python ${{ python_min }}\n            - requests-toolbelt\n        script:\n          - pip check\n          - ariadne-codegen --help\n          - python run_test_all.py\n\nabout:\n  summary: Generate fully typed GraphQL client from schema, queries and mutations!\n  license: BSD-3-Clause\n  license_f