      "description": "libshumate is a C library providing a GtkWidget to display maps in GTK4\napplications. It supports numerous free map sources such as OpenStreetMap,\nOpenCycleMap, OpenAerialMap and Maps for free.",
      "dev_url": "https://gitlab.gnome.org/GNOME/libshumate",
      "doc_url": "https://gnome.pages.gitlab.gnome.org/libshumate/",
      "home": "https://gitlab.gnome.org/GNOME/libshumate",
      "license": "LGPL-2.1-or-later",
      "license_family": "LGPL-2.1-or-later",
      "license_file": "COPYING",
      "summary": "libshumate is a GTK4 widget to display maps."
    },
    "build": {
      "number": "1",
      "run_exports": {
        "weak": [
          "libshumate"
        ]
      },
      "script": "cp -f ${BUILD_PREFIX}/bin/glib-mkenums ${PREFIX}/bin/glib-mkenums\ncp -f ${BUILD_PREFIX}/bin/glib-genmarshal ${PREFIX}/bin/glib-genmarshal\nfor tool in glib-compile-resources gperf; do\n  echo \"#!/bin/bash\" > ${PREFIX}/bin/${tool}\n  echo \"exec ${BUILD_PREFIX}/bin/${tool} \\\"\\$@\\\"\" >> ${PREFIX}/bin/${tool}\n  chmod +x ${PREFIX}/bin/${tool}\ndone\nmeson setup ${MESON_ARGS} \\\n    --prefix=$PREFIX \\\n    --default-library=shared \\\n    --wrap-mode=nofallback \\\n    -Dgtk_doc=false \\\n    -Dvapi=false \\\n    -Ddemos=false \\\n    -Dgir=false \\\n    builddir\nninja -v -C builddir -j ${CPU_COUNT}\nninja -C builddir install -j ${CPU_COUNT}\n"
    },
    "extra": {
      "recipe-maintainers": [
        "haecker-felix",
        "Hofer-Julian"
      ]
