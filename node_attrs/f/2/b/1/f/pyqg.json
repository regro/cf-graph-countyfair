    "about": {
      "description": "Pyqg is a python solver for quasigeostrophic systems. Quasigeostophic\nequations are an approximation to the full fluid equations of motion in\nthe limit of strong rotation and stratitifcation and are most applicable\nto geophysical fluid dynamics problems.\n",
      "dev_url": "https://github.com/pyqg/pyqg",
      "doc_url": "https://pyqg.readthedocs.io/en/latest/",
      "home": "https://github.com/pyqg/pyqg",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "python quasigeostrophic model"
    },
    "build": {
      "number": "1",
      "script": "PYTHON -m pip install . -vv",
      "script_env": [
        "SETUPTOOLS_SCM_PRETEND_VERSION=0.7.2"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "rabernat",
        "mfjansen"
      ]
    },
    "package": {
      "name": "pyqg",
      "version": "0.7.2"
    },
    "requirements": {
