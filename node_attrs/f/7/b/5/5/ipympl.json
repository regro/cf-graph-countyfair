  "raw_meta_yaml": "# yaml-language-server: $schema=https://raw.githubusercontent.com/prefix-dev/recipe-format/main/schema.json\nschema_version: 1\n\ncontext:\n  version: \"0.9.8\"\n  # build against the oldest available 4.0.x to avoid spurious `etc/jupyter/labconfig/page_config.json`\n  jupyterlab_host_version: \"4.0\"\n  # test against the latest supported version\n  jupyterlab_test_version: \"4\"\n\npackage:\n  name: ipympl\n  version: ${{ version }}\n\nsource:\n  url: https://pypi.org/packages/source/i/ipympl/ipympl-${{ version }}.tar.gz\n  sha256: 6d7230d518384521093f3854f7db89d069dcd9c28a935b371e9c9f126354dee1\n\nbuild:\n  number: 0\n  noarch: python\n  script:\n    - ${{ PYTHON }} -m pip install . --no-deps -vv --no-build-isolation --disable-pip-version-check\n\nrequirements:\n  host:\n    - hatch-jupyter-builder >=0.8.3\n    - hatch-nodejs-version >=0.3.2\n    - hatchling\n    - jupyterlab ${{ jupyterlab_host_version }}.*\n    - nodejs 24.*\n    - pip\n    - python ${{ python_min }}.*\n  run:\n    - ipython <10\n    - ipywidgets >=7.6.0,<9\n    - matplotlib-base >=3.5.0,<4\n    - numpy\n    - pillow\n    - python >=${{ python_min }}\n    - traitlets <6\n\ntests:\n  - python:\n      imports: ipympl\n      pip_check: true\n      python_version:\n        - ${{ python_min }}.*\n        - 3.13.*\n  - package_contents:\n      files:\n        - share/jupyter/nbextensions/jupyter-matplotlib/extension.js\n        - share/jupyter/nbextensions/jupyter-matplotlib/index.js\n        - share/jupyter/labextensions/jupyter-matplotlib/package.json\n  - requirements:\n      run:\n        - python ${{ python_min }}.*\n        - jupyterlab ${{ jupyterlab_test_version }}.*\n    script:\n      - jupyter labextension list --debug\n      # can't test the npm version as it does not align with the pypi version\n      - jupyter labextension list --debug 2>&1 | python -c \"import sys, re; assert re.search('jupyter-matplotlib.*enabled.*OK', sys.stdin.read())\"\n\nabout:\n  license: BSD-3