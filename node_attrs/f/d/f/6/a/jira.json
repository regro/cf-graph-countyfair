age(\"jira-base\", exact=True) }}\n        - requests-futures\n    tests:\n      - python:\n          imports: jira\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n\n  - package:\n      name: jira\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - ${{ pin_subpackage(\"jira-base\", exact=True) }}\n        - ${{ pin_subpackage(\"jira-with-cli\", exact=True) }}\n    tests:\n      - python:\n          imports: jira\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n\nabout:\n  license: BSD-2-Clause\n  license_file: LICENSE\n  summary: Python library for interacting with JIRA via REST APIs.\n  homepage: https://github.com/pycontribs/jira\n  documentation: https://jira.readthedocs.io\n  description: |-\n    This library eases the use of the Jira REST API from Python and it has been\n    used in production for years.\n\nextra:\n  feedstock-name: jira\n  recipe-maintainers:\n    - ericdill\n    - parente\n    - bollwyvl\n",
