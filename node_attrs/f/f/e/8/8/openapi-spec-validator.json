    "about": {
      "description": "OpenAPI Spec Validator is a Python library that validates OpenAPI Specs\nagainst the OpenAPI 2.0 (aka Swagger) and OpenAPI 3.0 specification. The\nvalidator aims to check for full compliance with the Specification.",
      "home": "https://github.com/p1c2u/openapi-spec-validator",
      "license": "Apache-2.0",
      "license_family": "Apache-2.0",
      "license_file": "LICENSE",
      "summary": "OpenAPI 2.0 (aka Swagger) and OpenAPI 3 spec validator"
    },
    "build": {
      "noarch": "python",
      "number": "0",
      "script": "${{ PYTHON }} -m pip install . -vv --no-deps --no-build-isolation"
    },
    "extra": {
      "recipe-maintainers": [
        "rvalieris",
        "bollwyvl"
      ]
    },
    "outputs": [
      {
        "build": null,
        "name": "openapi-spec-validator",
        "requirements": {
          "build": [],
          "host": [
            "pip",
            "poetry-core >=1.0.0",
            "python 3.9.*"
          ],
          "run": [
            "importlib_resources >=5.8,<7.0",
            "jsonschema >=4.18.0,<5.0.0",
            "jsonschema-path >=0.3.1,<0.4.0",
            "lazy-object-proxy >=1.7.1,<2.0.0",
            "openapi-schema-validator >=0.6.0,<0.7.0",
            "python >=3.9"
          ]
        },
        "tests": [
          {
            "python": {
              "imports": [
                "openapi_spec_validator"
              ],
              "python_version": [
                "3.9.*",
                "3.13.*"
              ]
            }
          },
          {
            "files": {
              "source": [
                "tests"
              ]
            },
            "requirements": {
              "run": [
                "pytest-cov",
                "python 3.9.*"
              ]
            },
            "script": [
              "openapi-spec-validator --help",
              "coverage run --source=openapi_spec_validator --branch -m pytest -vv --tb=long --color=yes",
              "coverage report --show-missing --skip-covered --fail-under=92"
            ]
          }
        ],
        "version": "0.7.2"
      }
    ],
