    "about": {
      "description": "PlantSeg is a tool for cell instance aware segmentation in densely packed 3D volumetric images.\nThe pipeline uses a two stage segmentation strategy (Neural Network + Graph Partitioning).\nThe pipeline is tuned for plant cell tissue acquired with confocal and light sheet microscopy.\nPre-trained models are provided.\n",
      "dev_url": "https://github.com/kreshuklab/plant-seg",
      "doc_url": "https://kreshuklab.github.io/plant-seg/",
      "home": "https://kreshuklab.github.io/plant-seg/",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "Cell instance segmentation in densely packed 3D volumetric images"
    },
    "build": {
      "entry_points": [
        "plantseg = plantseg.run_plantseg:main"
      ],
      "noarch": "python",
      "number": "0",
      "script": [
        "PYTHON -m pip install . -vv --no-deps --no-build-isolation --ignore-installed --no-cache-dir",
        "mkdir -p PREFIX/Menu",
        "cp SRC_DIR/Menu/plant-seg.json PREFIX/Menu/plant-seg_menu.json",
        "cp SRC_DIR/Menu/icon.png PREFIX/Menu/icon.png",
        "cp SRC_DIR/Menu/logo_horizontal.png PREFIX/Menu/logo_horizontal.png",
        "cp SRC_DIR/Menu/logo_vertical.png PREFIX/Menu/logo_vertical.png"
      ]
    },
    "extra": {
