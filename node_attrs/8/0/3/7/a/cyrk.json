        "c_compiler_stub",
        "cxx_compiler_stub",
        "c_stdlib_stub",
        "numpy"
      ],
      "host": [
        "python",
        "setuptools",
        "numpy",
        "cython",
        "wheel",
        "pip"
      ],
      "run": [
