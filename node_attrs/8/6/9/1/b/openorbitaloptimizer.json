        "loriab"
      ]
    },
    "outputs": [
      {
        "build": {
          "run_exports": [
            "openorbitaloptimizer"
          ]
        },
        "name": "openorbitaloptimizer",
        "requirements": {
          "build": [
            "c_stdlib_stub",
