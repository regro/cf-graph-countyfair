  "linux_64_meta_yaml": {
    "about": {
      "description": "BGEN is a file format for storing large genetic datasets. It supports both unphased\ngenotypes and phased haplotype data with variable ploidy and number of alleles. It\nwas designed to provide a compact data representation without sacrificing variant\naccess performance. This Python package is a wrapper around the bgen library, a\nlow-memory footprint reader that efficiently reads bgen files.\n",
      "dev_url": "https://github.com/limix/cbgen",
      "doc_url": "https://cbgen.readthedocs.io/en/stable/",
      "home": "https://github.com/limix/cbgen",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "Python wrapper around a BGEN library"
    },
    "build": {
      "number": "2",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "Horta"
      ]
    },
    "package": {
      "name": "cbgen",
      "version": "1.0.1"
    },
    "requirements": {
      "build": [
        "c_compiler_stub"
      ],
      "host": [
        "bgen >=4.1.3",
        "cffi >=1.14.0",
        "pip",
        "python",
        "setuptools",
        "setuptools-scm >=4.1.2",
        "wheel >=0.34.2"
      ],
      "run": [
        "appdirs >=1.4.3",
        "bgen >=4.1.3",
        "cffi >=1.14.0",
        "numpy >=1.17.0",
        "pooch >=1.1.1",
        "pytest >=5.4.1",
        "python"
      ]
    },
    "schema_version": 0,
