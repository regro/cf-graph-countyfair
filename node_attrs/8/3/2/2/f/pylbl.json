  "linux_64_meta_yaml": {
    "about": {
      "description": "pyLBL provides a python interface to line-by-line calculations of absorption\ncoefficients that is traceable to benchmarks. The overall goal is to treat lines,\ncross-sections, continua, collision-induced absorption, and line mixing. It is\nwritten in Python 3.5+ and includes a C extenstion module by default.\n",
      "dev_url": "https://github.com/GRIPS-code/pyLBL",
      "doc_url": "https://pylbl-1.readthedocs.io",
      "home": "https://github.com/GRIPS-code/pyLBL",
      "license": "LGPL-2.1-only",
      "license_family": "LGPL",
      "license_file": "LICENSE.md",
      "summary": "A python package for calculating line-by-line molecular spectra"
    },
    "build": {
      "number": "1",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "RobertPincus",
        "menzel-gfdl"
      ]
    },
    "package": {
      "name": "pylbl",
      "version": "0.0.1"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "python",
