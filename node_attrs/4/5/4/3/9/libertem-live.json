  "linux_64_meta_yaml": {
    "about": {
      "description": "LiberTEM-live is an extension module for\nLiberTEM that allows live data processing.\n",
      "home": "https://libertem.github.io/LiberTEM-live",
      "license": "GPL-3.0-or-later AND MIT",
      "license_file": [
        "LICENSE",
        "src/libertem_live/detectors/LICENSE"
      ],
      "summary": "Live processing with LiberTEM"
    },
    "build": {
      "entry_points": [
        "libertem-live-mib-sim=libertem_live.detectors.merlin.sim:main",
        "libertem-live-dectris-sim=libertem_live.detectors.dectris.sim:main",
        "libertem-live-asi-tpx-sim=libertem_live.detectors.asi_tpx3.sim:main"
      ],
      "number": "2",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "sk1p",
        "uellue",
        "matbryan52"
      ]
    },
    "package": {
      "name": "libertem-live",
      "version": "0.2.1"
    },
    "requirements": {
      "host": [
        "python",
        "pip"
      ],
      "run": [
        "python",
        "libertem >=0.11.0",
        "numpy",
        "click",
        "tqdm",
        "numba",
        "opentelemetry-api",
        "sparseconverter",
        "libertem-dectris",
        "libertem-asi-tpx3"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "27ff8de94b1ea34ee0f2f0b8ef13203d903b52b54946add72c39ab8984e5fe39",
      "url": "https://pypi.io/packages/source/l/libertem-live/libertem-live-0.2.1.tar.gz"
    },
    "test": {
      "commands": [
        "pip check",
        "libertem-live-mib-sim --help",
        "libertem-live-dectris-sim --help",
        "libertem-live-asi-tpx-sim --help",
        "pytest tests"
      ],
      "imports": [
        "libertem_live"
      ],
      "requires": [
        "pip",
        "flask",
        "pytest >=6",
        "pytest-cov",
