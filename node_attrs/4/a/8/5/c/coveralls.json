        "thekevjames"
      ]
    },
    "package": {
      "name": "coveralls",
      "version": "4.0.1"
    },
    "requirements": {
      "host": [
        "pip",
        "poetry-core >=1.0.0",
        "python 3.9"
      ],
      "run": [
        "coverage[toml] >=5.0, <8.0",
