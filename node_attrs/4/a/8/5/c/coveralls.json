      "description": "coveralls.io is a service for publishing your coverage stats online. This\npackage provides seamless integration with coverage.py (and thus py.test,\nnosetests, etc...) in your Python projects.\n",
      "dev_url": "https://github.com/coveralls-clients/coveralls-python",
      "home": "https://github.com/coveralls-clients/coveralls-python",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE.rst",
      "summary": "Show coverage stats online via coveralls.io"
    },
    "build": {
      "entry_points": [
        "coveralls = coveralls.cli:main",
        "python-coveralls = coveralls.cli:main"
      ],
      "noarch": "python",
      "number": "1",
      "script": "PYTHON -m pip install . --no-deps -vv"
    },
    "extra": {
      "recipe-maintainers": [
