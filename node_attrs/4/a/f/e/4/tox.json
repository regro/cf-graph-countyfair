      "doc_url": "https://tox.readthedocs.org/",
      "home": "https://tox.wiki/",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "virtualenv-based automation of test activities"
    },
    "build": {
      "noarch": "python",
      "number": "0",
      "script": "${{ PYTHON }} -m pip install . -vv --no-deps --no-build-isolation --disable-pip-version-check"
    },
    "extra": {
      "recipe-maintainers": [
        "rxm7706",
        "cshaley",
