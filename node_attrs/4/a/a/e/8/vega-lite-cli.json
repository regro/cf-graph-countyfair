      "doc_url": "https://vega.github.io/vega-lite/usage/compile.html#cli",
      "home": "https://vega.github.io",
      "license": "BSD-3-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE",
      "summary": "Command line interface of the VEGA-lite visualization grammar"
    },
    "build": {
      "number": "0"
    },
