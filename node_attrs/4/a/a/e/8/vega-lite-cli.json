        "johanneskoester",
        "xhochy"
      ]
    },
    "package": {
      "name": "vega-lite-cli",
      "version": "6.1.0"
    },
    "requirements": {
      "build": [
        "nodejs"
      ],
      "host": [
        "nodejs",
        "yarn <2"
      ],
      "run": [
        "nodejs",
        "vega-cli"
      ]
    },
    "schema_version": 0,
