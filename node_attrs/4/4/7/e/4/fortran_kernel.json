  "linux_64_meta_yaml": {
    "about": {
      "description": "LFortran is a modern open-source (BSD licensed) interactive Fortran\ncompiler built on top of LLVM. It can execute user's code interactively to\nallow exploratory work (much like Python, MATLAB or Julia) as well as\ncompile to binaries with the goal to run user’s code on modern\narchitectures such as multi-core CPUs and GPUs.\n\nThis package provides the Jupyter kernel.\n",
      "dev_url": "https://gitlab.com/lfortran/fortran_kernel",
      "doc_url": "https://docs.lfortran.org/",
      "home": "https://lfortran.org",
      "license": "BSD-3",
      "license_family": "BSD",
      "license_file": "LICENSE",
      "summary": "Fortran Jupyter kernel based on LFortran"
    },
    "build": {
      "number": "0",
      "script": [
        "PYTHON -m pip install . -vv",
        "PYTHON -m fortran_kernel.install --prefix $PREFIX"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "certik"
      ]
    },
    "package": {
      "name": "fortran_kernel",
      "version": "0.1.7"
    },
    "requirements": {
      "host": [
        "python",
        "pip",
        "setuptools",
        "lfortran",
        "ipykernel"
      ],
      "run": [
        "python",
        "lfortran",
        "ipykernel"
      ]
    },
