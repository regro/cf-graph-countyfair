    "source": {
      "patches": null,
      "sha256": "62838595ff707ad34100834ac545faf35feb5884ebde552f4b4ed56aeee08dfe",
      "url": "https://gitlab.com/lfortran/fortran_kernel/-/archive/v0.1.7/fortran_kernel-v0.1.7.tar.bz2"
