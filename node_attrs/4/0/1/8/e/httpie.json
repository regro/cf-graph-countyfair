  "linux_64_meta_yaml": {
    "about": {
      "description": "HTTPie (pronounced aitch-tee-tee-pie) is a command line HTTP client. Its\ngoal is to make CLI interaction with web services as human-friendly as\npossible. It provides a simple http command that allows for sending\narbitrary HTTP requests using a simple and natural syntax, and displays\ncolorized output. HTTPie can be used for testing, debugging, and generally\ninteracting with HTTP servers.\n",
      "dev_url": "https://github.com/jakubroztocil/httpie",
      "doc_url": "https://httpie.org/doc",
      "home": "https://httpie.org/",
      "license": "BSD-3-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE",
      "summary": "a CLI, cURL-like tool for humans"
    },
    "build": {
      "entry_points": [
        "http = httpie.__main__:main",
        "https = httpie.__main__:main",
        "httpie = httpie.manager.__main__:main"
      ],
      "noarch": "python",
      "number": "1",
      "script": "PYTHON -m pip install . -vv --no-deps --no-build-isolation"
    },
    "extra": {
      "recipe-maintainers": [
        "djsutherland",
        "ocefpaf",
        "honnorat"
      ]
    },
    "package": {
      "name": "httpie",
      "version": "3.2.4"
    },
    "requirements": {
      "host": [
        "python 3.9",
        "setuptools",
        "pip"
      ],
      "run": [
        "__unix",
        "python >=3.9",
        "charset-normalizer >=2.0.0",
        "defusedxml >=0.6.0",
        "multidict >=4.7.0",
        "pip",
        "pygments >=2.5.2",
        "requests >=2.22.0",
        "requests-toolbelt >=0.9.1",
        "rich >=9.10.0",
        "setuptools"
      ]
    },
    "schema_version": 0,
