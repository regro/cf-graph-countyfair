  "linux_64_meta_yaml": {
    "about": {
      "description": "Filebutler is a utility for managing old files in huge directory trees.\nHuge means of the order of 100 million files. The motivation is that\nfind is far too slow on directory trees with this many files. Even\nworking with filelists generated offline by find can be rather slow for\ninteractive use. Filebutler improves on this by structuring filelists by\nage, by size, by owner, and by dataset.\n",
      "dev_url": "https://github.com/tesujimath/filebutler",
      "doc_url": "http://filebutler.readthedocs.io/",
      "home": "https://github.com/tesujimath/filebutler",
      "license": "GPL-3.0",
      "license_family": "GPL",
      "license_file": "LICENSE",
      "summary": "Utility for managing old files in large directory structures"
    },
    "build": {
      "entry_points": [
        "filebutler = filebutler.__main__:main"
      ],
      "number": "4",
      "script": "python setup.py install --single-version-externally-managed --record record.txt"
    },
    "extra": {
      "recipe-maintainers": [
        "tesujimath"
      ]
    },
    "package": {
      "name": "filebutler",
      "version": "0.27.1"
    },
