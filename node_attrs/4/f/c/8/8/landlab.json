  "linux_64_meta_yaml": {
    "about": {
      "description": "Landlab creates an environment in which scientists can build\na numerical landscape model without having to code all of the\nindividual components. Landscape models compute flows of mass,\nsuch as water, sediment, glacial ice, volcanic material, or\nlandslide debris, across a gridded terrain surface. Landscape\nmodels have a number of commonalities, such as operating on a\ngrid of points and routing material across the grid. Scientists\nwho want to use a landscape model often build their own unique\nmodel from the ground up, re-coding the basic building blocks\nof their landscape model rather than taking advantage of codes\nthat have already been written.\n",
      "dev_url": "https://github.com/landlab/landlab",
      "doc_url": "https://landlab.readthedocs.io",
      "home": "https://landlab.github.io",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE.md",
      "summary": "A python toolkit for modeling earth surface processes"
    },
    "build": {
      "entry_points": [
        "landlab = landlab.cmd.landlab:landlab"
      ],
      "number": "0",
      "script": [
        "PYTHON -m pip install . --no-deps -vv",
        "PYTHON -c \"import pathlib, site, shutil; shutil.rmtree(pathlib.Path(site.getsitepackages()[0]) / 'tests', ignore_errors=True)\""
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "mcflugen",
        "lsetiawan",
        "ocefpaf"
      ]
    },
    "package": {
      "name": "landlab",
      "version": "2.10.1"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub",
        "cxx_compiler_stub"
      ],
      "host": [
        "python",
        "pip",
        "cython >=3",
        "numpy",
        "setuptools"
      ],
      "run": [
        "python",
        "importlib-resources",
        "bmipy",
        "matplotlib-base",
        "netcdf4",
        "pandas",
        "pyshp",
        "pyyaml",
        "rich-click",
        "richdem >=2.3",
        "setuptools",
        "scipy",
        "statsmodels",
        "xarray >=0.16"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "e6d14dbb9f06b4591895fbadce89cb8743c3e6e45b77190e4f077bb9ef42989e",
      "url": "https://github.com/landlab/landlab/archive/v2.10.1.tar.gz"
    },
    "test": {
