            "python >=3.10"
          ]
        },
        "tests": [
          {
            "python": {
              "imports": [
                "pytest_mypy_testing"
              ],
              "python_version": [
                "3.10.*",
                "3.14.*"
              ]
            }
          },
          {
            "files": {
              "source": [
                "tests/",
