              ]
            },
            "requirements": {
              "run": [
                "coverage",
                "python 3.10.*"
              ]
            },
            "script": [
              "coverage run --source=pytest_mypy_testing -m pytest -vv --tb=long --color=yes tests mypy_tests --doctest-modules -p no:mypy-testing",
              "coverage report --show-missing --skip-covered --fail-under=99"
            ]
          }
        ],
        "version": "0.2.0"
