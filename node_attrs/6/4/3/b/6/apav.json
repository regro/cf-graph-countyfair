  "linux_64_meta_yaml": {
    "about": {
      "description": "APAV (Atom Probe Analysis and Visualization) is a Python library\nfor the analysis and visualization of atom probe tomography\nexperiments\n",
      "dev_url": "https://gitlab.com/jesseds/apav",
      "doc_url": "https://apav.readthedocs.io/en/latest/",
      "home": "https://gitlab.com/jesseds/apav",
      "license": "GPL-3.0-or-later",
      "license_family": "GPL",
      "license_file": "LICENSE",
      "summary": "APAV: Python analysis for atom probe tomography"
    },
    "build": {
      "number": "3",
      "script": "PYTHON -m pip install . -vv",
      "script_env": [
        "QT_QPA_PLATFORM=offscreen"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "jan-janssen"
      ]
    },
    "package": {
      "name": "apav",
      "version": "1.4.0"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub",
        "libgomp"
      ],
      "host": [
        "pip",
        "python",
        "poetry",
        "cython"
      ],
      "run": [
        "fast-histogram",
        "lmfit >=1.0",
        "numba",
        "numpy >=1.17",
        "periodictable",
        "pyqt >=5.11",
        "pyqtgraph >=0.11.0",
        "pytest >=5",
        "pytest-qt >=3",
        "python",
        "tabulate"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "ae26caf1908da04ab88cfa6f16532ad8cb09548c2c11de40d95703afbc4f45d6",
      "url": "https://pypi.org/packages/source/a/apav/APAV-1.4.0.tar.gz"
    },
    "test": {
      "commands": [
        "pip check"
      ],
      "imports": [
        "apav",
        "apav.analysis"
      ],
      "requires": [
        "pip"
      ]
    }
  },
