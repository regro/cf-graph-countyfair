  "raw_meta_yaml": "{% set version = \"2.0.7\" %}\n{% set posix = 'm2-' if win else '' %}\n{% set native = 'm2w64-' if win else '' %}\n\npackage:\n  name: r-nlmixr\n  version: {{ version|replace(\"-\", \"_\") }}\n\nsource:\n  url:\n    - {{ cran_mirror }}/src/contrib/nlmixr_{{ version }}.tar.gz\n    - {{ cran_mirror }}/src/contrib/Archive/nlmixr/nlmixr_{{ version }}.tar.gz\n  sha256: 8bee46b48316a2d3434794099629a8a1d5f844a860deccb65d33e7e451861abb\n\nbuild:\n  merge_build_host: true  # [win]\n  number: 1\n  rpaths:\n    - lib/R/lib/\n    - lib/\n  # Unskip win when \"CXX14 not defined\" issue has a fix\n  skip: true  # [win or r_base == \"3.6\"]\n\nrequirements:\n  build:\n    - {{ compiler('c') }}              # [not win]\n    - {{ compiler('m2w64_c') }}        # [win]\n    - {{ compiler('cxx') }}            # [not win]\n    - {{ compiler('m2w64_cxx') }}      # [win]\n    - {{ posix }}filesystem        # [win]\n    - {{ posix }}sed               # [win]\n    - {{ posix }}grep              # [win]\n    - {{ posix }}autoconf\n    - {{ posix }}automake          # [not win]\n    - {{ posix }}automake-wrapper  # [win]\n    - {{ posix }}pkg-config\n    - {{ posix }}make\n    - {{ posix }}coreutils         # [win]\n    - {{ posix }}zip               # [win]\n  host:\n    - libblas\n    - liblapack\n    - r-base\n    - r-bh\n    - r-matrix\n    - r-rcpp >=0.12.3\n    - r-rcpparmadillo >=0.5.600.2.0\n    - r-rcppeigen >=0.3.3.3.0\n    - r-rxode >=1.0.6\n    - r-stanheaders >=2.18.0\n    - r-backports\n    - r-brew\n    - r-dparser\n    - r-fastghquad\n    - r-ggplot2\n    - r-lbfgsb3c\n    - r-magrittr\n    - r-minqa\n    - r-n1qn1 >=6.0.1_10\n    - r-nlme\n    - r-rex\n    - r-symengine\n  run:\n    - libblas\n    - liblapack\n    - r-base\n    - {{ native }}gcc-libs         # [win]\n    #- r-bh\n    - r-matrix\n    - r-rcpp >=0.12.3\n    - r-rcpparmadillo >=0.5.600.2.0\n    - r-rcppeigen >=0.3.3.3.0\n    - r-rxode >=1.0.6\n    - r-stanheaders >=2.18.0\n    - r-backports\n