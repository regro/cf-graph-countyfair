  "linux_64_meta_yaml": {
    "about": {
      "description": "tardigrade_error_tools A collection of tools used for error handling. These tools provide a\nlinked list class which can help a user both understand where an error happened and also help the developer to\ncorrect for the errors.\n",
      "dev_url": "https://github.com/UCBoulder/tardigrade_error_tools",
      "home": "https://github.com/UCBoulder/tardigrade_error_tools",
      "license": "BSD-3-Clause",
      "license_file": "LICENSE",
      "summary": "A collection of tools used for error handling. "
    },
    "build": {
      "number": "2",
      "script": [
        "set -x",
        "cmake -S . -B build -DCMAKE_BUILD_TYPE=Release ${CMAKE_ARGS}",
        "cmake --build build --target tardigrade_error_tools tardigrade_error_tools_PYTHON",
        "cmake --install build --prefix ${PREFIX}",
        "$PYTHON -m pip install build/src/python/dist/$PKG_NAME-$PKG_VERSION.tar.gz --no-deps --ignore-installed -v --no-build-isolation",
        "$PYTHON -m pip show $PKG_NAME"
      ],
      "script_env": [
        "SETUPTOOLS_SCM_PRETEND_VERSION=0.9.7"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "NateAM",
        "kbrindley"
      ]
    },
    "package": {
      "name": "tardigrade_error_tools",
      "version": "0.9.7"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
