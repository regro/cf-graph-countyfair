  "feedstock_name": "gmt",
  "hash_type": "sha256",
  "linux_64_meta_yaml": {
    "about": {
      "description": "GMT is an open source collection of about 80 command-line tools for\nmanipulating geographic and Cartesian data sets (including filtering,\ntrend fitting, gridding, projecting, etc.) and producing PostScript\nillustrations ranging from simple x–y plots via contour maps to\nartificially illuminated surfaces and 3D perspective views; the GMT\nsupplements add another 40 more specialized and discipline-specific\ntools. GMT supports over 30 map projections and transformations\n",
      "dev_url": "https://github.com/GenericMappingTools/gmt",
      "doc_url": "https://docs.generic-mapping-tools.org",
      "home": "https://www.generic-mapping-tools.org",
      "license": "LGPL-3.0-or-later",
      "license_file": "LICENSE.TXT",
      "summary": "The Generic Mapping Tools."
    },
    "build": {
      "number": "2"
    },
    "extra": {
      "recipe-maintainers": [
        "ocefpaf",
        "leouieda",
        "seisman"
      ]
    },
    "package": {
      "name": "gmt",
      "version": "6.6.0"
    },
    "requirements": {
      "build": [
        "cmake",
        "c_compiler_stub",
        "c_stdlib_stub",
        "make"
      ],
      "host": [
        "libblas",
        "libcblas",
        "liblapack",
        "fftw",
        "geos",
        "glib",
        "ghostscript",
        "libgdal-core",
        "libnetcdf",
        "hdf5",
        "zlib",
        "curl",
        "pcre",
        "gshhg-gmt",
        "dcw-gmt"
      ],
      "run": [
        "fftw",
        "geos",
        "ghostscript",
        "libgdal-core",
        "libgdal-jp2openjpeg",
        "libnetcdf",
        "hdf5",
        "zlib",
        "curl",
        "pcre",
        "gshhg-gmt",
        "dcw-gmt"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "28c0e56a31d8f3606da18a8e7473eb40647b031df21c69a3e42255953c95bfe4",
      "url": "https://github.com/GenericMappingTools/gmt/releases/download/6.6.0/gmt-6.6.0-src.tar.gz"
    },
    "test": {
      "commands": [
        "gmt defaults -Vd",
        "gmt psbasemap -R10/70/-3/8 -JX4i/3i -Ba -B+glightred+t\"TEST\" -P -Vd > test0.ps",
        "gmt pscoast -R0/10/0/10 -JM6i -Ba -Ggray -ENG+p1p,blue -P -Vd > test1.ps",
        "gmt begin -Vd",
        "gmt coast -R0/10/0/10 -JM6i -Ba -Ggray -ENG+p1p,blue -Vd",
        "gmt end",
        "gmt-config --all",
        "gmt grdmath -Rg -I1 0 90 SDIST = dist_to_NP.nc -x2",
        "gmt grdcut @earth_relief_05m -R-10/-9/3/5 -Greliefcut.jp2=gd:JP2OpenJPEG",
        "[ -e 'reliefcut.jp2' ] && exit 0 || exit 1"
      ]
    }
  },
