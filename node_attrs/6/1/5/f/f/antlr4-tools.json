  "raw_meta_yaml": "# yaml-language-server: $schema=https://raw.githubusercontent.com/prefix-dev/recipe-format/main/schema.json\nschema_version: 1\n\ncontext:\n  name: antlr4-tools\n  version: \"0.2.2\"\n\npackage:\n  name: ${{ name|lower }}\n  version: ${{ version }}\n\nsource:\n  url: https://pypi.org/packages/source/${{ name[0] }}/${{ name }}/${{ name | replace('-', '_') }}-${{ version }}.tar.gz\n  sha256: 8af6fba512fc168e48eb93690ed21bdbe8848ed812d12b365d9d259a26ad6ba3\n\nbuild:\n  number: 1\n  noarch: python\n  script: ${{ PYTHON }} -m pip install . -vv\n  python:\n    entry_points:\n      - antlr4=antlr4_tool_runner:tool\n      - antlr4-parse=antlr4_tool_runner:interp\n\nrequirements:\n  host:\n    - python ${{ python_min }}.*\n    - pip\n    - setuptools\n  run:\n    - python >=${{ python_min }},<4.0\n    # setup.py requires install-jdk to pass pip check, added install-jdk to requirements run section.\n    - install-jdk\n    - openjdk\n\ntests:\n  - python:\n      imports:\n        - antlr4_tool_runner\n      python_version: ${{ python_min }}.*\n      pip_check: true\n  - requirements:\n      run:\n        - pip\n        - python ${{ python_min }}.*\n    script:\n      - pip check\n      - antlr4-parse --help\n\nabout:\n  summary: Tools to run ANTLR4 tool and grammar interpreter/profiler\n  license: MIT\n  license_file: LICENSE\n  homepage: http://www.antlr.org\n\nextra:\n  recipe-maintainers:\n    - rxm7706\n",
