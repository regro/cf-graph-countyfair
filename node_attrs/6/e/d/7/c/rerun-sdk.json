  "raw_meta_yaml": "{% set name = \"rerun-sdk\" %}\n{% set version = \"0.29.2\" %}\n\npackage:\n  name: {{ name|lower }}\n  version: {{ version }}\n\nsource:\n  url: https://github.com/rerun-io/rerun/archive/refs/tags/{{ version }}.tar.gz\n  sha256: 9619626caa266ce614ae47547806cd65f9c0fa4cc974f98429fabbbf56dbd794\n  patches:\n    # We cant build with wayland support on conda-forge\n    # See: https://github.com/conda-forge/rerun-sdk-feedstock/issues/41\n    - disable_wayland.patch\n\nbuild:\n  number: 0\n  skip: true  # [py<38]\n  # nodejs doesn't support ppc64le\n  skip: true  # [ppc64le]\n  entry_points:\n    - rerun = rerun.__main__:main\n  script_env:\n    - AR\n\nrequirements:\n  build:\n    - cross-python_{{ target_platform }}     # [build_platform != target_platform]\n    - crossenv                               # [build_platform != target_platform]\n    - python                                 # [build_platform != target_platform]\n    - maturin >=1.3.0                        # [build_platform != target_platform]\n    - {{ compiler('c') }}\n    - {{ stdlib(\"c\") }}\n    - {{ compiler('cxx') }}\n    - {{ compiler('rust') }}  # [target_platform != \"osx-arm64\"]\n    - rust {{ rust_compiler_version }}  # [target_platform == \"osx-arm64\"]\n    - rust-std-aarch64-apple-darwin  # [target_platform == \"osx-arm64\"]\n    - cargo-bundle-licenses\n    - rust-std-wasm32-unknown-unknown\n    # binaryen gives us wasm-opt, for optimizing the an .wasm file for speed and size\n    - binaryen\n    - nodejs >=20.12\n    - nasm\n    - clang ==16.0.6  # [not osx]\n    # needed to get protoc for to lance\n    - libprotobuf\n  host:\n    - anywidget\n    - hatch\n    - hatch-jupyter-builder\n    - python\n    - pip\n    - maturin >=1.3.0\n  run:\n    - python\n    - attrs >=23.1.0\n    - anywidget\n    - jupyter-ui-poll\n    - numpy >=1.23\n    - pillow\n    - pyarrow >=14.0.2\n    - typing_extensions >=4.5\n\n# TODO(jleibs): Would be nice to add this constraint, but it causes