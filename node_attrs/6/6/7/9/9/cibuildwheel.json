  "linux_64_meta_yaml": {
    "about": {
      "description": "Python wheels are great. Building them across Mac, Linux, Windows,\non multiple versions of Python, is not.\ncibuildwheel is here to help. cibuildwheel runs on your CI server -\ncurrently it supports GitHub Actions, Azure Pipelines, Travis CI,\nAppVeyor, CircleCI, and GitLab CI - and it builds and tests your\nwheels across all of your platforms.",
      "doc_url": "https://cibuildwheel.pypa.io/en/stable/",
      "home": "https://github.com/pypa/cibuildwheel",
      "license": "BSD-2-Clause",
      "license_family": "BSD-2-Clause",
      "license_file": "LICENSE",
      "summary": "Building wheels across Mac, Linux, Windows, on multiple versions of Python."
    },
    "build": {
      "noarch": "python",
      "number": "0",
      "script": "${{ PYTHON }} -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "henryiii",
        "MementoRC"
      ]
    },
    "outputs": [
      {
        "build": null,
        "name": "cibuildwheel",
        "requirements": {
          "build": [],
          "host": [
            "hatchling",
            "python 3.11.*",
            "pip"
          ],
          "run": [
            "python >=3.11",
            "bashlex",
            "bracex",
            "certifi",
            "dependency-groups",
            "filelock",
            "humanize",
            "packaging",
            "platformdirs",
            "pyelftools",
            "python-build",
            "wheel"
          ]
        },
        "tests": [
          {
            "python": {
              "imports": [
                "cibuildwheel"
              ],
              "python_version": "3.11.*"
            }
          },
          {
            "files": {
              "source": [
                "test/"
              ]
            },
            "requirements": {
              "run": [
                "pip",
                "jinja2",
                "pytest >=6",
                "pytest-timeout",
                "pytest-xdist",
                "python-build",
                "validate-pyproject",
                "python 3.11.*"
              ]
            },
            "script": [
              "pip check",
              "cibuildwheel --help",
              "pytest test/test_dependency_versions.py",
              "pytest test/test_emulation.py",
              "pytest test/test_macos_archs.py",
              "pytest test/test_wheel_tag.py",
              "pytest test/test_windows.py"
            ]
          }
        ]
      }
    ],
