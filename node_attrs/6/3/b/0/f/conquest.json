  "linux_64_meta_yaml": {
    "about": {
      "description": "CONQUEST is a DFT code designed for large-scale calculations, with\nexcellent parallelisation. It gives a consistent, exact diagonalisation\napproach for systems from 1 to 10,000+ atoms, and brings the possibility\nof linear scaling calculations on over 1,000,000 atoms. The code has\nbeen demonstrated on scaling to nearly 200,000 cores and 2,000,000 atoms.\n",
      "dev_url": "https://github.com/OrderN/CONQUEST-release",
      "doc_url": "https://conquest.readthedocs.io",
      "home": "http://www.order-n.org",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "CONQUEST: Large-scale DFT calculations"
    },
    "build": {
      "number": "4"
    },
    "extra": {
      "recipe-maintainers": [
        "jan-janssen"
      ]
    },
    "package": {
      "name": "conquest",
      "version": "1.3"
    },
    "requirements": {
      "build": [
        "fortran_compiler_stub",
        "c_stdlib_stub",
        "make",
        "perl",
        "libgomp"
      ],
      "host": [
        "mpich",
        "libblas",
        "liblapack",
        "fftw",
        "scalapack",
        "libxc",
        "openmpi"
      ],
      "run": [
        "scalapack"
      ]
    },
    "schema_version": 0,
    "source": {
