  "linux_64_meta_yaml": {
    "about": {
      "description": "AbiPy is a Python library to analyze the results produced by\nAbinit, an open-source program for the ab-initio calculations\nof the physical properties of materials within Density Functional\nTheory and Many-Body perturbation theory. It also provides tools\nto generate input files and workflows to automate ab-initio\ncalculations and typical convergence studies. AbiPy is\ninterfaced with pymatgen and this allows users to benefit from\nthe different tools and python objects available in the pymatgen\necosystem.\n",
      "dev_url": "https://github.com/abinit/abipy",
      "doc_url": "http://abinit.github.io/abipy",
      "home": "http://abinit.github.io/abipy",
      "license": "GPL-2.0-only",
      "license_family": "GPL",
      "license_file": "LICENSE",
      "summary": "Open-source library for analyzing the results produced by ABINIT"
    },
    "build": {
      "noarch": "python",
      "number": "1",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "jan-janssen",
        "gmatteo"
      ]
    },
    "package": {
      "name": "abipy",
      "version": "0.9.8"
    },
    "requirements": {
      "host": [
        "python 3.9",
        "pip",
        "setuptools"
      ],
      "run": [
