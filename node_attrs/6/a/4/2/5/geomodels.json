  "linux_64_meta_yaml": {
    "about": {
      "description": "GeoModels provides tools for the management of Earth models like\ngeoids, gravity models, and magnetic field models.\nIt also provides some utility function to download and install\nsupport data to that are necessary for Earth models computation.\nIt is basically a Cython_ wrapper for part of the the GeographicLib\nC++ library.\n",
      "dev_url": "https://github.com/avalentino/geomodels",
      "doc_url": "https://geomodels.readthedocs.io",
      "home": "https://github.com/avalentino/geomodels",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "Python package for Earth data models management"
    },
    "build": {
      "entry_points": [
        "geomodels-cli = geomodels.cli:main"
      ],
      "number": "2",
      "script": "PYTHON -m pip install . -vv --no-deps --no-build-isolation",
      "script_env": [
        "GEOMODELS_IGNORE_BUNDLED_LIBS=TRUE"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "avalentino"
      ]
    },
    "package": {
      "name": "geomodels",
      "version": "1.0.0"
    },
    "requirements": {
      "build": [
        "cxx_compiler_stub",
        "c_stdlib_stub"
      ],
      "host": [
        "python",
        "cython",
        "geographiclib-cpp >=2.0",
        "numpy",
        "pip",
        "setuptools"
      ],
      "run": [
        "argcomplete",
        "geographiclib-cpp >=2.0",
        "numpy",
        "python",
        "tqdm"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "3ca0a607892317ba306a0e2d0a9d0bc6a856771c0c683cb5db3eddfec083d762",
      "url": "https://pypi.org/packages/source/g/geomodels/geomodels-1.0.0.tar.gz"
    },
    "test": {
      "commands": [
        "pip check",
        "geomodels-cli --help",
        "geomodels-cli install-data -d data recommended",
        "python -m geomodels info",
        "env GEOGRAPHICLIB_DATA=./data python -m pytest --pyargs geomodels"
      ],
      "imports": [
        "geomodels",
        "geomodels.tests"
      ],
      "requires": [
        "pip",
        "pytest"
      ]
    }
  },
