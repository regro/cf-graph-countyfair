  "linux_64_meta_yaml": {
    "about": {
      "description": "The LIGO Light-Weight XML format is used extensively by compact object\ndetection pipeline and associated tool sets.\nThis package provides a Python I/O library for reading, writing, and\ninteracting with documents in this format.\n",
      "dev_url": "https://git.ligo.org/computing/software/igwn-ligolw.git",
      "doc_url": "https://igwn-ligolw.readthedocs.io",
      "home": "https://git.ligo.org/computing/software/igwn-ligolw/",
      "license": "GPL-3.0-or-later",
      "license_file": "LICENSE",
      "summary": "Python LIGO Light-Weight XML I/O Library"
    },
    "build": {
      "number": "0",
      "script": "PYTHON -m pip install . -vv --no-deps --no-build-isolation"
    },
    "extra": {
      "recipe-maintainers": [
        "duncanmmacleod",
        "lpsinger",
        "titodalcanton"
      ]
    },
    "package": {
      "name": "igwn-ligolw",
      "version": "2.1.0"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub"
      ],
      "host": [
        "pip",
        "python",
        "setuptools >=64",
        "setuptools-scm >=8"
      ],
      "run": [
        "igwn-segments",
        "numpy",
        "python",
        "python-dateutil",
        "pyyaml",
        "tqdm"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "02a7201445d5dba8179a28f097b1ca9839a6564a5503201ee922dcbc9aa31c9d",
