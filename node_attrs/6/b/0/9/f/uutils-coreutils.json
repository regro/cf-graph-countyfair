  "linux_64_meta_yaml": {
    "about": {
      "description": "uutils is an attempt at writing universal (as in cross-platform) CLI utilities in Rust.\nuutils aims to work on as many platforms as possible,\nto be able to use the same utils on Linux, Mac, Windows and other platforms.\nThis ensures, for example, that scripts can be easily transferred between platforms.\nRust was chosen not only because it is fast and safe,\nbut is also excellent for writing cross-platform code.\n",
      "dev_url": "https://github.com/uutils/coreutils",
      "doc_url": "https://uutils.github.io/coreutils/docs/",
      "home": "https://uutils.github.io/coreutils/",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": [
        "LICENSE",
        "THIRDPARTY.yml"
      ],
      "summary": "Cross-platform Rust rewrite of the GNU coreutils"
    },
    "build": {
      "ignore_run_exports": [
        "libstdcxx",
        "libstdcxx-ng"
      ],
      "ignore_run_exports_from": null,
      "number": "0"
    },
    "extra": {
      "recipe-maintainers": [
        "sshockwave"
      ]
    },
    "package": {
      "name": "uutils-coreutils",
      "version": "0.1.0"
    },
    "requirements": {
      "build": [
        "c_stdlib_stub",
        "cxx_compiler_stub",
        "rust_compiler_stub",
        "cdt_stub",
        "make",
        "cargo-bundle-licenses",
        "libclang"
      ],
      "run_constrained": [
        "coreutils ==999999999",
        "gnu-coreutils ==999999999",
        "m2-coreutils ==999999999"
      ]
    },
    "schema_version": 0,
    "source": {
      "patches": [
        "0002-SHORTEN_SH_COMMANDS.patch",
        "0003-DISABLE_SELINUX.patch"
      ],
      "sha256": "55c528f2b53c1b30cb704550131a806e84721c87b3707b588a961a6c97f110d8",
      "url": "https://github.com/uutils/coreutils/archive/0.1.0.tar.gz"
    },
    "test": {
      "commands": [
