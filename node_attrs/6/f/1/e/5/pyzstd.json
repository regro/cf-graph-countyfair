  "feedstock_name": "pyzstd",
  "hash_type": "sha256",
  "linux_64_meta_yaml": {
    "about": {
      "description": "Pyzstd module provides classes and functions for compressing and decompressing data, using Facebook's Zstandard (or zstd as short name) algorithm. The API style is similar to Python's bz2/lzma/zlib modules.\n",
      "doc_url": "https://pyzstd.readthedocs.io/",
      "home": "https://github.com/Rogdham/pyzstd",
      "license": "BSD-3-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE",
      "summary": "Python bindings to Zstandard (zstd) compression library"
    },
    "build": {
      "number": "0",
      "script": "PYTHON -m pip install --config-settings=\"--build-option=--dynamic-link-zstd\" -vv ."
    },
    "extra": {
      "recipe-maintainers": [
        "rogdham"
      ]
    },
    "package": {
      "name": "pyzstd",
      "version": "0.17.0"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub"
      ],
      "host": [
        "python",
        "pip",
        "setuptools",
        "zstd"
      ],
      "run": [
        "python",
        "typing-extensions >=4.13.2"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "d84271f8baa66c419204c1dd115a4dec8b266f8a2921da21b81764fa208c1db6",
      "url": "https://pypi.org/packages/source/p/pyzstd/pyzstd-0.17.0.tar.gz"
    },
    "test": {
      "commands": [
        "python -m unittest -v"
      ],
      "source_files": [
        "tests/"
      ]
    }
  },
