        "pkg-config >=0.18.0"
      ],
      "host": [
        "gobject-introspection >=1.30.0",
        "gsl",
        "gst-plugins-base",
        "gstlal >=1.10.0",
        "gstreamer",
        "libglib",
        "liblal >=7.2.4",
        "liblalburst >=1.7.0",
        "liblalmetaio",
        "python"
      ],
      "run": [
        "gobject-introspection >=1.30.0",
        "gst-python",
        "gstlal >=1.10.0",
