  "linux_64_meta_yaml": {
    "about": {
      "description": "fontTools is a library for manipulating fonts, written in Python. The\nproject includes the TTX tool, that can convert TrueType and OpenType\nfonts to and from an XML text format, which is also called TTX. It\nsupports TrueType, OpenType, AFM and to an extent Type 1 and some\nMac-specific formats. The project has a BSD-style open-source licence.\n",
      "dev_url": "https://github.com/fonttools/fonttools",
      "doc_url": "https://groups.google.com/forum/#!forum/fonttools",
      "home": "https://github.com/fonttools/fonttools",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "fontTools is a library for manipulating fonts, written in Python."
    },
    "build": {
      "entry_points": [
        "fonttools = fontTools.__main__:main",
        "ttx = fontTools.ttx:main",
        "pyftsubset = fontTools.subset:main",
        "pyftmerge = fontTools.merge:main"
      ],
      "noarch": "python",
      "number": "0",
      "script": [
        "PYTHON -m pip install . --no-deps -vv",
        "export FONTTOOLS_WITH_CYTHON=0"
      ],
      "track_features": [
        "fonttools_no_compile"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "CJ-Wright",
        "dopplershift",
        "tacaswell"
      ]
    },
    "package": {
      "name": "fonttools",
      "version": "4.60.0"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub"
      ],
      "host": [
        "cython",
        "pip",
        "setuptools",
        "python",
        "python 3.10"
      ],
      "run": [
        "python",
        "brotli",
        "munkres",
        "unicodedata2 >=15.1.0",
        "python >=3.10"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "8f5927f049091a0ca74d35cce7f78e8f7775c83a6901a8fbe899babcc297146a",
      "url": "https://pypi.org/packages/source/f/fonttools/fonttools-4.60.0.tar.gz"
    },
    "test": {
      "commands": [
        "pip check",
