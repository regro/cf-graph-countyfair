      "url": "https://pypi.org/packages/source/s/shortuuid/shortuuid-1.0.13.tar.gz"
    },
    "test": {
      "commands": [
        "pip check",
        "shortuuid --help",
        "coverage run --branch --source=shortuuid -m unittest shortuuid.test_shortuuid",
        "coverage report --show-missing --skip-covered --fail-under=90"
      ],
      "imports": [
        "shortuuid"
      ],
