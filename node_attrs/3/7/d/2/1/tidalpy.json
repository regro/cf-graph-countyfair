  "linux_64_meta_yaml": {
    "about": {
      "description": "TidalPy provides efficient tools to estimate tidal dissipation within rocky and icy worlds.\nThe RaidalSolver package includes tools to calculate a planet or moon's \"Love\" numbers.\nThe Tides package includes tools to estimate tidal heating and spin-orbit evolution including the effects from\nhigher-order eccentricity and obliquity terms which are often left out of other models.\n",
      "dev_url": "https://github.com/jrenaud90/TidalPy",
      "doc_url": "https://github.com/jrenaud90/TidalPy/tree/main/Documentation",
      "home": "www.tidalpy.info/",
      "license": "CC-BY-SA-4.0",
      "license_file": "LICENSE.md",
      "summary": "Tidal Dynamics and Thermal-Orbital Evolution Software Suite Implemented in Cython and Python"
    },
    "build": {
      "number": "1",
      "script": "PYTHON -m pip install . -vv --no-deps --no-build-isolation --no-binary cyrk"
    },
    "extra": {
      "recipe-maintainers": [
        "jrenaud90"
      ]
    },
    "package": {
      "name": "tidalpy",
      "version": "0.6.5"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "cxx_compiler_stub",
        "c_stdlib_stub"
      ],
      "host": [
        "python",
