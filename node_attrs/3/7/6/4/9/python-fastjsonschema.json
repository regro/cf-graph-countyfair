          ],
          "run": [
            "python >=3.9"
          ]
        },
        "tests": [
          {
            "python": {
              "imports": [
                "fastjsonschema"
              ],
              "python_version": [
                "3.9.*",
                "3.13.*"
              ]
            }
          },
          {
            "files": {
              "source": [
                "src/tests/"
              ]
            },
            "requirements": {
              "run": [
                "coverage",
                "pytest-benchmark",
                "python 3.9.*"
              ]
            },
            "script": [
              "cd src/tests",
              "coverage run --source fastjsonschema -m pytest -vv --tb=long --color=yes -k \"not (compile_to_code_ipv6_regex or compile_to_code_custom_format_with_refs)\"",
              "coverage report --show-missing --skip-covered --fail-under 87"
            ]
          }
        ],
        "version": "2.21.2"
      }
