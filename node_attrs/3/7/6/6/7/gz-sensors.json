  "raw_meta_yaml": "schema_version: 1\n\ncontext:\n  component_name: sensors\n  repo_name: ${{ \"gz-\" ~ component_name }}\n  version: 10.0.0\n  major_version: \"10\"\n  name: ${{ repo_name }}\n  component_version: ${{ component_name ~ major_version }}\n  cxx_name: ${{ \"lib\" ~ name }}\n\nrecipe:\n  name: ${{ name }}\n  version: ${{ version }}\n\nsource:\n  - url: https://github.com/gazebosim/${{ repo_name }}/archive/${{ repo_name }}${{ major_version }}_${{ version }}.tar.gz\n    sha256: 9841854013d61114f7a8d20b918df651404bf5a28d52dd0b4fc4caba5dc8aa08\n\nbuild:\n  number: 0\n\noutputs:\n  - package:\n      name: ${{ cxx_name }}\n    build:\n      script:\n        - if: unix\n          then: build_cxx.sh\n        - if: win\n          then: bld_cxx.bat\n    requirements:\n      build:\n        - ${{ compiler('cxx') }}\n        - ${{ compiler('c') }}\n        - ${{ stdlib('c') }}\n        - ninja\n        - cmake\n        - pkg-config\n      host:\n        - libgz-cmake\n        - libabseil\n        - libprotobuf\n        - libgz-math\n        - libgz-common\n        - libgz-transport\n        - libgz-rendering\n        - libgz-msgs\n        - libgz-plugin\n        - libsdformat\n        - if: osx\n          then:\n            - libarchive\n        - ogre 1.10.*\n      run:\n        # This is actually only necessary to make sure that find_package(gz-sensors) works out of the box,\n        # as gz-rendering transitively calls find_package(OpenGL). As gz-sensors is mostly a C++ library we add it\n        # as run dependency, even if the proper solution may be to have dedicated *-devel packages, that itself should depend on libgl-devel (only on Linux)\n        - if: linux\n          then:\n            - libgl-devel\n      run_exports:\n        - ${{ pin_subpackage(cxx_name, upper_bound='x') }}\n\n    tests:\n      - package_contents:\n          include:\n            - gz/${{ component_version }}/gz/${{ component_name }}.hh\n          lib:\n            - gz-${{ component_name }}\n      - script:\n          - cmake-package-check gz-${{ component_name }} --targets gz-${{ component_name }}::gz-${{ component_name }}\n        requirements:\n          run:\n            - cmake-package-check\n            - ${{ compiler('c') }}\n            - ${{ compiler('cxx') }}\n\n  - package:\n      name: ${{ name }}\n    requirements:\n      run:\n        - ${{ pin_subpackage(cxx_name, exact=True) }}\n      run_exports:\n        - ${{ pin_subpackage(cxx_name, upper_bound='x') }}\n    tests:\n      - script:\n          - cmake-package-check gz-${{ component_name }} --targets gz-${{ component_name }}::gz-${{ component_name }}\n        requirements:\n          run:\n            - cmake-package-check\n            - ${{ compiler('c') }}\n            - ${{ compiler('cxx') }}\n\nabout:\n  homepage: https://github.com/gazebosim/${{ repo_name }}\n  license: Apache-2.0\n  license_file: LICENSE\n  summary: Gazebo Sensors provides numerous sensor models designed to generate realistic data from simulation environments.\n\nextra:\n  feedstock-name: ${{ repo_name }}\n  recipe-maintainers:\n    - wolfv\n    - traversaro\n    - Tobias-Fischer\n    - j-rivero\n",
