  "linux_64_meta_yaml": {
    "about": {
      "description": "TMSU is a tool for tagging your files. It provides a simple command-line utility for applying\ntags and a virtual filesystem to give you a tag-based view of your files from any other program.\n",
      "dev_url": "https://github.com/oniony/TMSU",
      "home": "https://tmsu.org/",
      "license": "GPL-3.0-only",
      "license_family": "GPL",
      "license_file": [
        "COPYING.md",
        "license-files/"
      ],
      "summary": "TMSU lets you tags your files and then access them through a nifty virtual filesystem from any other application."
    },
    "build": {
      "number": "1",
      "script": [
        "pushd src/github.com/oniony/TMSU",
        "cp $RECIPE_DIR/go.* .",
        "go build -trimpath -ldflags \"-s -w\" -o $PREFIX/bin/tmsu",
        "cp $SRC_DIR/COPYING.md .",
        "go-licenses save . --save_path=\"$SRC_DIR/license-files/\"",
        "popd",
        "cp -r ./misc/bin/. $PREFIX/bin/",
        "chmod -R u+w $(go env GOPATH) && rm -r $(go env GOPATH)"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "seowalex"
      ]
    },
    "package": {
      "name": "tmsu",
      "version": "0.7.5"
    },
    "requirements": {
      "build": [
        "c_stdlib_stub",
        "c_compiler_stub",
        "go-cgo_compiler_stub",
        "go-licenses"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "0ac7f09336aaedf73623c4f486c05137c024a726c16dd32525463aee9d70b46a",
      "url": "https://github.com/oniony/TMSU/archive/v0.7.5.tar.gz"
    },
    "test": {
      "commands": [
        "tmsu -V",
        "tmsu init",
        "which mount.tmsu",
        "which tmsu-fs-merge",
        "which tmsu-fs-mv",
        "which tmsu-fs-rm"
      ]
    }
  },
