    "package": {
      "name": "pyscf",
      "version": "2.9.0"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub",
        "cxx_compiler_stub",
        "cmake >=3.10"
      ],
      "host": [
        "libblas",
        "libcint",
        "libxc-c >=6,<8",
        "llvm-openmp",
        "make",
        "pip",
        "python",
        "setuptools",
        "xcfun 2.1.1"
      ],
      "run": [
        "h5py >=2.7",
        "numpy >=1.13,!=1.16,!=1.17",
        "python",
        "scipy >=0.19,!=1.5.0,!=1.5",
        "setuptools"
      ],
      "run_constrained": [
        "pyberny >=0.6.2",
        "geometric >=0.9.7.2"
