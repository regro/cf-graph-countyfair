  "linux_64_meta_yaml": {
    "about": {
      "description": "MDTraj is a python library that allows users to manipulate molecular dynamics\n(MD) trajectories and perform a variety of analyses, including fast RMSD,\nsolvent accessible surface area, hydrogen bonding, etc. A highlight of MDTraj\nis the wide variety of molecular dynamics trajectory file formats which are\nsupported, including RCSB pdb, GROMACS xtc, tng, and trr, CHARMM / NAMD dcd, AMBER\nbinpos, AMBER NetCDF, AMBER mdcrd, TINKER arc and MDTraj HDF5.\n",
      "dev_url": "https://github.com/mdtraj/mdtraj",
      "doc_url": "http://mdtraj.org/",
      "home": "https://github.com/mdtraj/mdtraj",
      "license": "LGPL-2.1-or-later",
      "license_file": "LICENSE",
      "summary": "A modern, open library for the analysis of molecular dynamics trajectories"
    },
    "build": {
      "entry_points": [
        "mdconvert = mdtraj.scripts.mdconvert:entry_point",
        "mdinspect = mdtraj.scripts.mdinspect:entry_point"
      ],
      "number": "1",
      "script": "PYTHON -m pip install . --no-deps -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "marscher",
        "mattwthompson",
        "sroet",
        "jeremyleung521"
      ]
    },
    "package": {
      "name": "mdtraj",
      "version": "1.11.0"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "cxx_compiler_stub",
        "c_stdlib_stub",
        "versioneer"
      ],
      "host": [
        "python",
        "pip",
        "setuptools",
        "versioneer",
        "cython =3",
        "numpy",
        "zlib"
      ],
      "run": [
        "python",
        "setuptools",
        "packaging",
        "numpy >=1.25.0",
        "scipy",
