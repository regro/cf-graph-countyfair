  "linux_64_meta_yaml": {
    "about": {
      "description": "R5py is a Python wrapper for the R5 routing analysis engine. It’s inspired\nby r5r, a wrapper for R, and is designed to interact with GeoPandas data\nframes.  Similar to r5r, r5py represents a simple way to run R5 locally. It\nallows users to generate detailed routing analyses or calculate travel time\nmatrices using parallel computing, and integrates seamlessly with\nPython/Geopandas workflows.\n",
      "dev_url": "https://github.com/r5py/r5py",
      "doc_url": "https://r5py.readthedocs.io/",
      "home": "https://github.com/r5py/r5py",
      "license": "GPL-3.0-or-later OR MIT",
      "license_file": [
        "LICENSE",
        "licenses/"
      ],
      "summary": "Python wrapper for the R5 routing analysis engine"
    },
    "build": {
      "noarch": "python",
      "number": "0",
      "script": "python -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "HTenkanen",
        "christophfink"
      ]
    },
    "package": {
      "name": "r5py",
      "version": "1.0.6"
    },
    "requirements": {
      "host": [
        "python 3.10",
        "pip",
        "setuptools"
      ],
      "run": [
        "python >=3.10",
        "openjdk >=21",
        "ConfigArgParse",
        "filelock",
        "geohexgrid",
        "geopandas <1.1.0",
        "joblib",
        "jpype1",
        "numpy",
        "pandas >=2.1.0",
        "psutil",
        "pyproj",
        "rasterio",
        "requests",
        "scikit-learn",
        "shapely >=2.0",
        "simplification",
        "typing-extensions"
      ]
    },
    "schema_version": 0,
    "source": {
      "patches": [
        "precision-of-comparison.patch"
      ],
      "sha256": "214827c13fa5f7fcb4f0cd76a6d102d18067306438312ae3d9aefdd1f3c67b41",
      "url": "https://pypi.org/packages/source/r/r5py/r5py-1.0.6.tar.gz"
    },
