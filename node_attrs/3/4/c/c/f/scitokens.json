    "about": {
      "description": "This library aims to be a reference implementation of the\nSciTokens\\' JSON Web Token (JWT) token format.\n\nSciTokens is built on top of the PyJWT and cryptography libraries.\nWe aim to provide a safe, high-level interface for token manipulation,\navoiding common pitfalls of using the underling libraries directly.\n",
      "dev_url": "https://github.com/scitokens/scitokens.git",
      "doc_url": "https://scitokens.readthedocs.io",
      "home": "https://scitokens.org",
      "license": "Apache-2.0",
      "license_file": "LICENSE",
      "summary": "SciToken reference implementation library"
    },
    "build": {
      "entry_points": [
        "scitokens-admin-create-key = scitokens.tools.admin_create_key:main",
        "scitokens-admin-create-token = scitokens.tools.admin_create_token:main",
        "scitokens-verify-token = scitokens.tools.verify_token:main"
      ],
      "noarch": "python",
      "number": "0",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "duncanmmacleod"
      ]
    },
    "package": {
      "name": "scitokens",
      "version": "1.8.1"
    },
    "requirements": {
      "host": [
        "pip",
        "python >=3.5",
        "setuptools"
      ],
      "run": [
        "cryptography",
        "pyjwt >=1.6.1",
        "python >=3.5",
        "requests"
      ]
    },
    "schema_version": 0,
    "source": {
