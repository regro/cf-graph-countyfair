    "about": {
      "description": "MediaMTX is a ready-to-use and zero-dependency real-time media server and\nmedia proxy that allows to publish, read, proxy, record and playback video and audio streams.\nIt has been conceived as a \"media router\" that routes media streams from one end to the other.",
      "dev_url": "https://github.com/bluenviron/mediamtx",
      "doc_url": "https://github.com/bluenviron/mediamtx/blob/main/README.md",
      "home": "https://github.com/bluenviron/mediamtx",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "src/LICENSE",
      "summary": "Ready-to-use and zero-dependency real-time media server and media proxy"
    },
    "build": {
      "number": "0",
      "script": [
        "cd src",
        "echo \"v${{ version }}\" > VERSION",
        "go generate ./...",
        "go-licenses save . --save_path ../library_licenses --ignore golang.org/x/sys --ignore golang.org/x/crypto  --ignore github.com/ugorji/go --ignore github.com/benburkert/openpgp",
        "go build -v -o $PREFIX/bin/mediamtx -ldflags=\"-s -w\"",
        "mkdir -p $PREFIX/config",
        "cp ../service-config/service.yaml $PREFIX/config/service.yaml",
        "cp -r ${RECIPE_DIR}/library_licenses/* ${SRC_DIR}/library_licenses"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "phreed"
      ]
    },
