  "linux_64_meta_yaml": {
    "about": {
      "description": "climlab-rrtmg makes the RRTMG_LW and RRTMG_SW radiation codes callable\nfrom Python by wrapping using f2py. This is a lightweight wrapper that\nemulates the Fortran interface as closely as possible. The primary use case\nis to drive the RRTMG radiation processes in climlab, but it can be used as\na standalone driver if you are familiar with the RRTMG Fortran interface.\n",
      "dev_url": "https://github.com/climlab/climlab-rrtmg",
      "home": "https://github.com/climlab/climlab-rrtmg",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "Standalone Python wrapper for the RRTMG radiation modules"
    },
    "build": {
      "number": "2"
    },
    "extra": {
      "recipe-maintainers": [
        "brian-rose"
      ]
    },
    "package": {
      "name": "climlab-rrtmg",
      "version": "0.4.1"
    },
    "requirements": {
      "build": [
        "ninja",
        "pkg-config",
        "c_stdlib_stub",
        "fortran_compiler_stub",
        "c_compiler_stub"
      ],
