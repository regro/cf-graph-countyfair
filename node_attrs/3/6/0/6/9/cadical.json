    "about": {
      "description": "CaDiCaL is a simplified SAT solver written in C++. It is designed to be \nclean, efficient, and easy to use. The solver is highly competitive in \ninternational SAT competitions and provides both a library and a standalone \nbinary for solving SAT problems.",
      "home": "https://fmv.jku.at/cadical/",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "Clean and efficient state-of-the-art SAT solver"
    },
    "build": {
      "number": "0",
      "script": "cp $RECIPE_DIR/CMakeLists.txt CMakeLists.txt\ncmake -GNinja $CMAKE_ARGS -B build -S .\ncmake --build build --config Release\ncmake --install build --prefix $PREFIX\n"
    },
    "extra": {
      "recipe-maintainers": [
        "wolfv"
      ]
    },
    "outputs": [
      {
        "build": null,
        "name": "cadical",
        "requirements": {
          "build": [
            "cxx_compiler_stub",
            "c_stdlib_stub",
            "cmake",
            "ninja"
          ],
          "host": [],
          "run": []
        },
        "tests": [
          {
            "package_contents": {
              "bin": [
                "cadical",
                "mobical"
              ],
              "include": [
                "cadical.hpp",
                "ccadical.h",
                "ipasir.h"
              ],
              "lib": [
                "cadical"
              ]
            }
          },
          {
            "script": "cadical --version"
          },
          {
            "files": {
              "recipe": [
                "simple.cnf"
              ]
            },
            "script": [
              "cadical simple.cnf > output.txt || EXIT_CODE=$?",
              "test $EXIT_CODE -eq 20",
              "cat output.txt",
              "grep \"s UNSATISFIABLE\" output.txt"
            ]
          },
          {
            "files": {
              "recipe": [
                "test.cpp"
              ]
            },
            "requirements": {
              "run": [
                "cxx_compiler_stub"
              ]
            },
            "script": [
              "$CXX test.cpp $CXXFLAGS $LDFLAGS -I$PREFIX/include -lcadical -o test -std=c++11",
              "./test"
            ]
          }
        ],
        "version": "2.1.3"
      }
    ],
