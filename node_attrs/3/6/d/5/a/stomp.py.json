  "linux_64_meta_yaml": {
    "about": {
      "description": "\"stomp.py\" is a Python client library for accessing messaging servers\n(such as ActiveMQ, Artemis or RabbitMQ) using the STOMP protocol\n(STOMP v1.0, STOMP v1.1 and STOMP v1.2).\nIt can also be run as a standalone, command-line client for testing.\n",
      "dev_url": "https://github.com/jasonrbriggs/stomp.py",
      "doc_url": "https://jasonrbriggs.github.io/stomp.py/",
      "home": "https://github.com/jasonrbriggs/stomp.py",
      "license": "Apache-2.0",
      "license_family": "APACHE",
      "license_file": "LICENSE",
      "summary": "Python STOMP client, supporting versions 1.0, 1.1 and 1.2 of the protocol"
    },
    "build": {
      "entry_points": [
        "stomp = stomp.__main__:main"
      ],
      "noarch": "python",
      "number": "0",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "anthchirp",
        "deepsworld"
      ]
    },
    "package": {
      "name": "stomp.py",
      "version": "8.2.0"
    },
    "requirements": {
      "host": [
        "pip",
        "poetry",
        "python >=3.6,<4.0"
      ],
      "run": [
        "docopt >=0.6.2,<0.7.0",
        "python >=3.6,<4.0",
        "websocket-client"
      ]
    },
    "schema_version": 0,
    "source": {
