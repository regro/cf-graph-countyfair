    "about": {
      "description": "Viskores is a toolkit of scientific visualization algorithms for emerging\nprocessor architectures. Viskores supports the fine-grained concurrency for\ndata analysis and visualization algorithms required to drive extreme scale\ncomputing by providing abstract models for data and execution that can be\napplied to a variety of algorithms across many different processor\narchitectures.",
      "dev_url": "https://github.com/Viskores/viskores",
      "doc_url": "https://viskores.readthedocs.io/",
      "home": "https://github.com/Viskores/viskores",
      "license": "BSD-3-Clause",
      "license_family": "BSD-3-Clause",
      "license_file": "LICENSE.txt",
      "summary": "Visualization ToolKit for Many-cores (viskores)"
    },
    "build": {
      "number": "1",
      "run_exports": {
        "weak": [
          "viskores"
        ]
      },
      "script": [
        "cmake -GNinja -S $SRC_DIR -B build -DCMAKE_INSTALL_PREFIX=$PREFIX -DViskores_ENABLE_OPENMP=ON ${{ common_flags }} ${{ cuda_flags }}",
        "cmake --build build ${{ parallel_build_flag }}",
        "cmake --install build",
        "Viskores_DIR=$PREFIX cmake -GNinja -S $SRC_DIR/examples/smoke_test/ -B smoke_test_build",
        "cmake --build smoke_test_build",
        "./smoke_test_build/smoke_test"
      ]
    },
