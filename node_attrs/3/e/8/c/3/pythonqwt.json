      "home": "https://github.com/PlotPyStack/PythonQwt",
      "license": "LGPL-2.1-only",
      "license_family": "LGPL-2.1-only",
      "license_file": "LICENSE",
      "summary": "PythonQwt: Qt plotting widgets for Python"
    },
    "build": {
      "noarch": "python",
      "number": "0",
      "script": "${{ PYTHON }} -m pip install . --no-deps -vv"
    },
