  "raw_meta_yaml": "schema_version: 1\n\ncontext:\n  component_name: physics\n  repo_name: ${{ \"gz-\" + component_name }}\n  version: 9.0.0\n  major_version: \"9\"\n  name: ${{ repo_name }}\n  component_version: ${{ component_name + major_version }}\n  cxx_name: ${{ \"lib\" + name }}\n\nrecipe:\n  name: ${{ name }}\n  version: ${{ version }}\n\nsource:\n  - url: https://github.com/gazebosim/${{ repo_name }}/archive/${{ repo_name }}${{ major_version }}_${{ version }}.tar.gz\n    sha256: d2e6e8caefec88d2c6e7cf60dc404a2761a11b36299cbb9f6e72571f78495b0e\n    patches:\n      - disable_fake_install.patch\n      - if: win\n        then:\n          - disable_integration_tests.patch\n          - workaround_win_base_test.patch\n      - if: arm64\n        then:\n          - disable_common_test.patch\n\nbuild:\n  number: 0\n\noutputs:\n  - package:\n      name: ${{ cxx_name }}\n    build:\n      script:\n        - if: unix\n          then: build_cxx.sh\n        - if: win\n          then: bld_cxx.bat\n    requirements:\n      build:\n        - ${{ compiler('cxx') }}\n        - ${{ compiler('c') }}\n        - ${{ stdlib('c') }}\n        - ninja\n        - cmake\n        - pkg-config\n      host:\n        - libgz-cmake\n        - libgz-plugin\n        - libgz-math\n        - libgz-common\n        - libgz-utils\n        - libsdformat\n        - eigen\n        - assimp\n        - dartsim-cpp\n        - libode\n        - bullet-cpp\n      run_exports:\n        - ${{ pin_subpackage(cxx_name, upper_bound='x') }}\n    tests:\n      - package_contents:\n          include:\n            - gz/${{ component_version }}/gz/${{ component_name }}.hh\n          lib:\n            - gz-${{ component_name }}\n      - script:\n          - cmake-package-check gz-${{ component_name }} --targets gz-${{ component_name }}::gz-${{ component_name }}\n        requirements:\n          run:\n            - cmake-package-check\n            - ${{ compiler('c') }}\n            - ${{ compiler('cxx') }}\n\n  - package:\n      name: ${{ name }}\n    requirements:\n      run:\n        - ${{ pin_subpackage(cxx_name, exact=True) }}\n      run_exports:\n        - ${{ pin_subpackage(cxx_name, upper_bound='x') }}\n    tests:\n      - script:\n          - cmake-package-check gz-${{ component_name }} --targets gz-${{ component_name }}::gz-${{ component_name }}\n        requirements:\n          run:\n            - cmake-package-check\n            - ${{ compiler('c') }}\n            - ${{ compiler('cxx') }}\n\nabout:\n  homepage: https://github.com/gazebosim/${{ repo_name }}\n  license: Apache-2.0\n  license_file: LICENSE\n  summary: Abstract physics interface designed to support simulation and rapid development of robot applications.\n\nextra:\n  feedstock-name: ${{ repo_name }}\n  recipe-maintainers:\n    - Tobias-Fischer\n    - traversaro\n",
