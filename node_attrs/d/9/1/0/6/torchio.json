      "dev_url": "https://github.com/TorchIO-project/torchio",
      "doc_url": "https://torchio.readthedocs.io/",
      "home": "https://torchio.readthedocs.io/",
      "license": "Apache-2.0",
      "license_family": "Apache-2.0",
      "license_file": "LICENSE",
      "summary": "Tools for medical image processing with PyTorch"
    },
    "build": {
      "noarch": "python",
      "number": "0",
      "script": "python -m pip install . -vv"
