  "linux_64_meta_yaml": {
    "about": {
      "description": "ABI Dumper — a tool to dump ABI of an ELF object containing DWARF debug info.\nThe tool is intended to be used with ABI Compliance Checker tool for tracking ABI changes of a C/C++ library or kernel module: https://github.com/lvc/abi-compliance-checker\nThe tool is developed by Andrey Ponomarenko.\n",
      "dev_url": "https://github.com/lvc/abi-dumper",
      "home": "https://github.com/lvc/abi-dumper",
      "license": "LGPL-2.1-or-later",
      "license_file": "LICENSE",
      "summary": "Dump ABI of an ELF object containing DWARF debug info"
    },
    "build": {
      "number": "1",
      "script": [
        "sed -i \"s|#!/usr/bin/perl|#!$PREFIX/bin/perl|\" abi-dumper.pl",
        "make install prefix=$PREFIX",
        "mv $RECIPE_DIR/activate.sh $PREFIX/bin/abi-dumper-set-flags.sh"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "carterbox"
      ]
    },
    "package": {
      "name": "abi-dumper",
      "version": "1.4"
    },
    "requirements": {
      "build": [
        "make"
      ],
      "host": [
        "perl >=5.8"
      ],
      "run": [
        "cxx_compiler_stub",
        "binutils",
        "elfutils",
        "perl >=5.8",
        "universal-ctags",
        "vtable-dumper >=1.1"
      ]
    },
    "schema_version": 0,
