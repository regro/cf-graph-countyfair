      "license": "GPL-3.0-only",
      "license_file": "LICENSE",
      "summary": "CLIMADA: Probabilistic natural catastrophe impact model in Python"
    },
    "build": {
      "noarch": "python",
      "number": "1",
      "script": "PYTHON -m pip install . -vv --no-deps --no-build-isolation"
