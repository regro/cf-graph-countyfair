  "linux_64_meta_yaml": {
    "about": {
      "description": "nputop is an interactive process viewer for Ascend-NPU, providing real-time monitoring of NPU metrics and processes.\nNote: Currently supports Linux only. Requires manual installation of Ascend-NPU drivers (see project documentation).\n",
      "dev_url": "https://github.com/youyve/nputop",
      "doc_url": "https://github.com/youyve/nputop",
      "home": "https://github.com/youyve/nputop",
      "license": "Apache-2.0",
      "license_file": "LICENSE",
      "summary": "nputop: Interactive Ascend-NPU Process Viewer"
    },
    "build": {
      "entry_points": [
        "nputop = nputop.cli:main"
      ],
      "number": "2",
      "script": "PYTHON -m pip install . -vv --no-build-isolation --no-deps"
    },
    "extra": {
      "recipe-maintainers": [
        "youyve"
      ]
    },
    "package": {
      "name": "nputop",
      "version": "0.0.4"
    },
    "requirements": {
      "build": [
        "python",
        "setuptools",
        "pip"
      ],
      "run": [
        "python",
        "psutil >=5.6.6",
        "cachetools >=1.0.1",
        "termcolor >=1.0.0"
      ]
    },
    "schema_version": 0,
