    },
    "test": {
      "commands": [
        "pip check",
        "coverage run --source=ddt --branch -m pytest test -vv --tb=long --color=yes -ra",
        "coverage report --show-missing --skip-covered --fail-under=97"
      ],
      "requires": [
        "aiounittest >=1.4.0",
        "pip",
        "pytest-cov",
        "pyyaml",
        "six >=1.4.0",
        "python 3.9"
      ],
      "source_files": [
