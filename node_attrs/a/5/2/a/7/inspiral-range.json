  "linux_64_meta_yaml": {
    "about": {
      "description": "The inspiral_range package provides tools for calculating various binary\ninspiral range measures useful as figures of merit for gravitational wave\ndetectors characterised by a strain noise spectral density.\n\nIt includes a command-line tool for calculating various inspiral ranges\nfrom a supplied file of detector noise spectral density (either ASD or\nPSD).\n",
      "dev_url": "https://git.ligo.org/gwinc/inspiral_range",
      "home": "https://git.ligo.org/gwinc/inspiral_range",
      "license": "GPL-3.0-or-later",
      "license_family": "GPL",
      "license_file": [
        "COPYING",
        "COPYING-GPL-3"
      ],
      "summary": "GW detector inspiral range calculation tools"
    },
    "build": {
      "entry_points": [
        "inspiral-range = inspiral_range.__main__:main"
      ],
      "noarch": "python",
      "number": "0",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "duncanmmacleod",
        "jrollins"
      ]
    },
    "package": {
      "name": "inspiral-range",
      "version": "0.9.3"
    },
    "requirements": {
      "host": [
        "pip",
        "python 3.9",
        "setuptools",
        "setuptools-scm"
      ],
      "run": [
        "__unix",
        "gpstime",
        "numpy",
        "python >=3.9",
        "python-lal",
        "python-lalsimulation",
        "pyyaml",
        "scipy >=1.8.0"
      ]
    },
    "schema_version": 0,
    "source": {
      "patches": [
        "no-astropy.patch"
      ],
      "sha256": "5b0ec4789ce227e8a73a2fee9470a78a4e0800836cb3dc46ae2d76f445555c88",
      "url": "https://pypi.org/packages/source/i/inspiral_range/inspiral_range-0.9.3.tar.gz"
    },
    "test": {
      "commands": [
        "python -m pip check inspiral-range",
        "python -m pip show inspiral-range",
        "python -m inspiral_range.test -dt",
        "inspiral-range --asd inspiral_range/test/O2.txt",
        "inspiral-range --asd inspiral_range/test/O3.txt m1=30 m2=30"
      ],
      "requires": [
        "pip",
        "python 3.9"
      ],
      "source_files": [
        "inspiral_range/test"
      ]
    }
  },
