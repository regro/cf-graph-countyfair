    "about": {
      "description": "PySDL2 is a wrapper around the SDL2 library and as such similar to the\ndiscontinued PySDL project. In contrast to PySDL, it has no licensing\nrestrictions, nor does it rely on C code, but uses ctypes instead.\n",
      "dev_url": "https://bitbucket.org/marcusva/py-sdl2",
      "doc_url": "https://pysdl2.readthedocs.io",
      "home": "https://github.com/marcusva/py-sdl2",
      "license": "CC0-1.0",
      "license_file": "doc/copying.rst",
      "summary": "A wrapper around the SDL2 library"
    },
    "build": {
      "number": "0",
      "script": "PYTHON -m pip install . --no-deps -vv",
      "script_env": [
        "SDL_VIDEODRIVER=dummy",
        "SDL_AUDIODRIVER=dummy",
        "SDL_RENDER_DRIVER=software"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "dschreij",
        "a-hurst",
        "Tobias-Fischer"
      ]
    },
    "package": {
      "name": "pysdl2",
      "version": "0.9.17"
    },
    "requirements": {
