  "linux_64_meta_yaml": {
    "about": {
      "description": "This python package implements k-medoids clustering with PAM and variants of clustering by direct optimization of the (Medoid) Silhouette, including the DynMSC variant that automatically chooses the number of clusters (based on the optimum medoid silhouette). It can be used with arbitrary dissimilarites, as it only requires a dissimilarity matrix as input. A sklearn compatible API is included.",
      "dev_url": "https://github.com/kno10/python-kmedoids",
      "doc_url": "https://python-kmedoids.readthedocs.io/",
      "home": "https://github.com/kno10/python-kmedoids",
      "license": "GPL-3.0-or-later",
      "license_family": "GPL",
      "license_file": [
        "THIRDPARTY.yml",
        "LICENSE"
      ],
      "summary": "k-Medoids Clustering in Python with FasterPAM"
    },
    "build": {
      "number": "0",
      "script": [
        "export CARGO_NET_GIT_FETCH_WITH_CLI=true",
        "export PYO3_USE_ABI3_FORWARD_COMPATIBILITY=1",
        "cargo-bundle-licenses --format yaml --output THIRDPARTY.yml",
        "PYTHON -m pip install . --no-deps --ignore-installed -vv"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "kno10",
        "larslenssen"
      ]
    },
