      "doc_url": "https://imas-python.readthedocs.io/",
      "home": "https://imas-python.readthedocs.io/",
      "license": "LGPL-3.0-only",
      "license_family": "LGPL-3.0-only",
      "license_file": "LICENSE.txt",
      "summary": "Python data access library for IMAS Data Model"
    },
    "build": {
      "noarch": "python",
      "number": "0",
      "script": {
        "content": "${{ PYTHON }} -m pip install . -vv --no-deps --no-build-isolation --ignore-installed",
        "env": {
          "SETUPTOOLS_SCM_PRETEND_VERSION": "2.0.1"
        }
      }
    },
    "extra": {
      "recipe-maintainers": [
        "munechika-koyo",
        "SimonPinches"
      ]
    },
    "outputs": [
