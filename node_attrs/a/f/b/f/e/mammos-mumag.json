    "about": {
      "description": "`mammos-mumag` is a finite-element micromagnetic simulation tool capable of simulating hysteresis loops of magnetic materials with multiple grains.\n",
      "dev_url": "https://github.com/mammos-project/mammos-mumag",
      "doc_url": "https://mammos-project.github.io/mammos",
      "home": "https://mammos-project.github.io",
      "license": "MIT",
      "license_file": "LICENSE",
      "summary": "Finite element micromagnetic simulation tool."
    },
    "build": {
      "entry_points": [
        "unv2fly = mammos_mumag.cli:convert_mesh"
      ],
      "noarch": "python",
      "number": "1",
      "script": "PYTHON -m pip install . -vv --no-deps --no-build-isolation"
    },
    "extra": {
      "recipe-maintainers": [
        "andrea-petrocchi",
        "lang-m",
        "fangohr"
      ]
    },
    "package": {
      "name": "mammos-mumag",
      "version": "0.10.1"
    },
    "requirements": {
      "host": [
        "python 3.11",
        "hatchling",
        "pip"
      ],
      "run": [
        "python >=3.11",
        "jinja2 >=3.1.6",
        "mammos-entity >=0.10.0",
