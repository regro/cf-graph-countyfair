    "about": {
      "description": "QuackOSM is an OpenStreetMap data downloader and reader for Python 3.9+.\nIt can work with bigger-than-memory PBF files and transform them into\nGeoParquet files with proper geometries. It is built on top of DuckDB's\nspatial extension with a dedicated CLI.\n",
      "dev_url": "https://github.com/kraina-ai/quackosm",
      "doc_url": "https://kraina-ai.github.io/quackosm/",
      "home": "https://github.com/kraina-ai/quackosm",
      "license": "Apache-2.0",
      "license_file": "LICENSE",
      "summary": "An open-source tool for reading OpenStreetMap PBF files using DuckDB"
    },
    "build": {
      "entry_points": [
        "quackosm = quackosm.__main__:main",
        "QuackOSM = quackosm.__main__:main"
      ],
      "noarch": "python",
      "number": "0",
      "script": "PYTHON -m pip install . -vv --no-deps --no-build-isolation"
    },
    "extra": {
      "recipe-maintainers": [
        "RaczeQ"
      ]
    },
    "package": {
      "name": "quackosm",
      "version": "0.15.3"
    },
    "requirements": {
      "host": [
        "python 3.9",
        "pdm-backend",
        "pip"
      ],
      "run": [
        "python >=3.9",
        "geopandas >=0.6",
        "shapely >=2",
        "pyarrow >=16.0.0",
        "python-duckdb >=1.1.2",
        "geoarrow-pyarrow >=0.1.2",
        "typeguard >=3.0.0",
        "psutil >=5.6.2",
        "pooch >=1.6.0",
        "tqdm >=4.42.0",
