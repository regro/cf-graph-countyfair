      "license_family": "Apache",
      "license_file": "LICENSE",
      "summary": "A command-line application for tracking, reporting on complexity of Python tests and applications."
    },
    "build": {
      "noarch": "python",
      "number": "1",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
