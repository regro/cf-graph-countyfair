  "linux_64_meta_yaml": {
    "about": {
      "description": "CSR is a compressed sparse row sparse matrix package for Python,\nwith Numba-accessible APIs and optional MKL acceleration.\n",
      "dev_url": "https://github.com/lenskit/csr",
      "doc_url": "https://csr.lenskit.org",
      "home": "https://csr.lenskit.org",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "Compressed sparse matrices for Python and Numba"
    },
    "build": {
      "ignore_run_exports": [
        "mkl"
      ],
      "number": "0",
      "script": "\ninvoke build-mkl\nPYTHON -m pip install --no-deps -vv .\n"
    },
    "extra": {
      "recipe-maintainers": [
        "mdekstrand"
      ]
    },
    "package": {
      "name": "csr",
      "version": "0.5.2"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub"
      ],
      "host": [
        "python",
        "pip",
        "flit-core >=3.2",
        "invoke =2",
        "cffi >=1.12.2",
        "mkl-devel"
      ],
      "run": [
        "python",
        "numba >=0.51",
        "numpy >=1.21",
        "scipy >=1.4,<2",
        "cffi >=1.12.2"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "1c31260d70d1072a394737a4a754c26cac1dc946f0a965e3c33877c153215031",
      "url": "https://pypi.io/packages/source/c/csr/csr-0.5.2.tar.gz"
    },
    "test": {
      "commands": [
        "python -m pytest tests --hypothesis-profile=nojit"
      ],
      "imports": [
        "csr",
        "csr.kernel",
        "csr.kernels.mkl"
      ],
      "requires": [
        "pytest",
        "hypothesis",
        "pytest-benchmark",
        "psutil",
        "libblas=*=*mkl"
      ],
      "source_files": [
        "pytest.ini",
        "conftest.py",
        "tests",
        "tests/*.py"
      ]
    }
  },
