  "feedstock_name": "distopia",
  "linux_64_meta_yaml": {
    "about": {
      "description": "Distopia is a package to rapidly calculate distances under periodic boundary conditions.\nExplicit SIMD vectorisation allows awesome speedups over autovectorised code.\nThe distopia package consists of consists of the python layer (distopia)\nand a C++ library (libdistopia) that does the heavy lifting.\n",
      "dev_url": "https://github.com/MDAnalysis/distopia",
      "doc_url": "https://www.mdanalysis.org/distopia/",
      "home": "https://github.com/MDAnalysis/distopia",
      "license": "MIT",
      "license_file": [
        "LICENSE.txt"
      ],
      "summary": "Fast distance calculations using explicitly vectorised SIMD"
    },
    "build": {
      "number": "0",
      "script": "PYTHON -m pip install . -vv",
      "script_env": [
        "SKBUILD_CONFIGURE_OPTIONS=-DDISTOPIA_BUILD_TESTS=OFF -DDISTOPIA_USE_EXTERNAL_HWY=ON"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "IAlibay",
        "hmacdope",
        "richardjgowers",
        "orbeckst"
      ]
    },
    "package": {
      "name": "distopia",
      "version": "0.4.1"
    },
    "requirements": {
      "build": [
        "cxx_compiler_stub",
        "c_compiler_stub",
        "c_stdlib_stub",
        "cmake",
        "make",
        "pip",
        "python",
        "versioningit",
        "cython",
        "numpy"
      ],
      "host": [
        "python",
        "pip",
        "numpy",
        "scikit-build >=0.17",
        "cython",
        "versioningit",
        "make",
        "ninja",
        "libhwy",
        "setuptools"
      ],
      "run": [
        "python",
        "numpy",
        "libhwy"
      ]
    },
    "schema_version": 0,
    "source": {
      "git_depth": "-1",
      "git_rev": "0.4.1",
      "git_url": "https://github.com/MDAnalysis/distopia.git"
    },
    "test": {
      "imports": [
        "distopia"
      ]
    }
  },
