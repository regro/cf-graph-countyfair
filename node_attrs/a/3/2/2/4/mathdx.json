      "license_url": "https://docs.nvidia.com/cuda/cufftdx/license.html",
      "summary": "NVIDIA MathDx device libraries"
    },
    "build": {
      "noarch": "generic",
      "number": "0",
      "script": [
        "cp -vr $SRC_DIR/nvidia/mathdx/25.06/include/* $PREFIX/include/",
        "cp -vr $SRC_DIR/nvidia/mathdx/25.06/lib/* $PREFIX/lib/",
        "mv -v $SRC_DIR/nvidia/mathdx/25.06/src $PREFIX/",
        "cp $SRC_DIR/nvidia/mathdx/25.06/LICENSE.txt $SRC_DIR/",
        "sed -i -E 's|get_filename_component\\(PACKAGE_PREFIX_DIR.*|set\\(PACKAGE_PREFIX_DIR \"/opt/anaconda1anaconda2anaconda3\"\\)|g' $PREFIX/lib/cmake/*/*dx-config.cmake",
        "sed -i -E 's|\\$\\{PACKAGE_PREFIX_DIR\\}/external/cutlass|/opt/anaconda1anaconda2anaconda3|g' $PREFIX/lib/cmake/*/*dx-config.cmake",
        "check-glibc \"$PREFIX\"/lib*/*.so.* \"$PREFIX\"/bin/* \"$PREFIX\"/targets/*/lib*/*.so.* \"$PREFIX\"/targets/*/bin/*"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "conda-forge/cuda",
        "leofang"
      ]
    },
    "package": {
      "name": "mathdx",
      "version": "25.06.0"
    },
    "requirements": {
      "build": [
        "cf-nvidia-tools 1"
      ],
      "host": [
        "cutlass 3.9.0.*"
      ],
      "run": [
        "__unix",
        "cutlass"
      ],
      "run_constrained": [
        "cuda-version >=11.0.*,<13.a0"
      ]
    },
