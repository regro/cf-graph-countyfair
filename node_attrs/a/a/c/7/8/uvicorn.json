  "raw_meta_yaml": "# yaml-language-server: $schema=https://raw.githubusercontent.com/prefix-dev/recipe-format/main/schema.json\nschema_version: 1\n\ncontext:\n  version: \"0.40.0\"\n  python_check_max: \"3.14\"\n\nrecipe:\n  name: uvicorn\n  version: ${{ version }}\n\nsource:\n  url: https://pypi.org/packages/source/u/uvicorn/uvicorn-${{ version }}.tar.gz\n  sha256: 839676675e87e73694518b5574fd0f24c9d97b46bea16df7b8c05ea1a51071ea\n\nbuild:\n  number: 0\n  noarch: python\n\noutputs:\n  - package:\n      name: uvicorn\n    build:\n      noarch: python\n      script:\n        - ${{ PYTHON }} -m pip install . -vv --no-deps --ignore-installed --disable-pip-version-check\n      python:\n        entry_points:\n          - uvicorn = uvicorn.main:main\n    requirements:\n      host:\n        - hatchling\n        - pip\n        - python ${{ python_min }}.*\n      run:\n        - __${{ noarch_platform }}\n        - click >=7.0\n        - h11 >=0.8\n        - python >=${{ python_min }}\n        - typing_extensions >=4.0\n    tests:\n      - python:\n          imports: uvicorn\n          pip_check: true\n          python_version:\n            - ${{ python_min }}.*\n            - ${{ python_check_max }}.*\n      - requirements:\n          run:\n            - pip\n            - python ${{ python_min }}.*\n        script:\n          - uvicorn --help\n\n  - package:\n      name: uvicorn-standard\n    build:\n      noarch: generic\n    requirements:\n      run:\n        - __${{ noarch_platform }}\n        - ${{ pin_subpackage(\"uvicorn\", exact=True) }}\n        - websockets >=10.4\n        - httptools >=0.6.3\n        - watchfiles >=0.13\n        - python-dotenv >=0.13\n        - pyyaml >=5.1\n        - if: noarch_platform == \"win\"\n          then:\n            - colorama >=0.4\n          else:\n            - uvloop >=0.14.0,!=0.15.0,!=0.15.1\n    tests:\n      - python:\n          imports: uvicorn.supervisors.watchfilesreload\n          pip_check: true\n          python_version: