    "about": {
      "description": "This is a framework for building checks designed to check to see if a repository follows\nguidelines. By itself, it does nothing - it requires at least one plugin to be installed.\n\nWith one or more plugins, it will produce a list of results - green checkmarks mean this\nrule is followed, red x’s mean the rule is not. A yellow warning sign means that the check\nwas skipped because a previous required check failed. Four output formats are supported;\nrich, svg, html, and json.",
      "doc_url": "https://repo-review.readthedocs.io/en/latest/",
      "home": "https://github.com/scientific-python/repo-review",
      "license": "BSD-3-Clause",
      "license_family": "BSD-3-Clause",
      "license_file": "LICENSE",
      "summary": "This is a framework for building checks designed to check to see if a repository follows guidelines"
    },
    "build": {
      "noarch": "python",
      "number": "0",
      "script": "python -m pip install . -v"
    },
    "extra": {
      "recipe-maintainers": [
        "henryiii",
        "MementoRC"
      ]
    },
    "outputs": [
      {
        "build": null,
        "name": "repo-review",
        "requirements": {
          "build": [],
          "host": [
            "hatchling",
            "hatch-vcs",
            "python 3.10.*",
            "pip"
          ],
          "run": [
            "click",
            "markdown-it-py",
            "python >=3.10",
            "pyyaml",
            "rich",
            "rich-click",
            "tomli",
            "typing-extensions"
          ]
        },
        "tests": [
          {
            "python": {
              "imports": [
                "repo_review",
                "repo_review.fixtures",
                "repo_review.schema"
              ]
            }
          },
          {
            "downstream": "sp-repo-review"
          },
          {
            "files": {
              "source": [
                "docs",
                "tests",
                "README.md",
                "LICENSE",
                "pyproject.toml",
                ".github",
                ".pre-commit-config.yaml",
                ".readthedocs.yaml"
              ]
            },
            "requirements": {
              "run": [
                "pip",
                "pytest",
                "sp-repo-review",
                "validate-pyproject >=0.14",
                "python ==3.10.*"
              ]
            },
            "script": [
              "repo-review --help",
              "validate-pyproject -E repo-review --help",
              "mkdir src",
              "ls",
              "pytest -ra"
            ]
          }
        ],
        "version": "0.12.2"
      }
