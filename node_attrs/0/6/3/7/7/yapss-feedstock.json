    "outputs": [
      {
        "about": {
          "description": "YAPSS is a Python package for numerically solving multi-phase optimal control problems using\npseudospectral methods, similar to the GPOPS-II algorithm. The package supports multiple\ndifferentiation methods, including automatic differentiation using CasADi, as well as numerical\nderivatives found by central difference methods for problems that cannot be differentiated by\nautomatic methods. YAPSS supports multiple collocation methods, including Legendre-Gauss (LG),\nLegendre-Gauss-Radau (LGR), and Legendre-Gauss-Lobatto (LGL) collocation points. The mesh\nstructure can be specified by the user to improve the accuracy of the solution.\n",
          "dev_url": "https://github.com/stevenrhall/yapss",
          "doc_url": "https://yapss.readthedocs.io",
          "home": "https://github.com/stevenrhall/yapss",
          "license": "MIT",
          "license_file": "LICENSE",
          "summary": "A Python package for solving optimal control problems using pseudospectral methods."
        },
        "build": {
          "script": "PYTHON -m pip install . --no-deps -vv"
        },
        "name": "yapss",
        "requirements": {
          "host": [
            "python 3.9",
