  "linux_64_meta_yaml": {
    "about": {
      "description": "Gmsh is a free 3D finite element grid generator with a build-in CAD\nengine and post-processor. Its design goal is to provide a fast, light\nand user-friendly meshing tool with parametric input and advanced\nvisualization capabilities. Gmsh is built around four modules:\ngeometry, mesh, solver and post-processing. The specification of any\ninput to these modules is done either interactively using the graphical\nuser interface or in ASCII text files using Gmsh's own scripting\nlanguage.\n",
      "dev_url": "https://gitlab.onelab.info/gmsh/gmsh/",
      "doc_url": "https://gmsh.info/doc/texinfo/gmsh.html",
      "home": "https://gmsh.info/",
      "license": "GPL-2.0-or-later",
      "license_family": "GPL",
      "license_file": "/LICENSE.txt",
      "summary": "A three-dimensional finite element mesh generator with built-in pre- and post-processing facilities"
    },
    "build": {
      "number": "4"
    },
    "extra": {
      "recipe-maintainers": [
        "oliviertremblaynoel",
        "adrianinsaval",
        "guyer",
        "isuruf",
        "looooo",
        "maresb",
        "matthiasdiener",
        "xywei"
      ]
    },
    "outputs": [
      {
        "name": "gmsh",
        "requirements": {
          "build": [
            "cmake",
            "make",
            "c_compiler_stub",
            "c_stdlib_stub",
            "cxx_compiler_stub",
            "python *"
          ],
          "host": [
            "libblas",
            "liblapack",
            "gmp",
            "occt",
            "fltk",
            "libpng",
            "libjpeg-turbo",
            "libglu",
            "zlib",
            "xorg-libxfixes",
            "xorg-libxext",
            "xorg-libxrender",
            "xorg-libx11",
            "xorg-libxmu",
            "xorg-xorgproto",
            "libgl-devel",
            "cairo",
            "libgomp"
          ],
          "run": [
            "occt"
          ]
        },
        "script": "build_gmsh.sh",
        "test": {
          "commands": [
            "gmsh -help",
            "test \"$(gmsh --version 2>&1)\" == \"4.15.0\""
          ]
        }
      },
      {
        "build": {
          "noarch": "python"
        },
        "name": "python-gmsh",
        "requirements": {
          "host": [
            "python 3.10"
          ],
