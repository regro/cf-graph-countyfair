  "linux_64_meta_yaml": {
    "about": {
      "description": "BoltzTraP2 is a modern implementation of the smoothed Fourier\ninterpolation algorithm for electronic bands that formed the base of\nthe original and widely used BoltzTraP code. One of the most typical\nuses of BoltzTraP is the calculation of thermoelectric transport\ncoefficients as functions of temperature and chemical potential in\nthe rigid-band picture. However, many other features are available,\nincluding 3D plots of Fermi surfaces based on the reconstructed bands.\n",
      "dev_url": "https://gitlab.com/sousaw/BoltzTraP2/",
      "doc_url": "https://gitlab.com/sousaw/BoltzTraP2/",
      "home": "https://www.imc.tuwien.ac.at/forschungsbereich_theoretische_chemie/forschungsgruppen/prof_dr_gkh_madsen_theoretical_materials_chemistry/boltztrap/",
      "license": "GPL-3.0-only",
      "license_family": "GPL",
      "license_file": "LICENSE.txt",
      "summary": "Boltzmann Transport Properties (BoltzTraP) is a program for calculating the semi-classic transport coefficients"
    },
    "build": {
      "entry_points": [
        "btp2 = BoltzTraP2.interface:btp2_main"
      ],
      "number": "1",
      "script": "PYTHON -m pip install . -vv"
