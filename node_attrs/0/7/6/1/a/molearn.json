    "about": {
      "description": "This software trains a generative neural network on an ensemble of molecular conformations (typically\nobtained by molecular dynamics). The trained model can be used to generate new, plausible\nconformations repesentative of poorly sampled transition states.\n",
      "dev_url": "https://github.com/Degiacomi-Lab/molearn",
      "doc_url": "https://github.com/Degiacomi-Lab/molearn",
      "home": "https://github.com/Degiacomi-Lab/molearn",
      "license": "GPL-3.0-only",
      "license_file": "LICENSE",
      "summary": "Molearn contains methods to train neural networks on ensembles of molecular conformations"
    },
    "build": {
      "number": "4",
      "script": "PYTHON -m pip install . -vv"
    },
