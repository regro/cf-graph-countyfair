  "linux_64_meta_yaml": {
    "about": {
      "description": "GDB, the GNU Project debugger, allows you to see what is going on `inside'\nanother program while it executes -- or what another program was doing at\nthe moment it crashed.\nThe program being debugged can be written in Ada, C, C++, Objective-C,\nPascal (and many other languages). Those programs might be executing on the\nsame machine as GDB (native) or on another machine (remote).\nGDB can run on most popular UNIX and Microsoft Windows variants.\n",
      "dev_url": "https://sourceware.org/git/binutils-gdb.git",
      "doc_url": "https://sourceware.org/gdb/current/onlinedocs/gdb/",
      "home": "https://www.gnu.org/software/gdb/",
      "license": "GPL-3.0-only",
      "license_family": "GPL",
      "license_file": "gdb/COPYING",
      "summary": "GDB, the GNU Project debugger, allows you to see what is going on inside another program while it executes -- or what another program was doing at the moment it crashed."
    },
    "build": {
      "number": "2",
      "script_env": [
        "TMPDIR"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "gqmelo",
        "marcelotrevisani",
        "phil-blain",
        "matthiasdiener"
      ]
    },
    "package": {
      "name": "gdb",
      "version": "16.3"
    },
    "requirements": {
      "build": [
        "gnuconfig",
        "fortran_compiler_stub",
        "c_compiler_stub",
        "c_stdlib_stub",
        "cxx_compiler_stub",
        "make >=3.82"
      ],
      "host": [
        "python",
        "ncurses",
        "texinfo",
        "liblzma-devel",
        "zlib",
        "readline",
        "libiconv",
        "gmp",
        "mpfr"
      ],
      "run": [
        "python",
        "ncurses",
        "liblzma-devel",
        "zlib",
        "six",
        "libiconv",
        "pygments"
