  "raw_meta_yaml": "{% set version = \"4.1.2\" %}\n\npackage:\n  name: openfast-split\n  version: {{ version }}\n\nsource:\n  # See https://conda-forge.org/docs/maintainer/adding_pkgs/#source\n  # conda-forge requests tar-ball urls instead of git actions for source\n  # Use `openssl sha256 file.tar.gz` to get sha256 stamp\n  url: https://github.com/openfast/openfast/archive/v{{ version }}.tar.gz\n  sha256: 940166d4a0d3c1349d8451c7080093f00711c2be38cbf28f2452912b46e52c7a\n  #git_url: https://github.com/openfast/openfast.git\n  #git_rev: v{{ version }}\n  #git_depth: 1 # (Defaults to -1/not shallow)\n  patches:\n    - ofpy.patch\n\nbuild:\n  number: 3\n\noutputs:\n\n  - name: openfast\n    version: {{ version }}\n    script: build_of.sh  # [not win]\n    script: bld_of.bat  # [win]\n    build:\n      #run_exports:\n      #    - {{ pin_subpackage('openfast', max_pin='x.x') }}\n      missing_dso_whitelist:\n        - \"*/libopenfastlib.dll\"         # [win]\n    requirements:\n      build:\n        - {{ stdlib(\"c\") }}                       # [not win]\n        - {{ compiler('c') }}                     # [not win]\n        - {{ compiler('cxx') }}                   # [not win]\n        - {{ compiler('fortran') }}               # [not win]\n        - {{ compiler('m2w64_c') }}               # [win]\n        - {{ compiler('m2w64_cxx') }}             # [win]\n        - {{ compiler('m2w64_fortran') }}         # [win]\n        - cmake\n        - ninja\n      host:\n        - libgomp            # [not osx]\n        - llvm-openmp        # [osx]\n        - openblas           # [not win]\n        - libblas            # [win]\n        - liblapack          # [win]\n        #- libcblas\n        #- liblapacke    \n      run:\n        - libgomp            # [not osx]\n        - llvm-openmp        # [osx]\n\n    test:\n      commands:\n        #- aerodisk_driver -h\n        - aerodyn_driver -h\n        #- beamdyn_driver -h\n        - FAST.Farm -h\n        - hydrodyn_driver -h\n        #- inflowwind_driver -h\n        - moordyn_driver -h\n        - openfast -h\n        - seastate_driver -h\n        #- sed_driver -h\n        - servodyn_driver -h\n        #- subdyn_driver -h\n        - turbsim -h\n\n  - name: openfast-io\n    version: {{ version }}\n    script: build_io.sh  # [not win]\n    script: bld_io.bat  # [win]\n    build:\n      #noarch: python\n      skip: true  # [py<311]\n      #script: {{ PYTHON }} -m pip install openfast_io -vv --no-deps --no-build-isolation\n    requirements:\n      build:\n        - cross-python_{{ target_platform }}               # [build_platform != target_platform]\n        - python                                           # [build_platform != target_platform]\n      host:\n        - python       #{{ python_min }}\n        - setuptools\n        - hatchling\n        - hatch-vcs\n        - pip\n        #- {{ pin_subpackage('openfast', exact=True) }}\n      run:\n        - python       #>={{ python_min }}\n        - numpy\n        - pandas\n        - ruamel.yaml\n        - deepdiff\n        #- {{ pin_subpackage('openfast', exact=True) }}\n\n    test:\n      imports:\n        - openfast_io\n      requires:\n        - python #{{ python_min }}\n\n  - name: pyopenfast\n    version: {{ version }}\n    script: build_pyof.sh  # [not win]\n    script: bld_pyof.bat  # [win]\n    build:\n      #noarch: python\n      skip: true  # [py<311]\n      #script: {{ PYTHON }} -m pip install openfast_io -vv --no-deps --no-build-isolation\n    requirements:\n      build:\n        - cross-python_{{ target_platform }}               # [build_platform != target_platform]\n        - python                                           # [build_platform != target_platform]\n      host:\n        - python       #{{ python_min }}\n        - setuptools\n        - pip\n        #- {{ pin_subpackage('openfast', exact=True) }}\n      run:\n        - python       #>={{ python_min }}\n        - numpy\n        #- {{ pin_subpackage('openfast', exact=True) }}\n\n    test:\n      imports:\n        - pyOpenFAST\n      requires:\n        - python #{{ python_min }}\n\nabout:\n  home: https://github.com/openfast/openfast\n  license: Apache-2.0\n  license_file: LICENSE\n  summary: OpenFAST whole turbine simulation tool\n  description: NREL-supported OpenFAST whole-turbine simulation code\n  doc_url: http://openfast.readthedocs.io\n  dev_url: https://github.com/openfast/openfast\n\nextra:\n  feedstock-name: openfast\n  recipe-maintainers:\n    - deslaughter\n    - gbarter\n    - andrew-platt\n",
