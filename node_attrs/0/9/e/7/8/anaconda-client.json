  "raw_meta_yaml": "# yaml-language-server: $schema=https://raw.githubusercontent.com/prefix-dev/recipe-format/main/schema.json\nschema_version: 1\n\ncontext:\n  version: \"1.14.0\"\n\npackage:\n  name: anaconda-client\n  version: ${{ version }}\n\nsource:\n  url: https://github.com/anaconda/anaconda-client/archive/${{ version }}.tar.gz\n  sha256: 2f3f26bff6753d5a55bc21736626080613a34848a1f4072afad5f56775a2bd85\n\nbuild:\n  number: 0\n  noarch: python\n  script:\n    - SETUPTOOLS_SCM_PRETEND_VERSION=${{ version }} ${{ PYTHON }} -m pip install . -vv --no-deps --ignore-installed --disable-pip-version-check\n  python:\n    entry_points:\n      - binstar = binstar_client.scripts.cli:main\n      - conda-server = binstar_client.scripts.cli:main\n\nrequirements:\n  host:\n    - pip\n    - python ${{ python_min }}.*\n    - hatchling\n    - hatch-vcs >=0.3\n    - setuptools-scm >=7.1\n    - hatch-requirements-txt\n  run:\n    - anaconda-cli-base >=0.7.0\n    - anaconda-auth >=0.12.3\n    - conda-package-handling >=1.7.3\n    - conda-package-streaming >=0.9.0\n    - defusedxml >=0.7.1\n    - nbformat >=4.4.0\n    - platformdirs >=3.10.0,<5.0\n    - python >=${{ python_min }}\n    - python-dateutil >=2.6.1\n    - pytz >=2021.3\n    - pyyaml >=3.12\n    - requests >=2.20.0\n    - requests-toolbelt >=0.9.1\n    - setuptools >=58.0.4\n    - tqdm >=4.56.0\n    - urllib3 >=1.26.4\n    # Technically an `[extra]`, but fails to upload on any `icon`\n    # https://github.com/conda-forge/anaconda-client-feedstock/issues/57\n    - pillow >=8.2\n    # anaconda-cli-base requires it and doesn't have it declared\n    # without this, pip check isn't passing\n    # https://github.com/conda-forge/anaconda-cli-base-feedstock/issues/19\n    - packaging\n\ntests:\n  - python:\n      python_version: ${{ python_min }}.*\n      pip_check: true\n      imports:\n        - binstar_client\n        - binstar_client.commands\n        - binstar_client.inspect_package\n        - binstar_client.mixins\n      