      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE.txt",
      "summary": "py2app is a Python setuptools command which will allow you to make\nstandalone Mac OS X application bundles and plugins from Python scripts.\npy2app is similar in purpose and design to py2exe for Windows.\npy2app must be used on OSX to build applications, it cannot create Mac\napplications on other platforms.\n"
    },
    "build": {
      "binary_relocation": false,
      "entry_points": [
        "py2applet = py2app.script_py2applet:main"
      ],
      "number": "1",
      "script": "PYTHON -m pip install . --no-deps -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "bkpoon"
      ]
    },
    "package": {
      "name": "py2app",
      "version": "0.28.8"
    },
