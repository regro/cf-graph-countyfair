  "raw_meta_yaml": "# yaml-language-server: $schema=https://raw.githubusercontent.com/prefix-dev/recipe-format/main/schema.json\nschema_version: 1\n\ncontext:\n  version: \"0.46.0\"\n  python_check_max: \"3.14\"\n\npackage:\n  name: jedi-language-server\n  version: ${{ version }}\n\nsource:\n  - url: https://pypi.org/packages/source/j/jedi-language-server/jedi_language_server-${{ version }}.tar.gz\n    sha256: b9ee7cce6cb2bd6fce11115906db41549d7e97b358e0818a23dca164ed0b961a\n    target_directory: dist\n  - url: https://github.com/pappasam/jedi-language-server/archive/refs/tags/v${{ version }}.tar.gz\n    sha256: 1f67e77881440dd4f4e408a14acb4d8cfda2e0aa5c294aeaa35a9c46634c5981\n    target_directory: src\n\nbuild:\n  number: 0\n  noarch: python\n  script:\n    - cd dist\n    - ${{ PYTHON }} -m pip install . -vv --no-deps --no-build-isolation --disable-pip-version-check\n  python:\n    entry_points:\n      - jedi-language-server = jedi_language_server.cli:cli\n\nrequirements:\n  host:\n    - pip\n    - poetry-core >=2.0,<3\n    - python ${{ python_min }}.*\n  run:\n    - docstring-to-markdown >=0.4,<1.0\n    - cattrs >=23.1.2\n    - jedi >=0.19.2,<0.20.0\n    - lsprotocol >=2023.0.1\n    - pygls >=2.0.0,<3\n    - python >=${{ python_min }}\n\ntests:\n  - python:\n      imports: jedi_language_server\n      python_version:\n        - ${{ python_min }}.*\n        - ${{ python_check_max }}.*\n  - files:\n      source:\n        - src/tests/\n    requirements:\n      run:\n        - pyhamcrest\n        - pytest-cov\n        - python ${{ python_min }}.*\n        - python-lsp-jsonrpc\n    script:\n      - jedi-language-server --help\n      - jedi-language-server --version\n      - if: unix\n        then: jedi-language-server --version | grep -iE \"${{ version | replace('.', '\\\\.') }}\"\n        else: jedi-language-server --version | findstr \"${{ version | replace('.', '\\\\.') }}\"\n      - cd src\n      - coverage run --source=jedi_language_server --branch -m pytest -vv --