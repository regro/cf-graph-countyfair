  "raw_meta_yaml": "{% set name = \"parsl\" %}\n{% set version = \"2025.10.20\" %}\n\npackage:\n  name: {{ name|lower }}-split\n  version: {{ version }}\n\nsource:\n  url: https://pypi.org/packages/source/{{ name[0] }}/{{ name }}/parsl-{{ version }}.tar.gz\n  sha256: b494cc36b44e034c8945ad1347f2560afc5fb7ece1eaa6500c2037ac6ec43991\n\nbuild:\n  number: 0\n  noarch: python\n\nrequirements:\n  host:\n    - python {{ python_min }}\n  run:\n    - python >={{ python_min }}\n\noutputs:\n  - name: {{ name }}\n    script: install_parsl.sh\n    build:\n      noarch: python\n      entry_points:\n        - parsl-globus-auth=parsl.data_provider.globus:cli_run\n        - parsl-visualize=parsl.monitoring.visualization.app:cli_run\n        - parsl-perf=parsl.benchmark.perf:cli_run\n\n    requirements:\n      host:\n        - python {{ python_min }}\n        - pip\n        - setuptools\n      run:\n        - python >={{ python_min }}\n        # requirements.txt\n        - pyzmq >=17.1.2\n        - typeguard >=2.10,!=3.*,<5\n        - typing-extensions\n        - globus-sdk\n        - dill\n        - tblib\n        - requests\n        - sortedcontainers\n        - psutil >=5.5.1\n        - setproctitle\n        - filelock >=3.13,<4\n\n    test:\n      requires:\n        - python {{ python_min }}\n        - pip\n      imports:\n        - parsl\n      commands:\n        - pip check\n\n  - name: {{ name }}-with-monitoring\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python {{ python_min }}\n      run:\n        - python >={{ python_min }}\n        - {{ pin_subpackage(name, max_pin='x.x.x.x.x.x') }}\n        - sqlalchemy >=2,<2.1\n\n    test:\n      imports:\n        - parsl\n        - parsl.monitoring\n      commands:\n        - pip check\n      requires:\n        - python {{ python_min }}\n        - pip\n\n  - name: {{ name }}-with-visualization\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python {{ python_min }}\n      run:\n        - python >={{ python_min }}\n        - {{ pin_subpackage(name, max_pin='x.x.x.x.x.x') }}\n        - {{ name }}-with-monitoring\n        - pydot >=1.4.2\n        - networkx >=3.2,<3.3\n        - flask >=1.0.2\n        - flask-sqlalchemy\n        - pandas >=2.2,<3\n        - plotly\n        # 3.0.1 has a build without proper constraints on setuptools\n        - python-daemon !=3.0.1\n\n    test:\n      imports:\n        - parsl\n        - parsl.monitoring\n      commands:\n        - pip check\n        - parsl-visualize --help\n      requires:\n        - python {{ python_min }}\n        - pip\n\n  - name: {{ name }}-with-aws\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python {{ python_min }}\n      run:\n        - python >={{ python_min }}\n        - {{ pin_subpackage(name, max_pin='x.x.x.x.x.x') }}\n        - boto3\n    test:\n      imports:\n        - parsl\n      commands:\n        - pip check\n      requires:\n        - python {{ python_min }}\n        - pip\n\n  - name: {{ name }}-with-kubernetes\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python {{ python_min }}\n      run:\n        - python >={{ python_min }}\n        - {{ pin_subpackage(name, max_pin='x.x.x.x.x.x') }}\n        - python-kubernetes\n    test:\n      imports:\n        - parsl.providers.kubernetes.kube\n        - kubernetes\n      commands:\n        - pip check\n      requires:\n        - python {{ python_min }}\n        - pip\n\n  - name: {{ name }}-with-google_cloud\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python {{ python_min }}\n      run:\n        - python >={{ python_min }}\n        - {{ pin_subpackage(name, max_pin='x.x.x.x.x.x') }}\n        - google-auth\n        - google-api-python-client\n    test:\n      imports:\n        - parsl\n      commands:\n        - pip check\n      requires:\n        - python {{ python_min }}\n        - pip\n\n  - name: {{ name }}-with-gssapi\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python {{ python_min }}\n      run:\n        - python >={{ python_min }}\n        - {{ pin_subpackage(name, max_pin='x.x.x.x.x.x') }}\n        - python-gssapi\n    test:\n      imports:\n        - parsl\n      commands:\n        - pip check\n      requires:\n        - python {{ python_min }}\n        - pip\n\n# azure 3.0.0 was not built for linux\n#  - name: {{ name }}-with-azure\n#    build:\n#      noarch: python\n#    requirements:\n#      host:\n#        - python {{ python_min }}\n#      run:\n#        - python >={{ python_min }}\n#        - {{ pin_subpackage(name, max_pin='x.x.x.x.x.x') }}\n#        - azure <=4\n#        - msrestazure\n#    test:\n#      imports:\n#        - parsl\n#      commands:\n#        - pip check\n#      requires:\n#        - python {{ python_min }}\n#        - pip\n\n  - name: {{ name }}-with-workqueue\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python {{ python_min }}\n      run:\n        - python >={{ python_min }}\n        - {{ pin_subpackage(name, max_pin='x.x.x.x.x.x') }}\n        - ndcctools\n        # there is no work_queue pypi or conda-forge package\n        # - work_queue\n    test:\n      imports:\n        - parsl\n      commands:\n        - pip check\n      requires:\n        - python {{ python_min }}\n        - pip\n\n  - name: {{ name }}-with-flux\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python {{ python_min }}\n      run:\n        - python >={{ python_min }}\n        - {{ pin_subpackage(name, max_pin='x.x.x.x.x.x') }}\n        - pyyaml\n        - cffi\n        - jsonschema\n    test:\n      imports:\n        - parsl\n      commands:\n        - pip check\n      requires:\n        - python {{ python_min }}\n        - pip\n\n  - name: {{ name }}-with-proxystore\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python {{ python_min }}\n      run:\n        - python >={{ python_min }}\n        - {{ pin_subpackage(name, max_pin='x.x.x.x.x.x') }}\n        - proxystore\n    test:\n      imports:\n        - parsl\n      commands:\n        - pip check\n      requires:\n        - python {{ python_min }}\n        - pip\n\n  - name: {{ name }}-with-radical-pilot\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python {{ python_min }}\n      run:\n        - python >={{ python_min }}\n        - {{ pin_subpackage(name, max_pin='x.x.x.x.x.x') }}\n        - radical.pilot 1.90\n        - radical.utils 1.90\n    test:\n      imports:\n        - parsl\n      commands:\n        - pip check\n      requires:\n        - python {{ python_min }}\n        - pip\n\n  - name: {{ name }}-with-globus_compute\n    build:\n      noarch: python\n    requirements:\n      host:\n        - python {{ python_min }}\n      run:\n        - python >={{ python_min }}\n        - {{ pin_subpackage(name, max_pin='x.x.x.x.x.x') }}\n        - globus-compute-sdk >=2.34.0\n    test:\n      imports:\n        - parsl\n      commands:\n        - pip check\n      requires:\n        - python {{ python_min }}\n        - pip\n\nabout:\n  home: https://github.com/Parsl/parsl\n  license: Apache-2.0\n  license_file: LICENSE\n  summary: Simple parallel workflows system for Python\n\n  description: |\n    Parsl is a Python library for programming and executing \n    data-oriented workflows (dataflows) in parallel.\n  doc_url: https://parsl.readthedocs.io/\n  dev_url: https://github.com/Parsl/parsl\n\nextra:\n  recipe-maintainers:\n    - ZhuozhaoLi\n    - yadudoc\n    - xylar\n",
