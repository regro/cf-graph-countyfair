  "linux_64_meta_yaml": {
    "about": {
      "description": "CoinUtils (Coin-OR Utilities) is an open-source collection of classes and\nfunctions that are generally useful to more than one COIN-OR project. These\nutilities include:\n\n  - classes for storing and manipulating sparse matrices and vectors,\n  - performing matrix factorization,\n  - parsing input files in standard formats, e.g. MPS,\n  - building representations of mathematical programs,\n  - performing simple presolve operations,\n  - warm starting algorithms for mathematical programs, and\n  - comparing floating point numbers with a tolerance, among others.\n",
      "dev_url": "https://github.com/coin-or/CoinUtils",
      "home": "https://github.com/coin-or/CoinUtils",
      "license": "EPL-2.0",
      "license_family": "OTHER",
      "license_file": "LICENSE",
      "summary": "COIN-OR Utilities (CoinUtils)"
    },
    "build": {
      "number": "3",
      "run_exports": [
        "coin-or-utils"
      ]
