    "about": {
      "description": "Python replacements for functions that are part of the IDL built-in\nlibrary, or part of astronomical IDL libraries.  The emphasis is on\nreproducing results of the astronomical library functions.  Only the bare\nminimum of IDL built-in functions are implemented to support this. pydl is\nan Astropy affiliated package.\n",
      "dev_url": "https://github.com/weaverba137/pydl",
      "doc_url": "https://pydl.readthedocs.io/en/latest/",
      "home": "https://github.com/weaverba137/pydl",
      "license": "BSD-3-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE.rst",
      "summary": "Python implementations of some functions from astronomical IDL libraries."
    },
    "build": {
      "entry_points": [
        "get_juldate = pydl.goddard.astro:get_juldate_main",
        "hogg_iau_name = pydl.pydlutils.misc:hogg_iau_name_main",
        "compute_templates = pydl.pydlspec2d.spec1d:template_input_main"
      ],
      "number": "3",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "mwcraig",
        "weaverba137"
      ]
    },
    "package": {
      "name": "pydl",
      "version": "1.0.1"
    },
    "requirements": {
      "host": [
        "python",
        "setuptools",
        "pip"
      ],
      "run": [
        "python",
        "astropy-base",
        "scipy",
        "matplotlib-base"
      ]
    },
    "schema_version": 0,
