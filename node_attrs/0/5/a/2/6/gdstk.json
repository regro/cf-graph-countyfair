  "linux_64_meta_yaml": {
    "about": {
      "description": "Gdstk (GDSII Tool Kit) is a C++ library for creation and manipulation of\nGDSII and OASIS files.  It is also available as a Python module meant to\nbe a successor to Gdspy.  Key features for the creation of complex CAD\nlayouts are included: boolean operations on polygons (AND, OR, NOT, XOR)\nbased on clipping algorithm; polygon offset (inward and outward rescaling\nof polygons); efficient point-in-polygon solutions for large array sets.\n",
      "dev_url": "https://github.com/heitzmann/gdstk",
      "doc_url": "https://heitzmann.github.io/gdstk/",
      "home": "https://github.com/heitzmann/gdstk",
      "license": "BSL-1.0",
      "license_file": "LICENSE.txt",
      "summary": "Gdstk (GDSII Tool Kit) is a C++/Python library for creation and manipulation of GDSII and OASIS files."
    },
    "build": {
      "number": "0",
      "script": "PYTHON -m pip install ./gdstk -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "heitzmann",
        "wshanks"
      ]
    },
    "package": {
      "name": "gdstk",
      "version": "0.9.61"
    },
    "requirements": {
      "build": [
        "cmake >=3.26.0",
        "cxx_compiler_stub",
        "c_stdlib_stub",
        "make"
      ],
      "host": [
