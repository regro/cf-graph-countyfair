    "about": {
      "description": "The libnl suite is a collection of libraries providing APIs to netlink\nprotocol based Linux kernel interfaces. Netlink is a IPC mechanism primarly\nbetween the kernel and user space processes. It was designed to be a more\nflexible successor to ioctl to provide mainly networking related kernel\nconfiguration and monitoring interfaces.\n",
      "dev_url": "https://github.com/tgraf/libnl-1.1-stable",
      "home": "https://www.infradead.org/~tgr/libnl/",
      "license": "LGPL-2.1-or-later",
      "license_family": "LGPL",
      "license_file": "COPYING",
      "summary": "Netlink Library Suite"
    },
    "build": {
      "number": "0",
      "run_exports": [
        "libnl"
      ],
      "script": [
        "./configure --prefix=$PREFIX",
        "make -j$CPU_COUNT",
        "make install"
      ]
    },
    "extra": {
      "recipe-maintainers": [
