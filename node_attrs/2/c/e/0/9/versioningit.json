    "about": {
      "description": "`versioningit` is yet another Python packaging plugin for automatically\ndetermining your package's version based on your version control\nrepository's tags.  Unlike others, it allows easy customization of the\nversion format and even lets you easily override the separate functions\nused for version extraction & calculation.\n",
      "dev_url": "https://github.com/jwodder/versioningit",
      "home": "https://github.com/jwodder/versioningit",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "Versioning It with your Version In Git"
    },
    "build": {
      "entry_points": [
        "versioningit = versioningit.__main__:main"
      ],
      "noarch": "python",
      "number": "0",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "jwodder"
      ]
    },
    "package": {
      "name": "versioningit",
      "version": "3.3.0"
    },
    "requirements": {
      "host": [
        "hatchling",
        "python 3.9",
        "pip"
      ],
      "run": [
        "python >=3.9",
        "importlib-metadata >=3.6",
        "packaging >=17.1",
        "tomli >=1.2,<3.0"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "b91ad7d73e73d21220e69540f20213f2b729a1f9b35c04e9e137eaf28d2214da",
