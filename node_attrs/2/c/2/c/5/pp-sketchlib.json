  "linux_64_meta_yaml": {
    "about": {
      "dev_url": "https://github.com/bacpop/pp-sketchlib",
      "doc_url": "https://poppunk.bacpop.org/",
      "home": "https://github.com/bacpop/pp-sketchlib",
      "license": "Apache-2.0 AND MIT AND BSD-3-Clause",
      "license_file": [
        "NOTICE",
        "LICENSE",
        "LICENSE_highfive",
        "LICENSE_bindash",
        "LICENSE_nthash",
        "LICENSE_kseq",
        "LICENSE_json.txt",
        "LICENSE_KMeansRex",
        "LICENSE_unordered_dense"
      ],
      "summary": "Library of sketching functions used by PopPUNK"
    },
    "build": {
      "entry_points": [
        "sketchlib = pp_sketch.__main__:main"
      ],
      "ignore_run_exports": [
        "cudatoolkit"
      ],
      "missing_dso_whitelist": [
        "*/libcuda.*"
      ],
      "number": "2",
      "script": [
        "echo $SKETCHLIB_INSTALL",
        "echo $CUDA_HOME",
        "PYTHON -m pip install . -vvv"
      ],
      "script_env": null
    },
    "extra": {
      "recipe-maintainers": [
        "johnlees"
      ]
    },
    "package": {
      "name": "pp-sketchlib",
      "version": "2.1.5"
    },
    "requirements": {
      "build": [
        "c_stdlib_stub",
        "c_compiler_stub",
        "cxx_compiler_stub",
        "cuda_compiler_stub",
        "cmake >=3.19",
        "make"
      ],
      "host": [
        "python",
        "pip",
        "setuptools",
        "pybind11",
        "highfive",
        "zlib",
        "eigen",
        "hdf5",
        "openblas",
        "libgomp"
      ],
      "run": [
        "python",
        "numpy",
        "scipy",
        "libblas * *openblas",
        "openblas",
        "zlib",
        "h5py",
        "docopt",
        "libgfortran"
      ]
    },
    "schema_version": 0,
    "source": {
      "patches": null,
      "sha256": "beb7f76dec502d8ee5df770cd768929a1f25db793f972efe7208fe972c8face6",
      "url": "https://github.com/bacpop/pp-sketchlib/archive/v2.1.5.tar.gz"
    },
    "test": {
      "commands": [
        "sketchlib --version"
      ],
      "imports": [
        "pp_sketchlib"
      ]
    }
  },
