    "about": {
      "description": "The python-javabridge package makes it easy to start a Java virtual machine (JVM) from Python and interact with it. Python code can interact with the JVM using a low-level API or a more convenient high-level API. Python-javabridge was developed for and is used by cell image analysis software CellProfiler.\n",
      "dev_url": "http://github.com/CellProfiler/python-javabridge/",
      "doc_url": "https://pythonhosted.org/javabridge/",
      "home": "http://github.com/CellProfiler/python-javabridge/",
      "license": "BSD-3-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE",
      "summary": "Python wrapper for the Java Native Interface"
    },
    "build": {
      "number": "2",
      "script": "PYTHON -m pip install --no-deps --ignore-installed . "
    },
    "extra": {
      "recipe-maintainers": [
        "sunyi000",
        "raivivek"
      ]
    },
