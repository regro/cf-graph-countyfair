  "linux_64_meta_yaml": {
    "about": {
      "description": "PySpice is a free and open source Python module which interfaces Python to\nthe Ngspice and Xyce circuit simulators. PySpice implements a Ngspice\nbinding and provides an oriented object API on top of SPICE, the simulation\noutput is converted to Numpy arrays for convenience.\n",
      "dev_url": "https://github.com/FabriceSalvaire/PySpice",
      "doc_url": "https://pyspice.fabrice-salvaire.fr",
      "home": "https://pyspice.fabrice-salvaire.fr",
      "license": "GPL-3.0-or-later",
      "license_family": "GPL",
      "license_file": "LICENSE.txt",
      "summary": "Simulate electronic circuit using Python and the Ngspice / Xyce simulators"
    },
    "build": {
      "number": "1",
      "script": [
        "PYTHON setup.py install --single-version-externally-managed --record=record.txt"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "FabriceSalvaire",
        "stuarteberg",
        "proppy"
      ]
    },
    "package": {
      "name": "pyspice",
      "version": "1.5"
    },
    "requirements": {
      "host": [
        "python",
        "pip"
      ],
      "run": [
        "python",
        "pyyaml >=5.3",
        "cffi >=1.14",
        "numpy >=1.18",
        "scipy >=1.4",
        "matplotlib-base >=3.4",
        "ply >=3.11",
        "ngspice-lib >=32"
      ]
    },
    "schema_version": 0,
    "source": {
      "patches": [
        "patches/matplotlib-3.4-api.patch"
      ],
      "sha256": "9779f8d32aab078b1bc4f3b92ae72cd578a5a0fa512b3f66ec3312ca97cb6773",
      "url": "https://github.com/FabriceSalvaire/PySpice/archive/v1.5.tar.gz"
    },
    "test": {
      "commands": [
        "python -c \"import PySpice; print(PySpice.__version__)\"",
        "pytest unit-test",
        "pyspice-post-installation --check-install",
        "invoke test.run-examples"
      ],
      "imports": [
        "PySpice",
        "PySpice.Config",
        "PySpice.Doc",
        "PySpice.Logging",
        "PySpice.Math",
        "PySpice.Physics",
        "PySpice.Plot",
        "PySpice.Probe",
        "PySpice.Spice",
        "PySpice.Tools",
        "PySpice.Unit"
      ],
      "requires": [
        "invoke",
        "pint",
        "pytest",
        "requests"
      ],
      "source_files": [
        "examples",
        "unit-test",
        "invoke.yaml",
        "tasks"
      ]
    }
  },
