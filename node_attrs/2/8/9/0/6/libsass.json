  "linux_64_meta_yaml": {
    "about": {
      "description": "This package provides a simple Python extension module sass which is binding\nLibsass (written in C/C++ by Hampton Catlin and Aaron Leung). It's very\nstraightforward and there isn't any headache related Python distribution/deployment.\n",
      "dev_url": "https://github.com/dahlia/libsass-python",
      "doc_url": "https://hongminhee.org/libsass-python/",
      "home": "https://github.com/dahlia/libsass-python",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "A straightforward binding of libsass for Python. Compile Sass/SCSS in Python with no Ruby stack at all!"
    },
    "build": {
      "entry_points": [
        "pysassc = pysassc:main"
      ],
      "number": "2",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "cshaley",
        "sannykr",
        "isuruf",
        "marcelotrevisani",
        "xhochy"
      ]
    },
