    "about": {
      "description": "21cmFAST provides a simple and fast simulation package for the cosmological\n21cm signal as either coeval cubes or full lightcones.\n",
      "dev_url": "https://github.com/21cmFAST/21cmFAST",
      "doc_url": "https://21cmFAST.readthedocs.io/",
      "home": "https://github.com/21cmFAST/21cmFAST",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "A semi-numerical cosmological simulation code for the 21cm signal"
    },
    "build": {
      "number": "1",
      "script": [
        "export SETUPTOOLS_SCM_PRETEND_VERSION=3.3.1",
        "INC=${PREFIX}/include LIB=${PREFIX}/lib PYTHON -m pip install . -vv"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "daviesje",
        "steven-murray"
      ]
    },
    "package": {
      "name": "21cmfast",
      "version": "3.3.1"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "libgomp"
      ],
      "host": [
        "python",
        "pip",
        "cffi",
        "fftw",
        "gsl",
        "libgomp",
        "libblas",
        "nomkl"
      ],
      "run": [
        "python",
        "scipy",
        "numpy",
        "click",
        "pyyaml",
        "cffi >=1.0",
        "astropy >=2.0",
        "h5py >=2.8.0",
        "cached-property",
        "matplotlib-base",
        "fftw",
        "gsl",
        "nomkl",
        "bidict",
        "psutil",
        "setuptools_scm"
      ]
    },
