  "linux_64_meta_yaml": {
    "about": {
      "description": "OMAS is a set of Python tools that aim at simplifying the interface\nbetween third-party codes and the ITER IMAS data storage\ninfrastructure. IMAS is a set of codes, an execution framework, a\ndata model, a data get/put API, and a data storage infrastructure\nused for manipulating ITER data. The IMAS data model organizes data\ninto a hierarchical tree. There data is stored in the leaf nodes, and\nthe branches are structures or arrays of structures.\n",
      "dev_url": "https://github.com/gafusion/omas",
      "doc_url": "https://gafusion.github.io/omas",
      "home": "https://gafusion.github.io/omas",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE.txt",
      "summary": "Ordered Multidimensional Array Structure"
    },
    "build": {
      "noarch": "python",
      "number": "0",
      "script": "PYTHON -m pip install . --no-deps -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "smithsp",
        "orso82"
      ]
    },
