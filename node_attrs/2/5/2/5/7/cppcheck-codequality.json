    "outputs": [
      {
        "build": null,
        "name": "cppcheck-codequality",
        "requirements": {
          "build": [],
          "host": [
            "pip",
            "poetry-core",
            "poetry-dynamic-versioning >=1.0.0,<2.0.0",
            "python 3.10.*"
          ],
          "run": [
            "python >=3.10",
            "xmltodict >=0.14.0,<0.15.0"
          ]
        },
        "tests": [
          {
            "python": {
              "imports": [
                "cppcheck_codequality"
              ],
              "python_version": [
                "3.10.*",
                "3.13.*"
              ]
            }
          },
          {
            "files": {
              "source": [
                "src/tests/"
              ]
            },
            "requirements": {
              "run": [
                "python 3.10.*",
                "pytest-cov"
              ]
            },
            "script": [
              "cppcheck-codequality --help",
              "cd src",
              "coverage run --source=cppcheck_codequality --branch -m pytest -vv --tb=long --color=yes",
              "coverage report --show-missing --skip-covered --fail-under=85"
            ]
          }
        ],
        "version": "1.5.0"
      }
    ],
