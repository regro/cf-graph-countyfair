              "gunzip z_ee.hep.gz",
              "DelphesSTDHEP \"${PREFIX}\"/cards/delphes_card_CMS.tcl delphes_output.root z_ee.hep",
              "root -l -b -q \"${PREFIX}\"/examples/Example1.C'(\"delphes_output.root\")' &> /dev/null",
