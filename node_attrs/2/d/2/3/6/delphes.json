  "linux_64_meta_yaml": {
    "about": {
      "description": "Delphes is a C++ framework, performing a fast multipurpose detector\nresponse simulation. The simulation includes a tracking system, embedded\ninto a magnetic field, calorimeters and a muon system. The framework is\ninterfaced to standard file formats (e.g. Les Houches Event File or HepMC)\nand outputs observables such as isolated leptons, missing transverse energy\nand collection of jets which can be used for dedicated analyses.\nThe simulation of the detector response takes into account the effect of\nmagnetic field, the granularity of the calorimeters and sub-detector resolutions.\nVisualisation of the final state particles is also built-in using the\ncorresponding ROOT library.",
      "dev_url": "https://github.com/delphes/delphes",
      "doc_url": "https://cp3.irmp.ucl.ac.be/projects/delphes",
      "home": "https://github.com/delphes/delphes",
      "license": "GPL-3.0-or-later",
      "license_family": "GPL-3.0-or-later",
      "license_file": "COPYING",
      "summary": "A framework for fast simulation of a generic collider experiment"
    },
    "build": {
      "number": "5",
      "script": [
        "grep -r -l \"external/\" examples/ | xargs sed -i 's|external/||g'",
        "CXXFLAGS=\"${CXXFLAGS}\" cmake ${CMAKE_ARGS} -DCMAKE_INSTALL_PREFIX=$PREFIX -DPYTHIA8_DATA=$(pythia8-config --datadir) -S . -B build",
        "cmake build -LH",
        "cmake --build build --clean-first --parallel=\"${CPU_COUNT}\"",
        "cmake --install build"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "pavel-demin",
        "matthewfeickert",
        "kratsg"
      ]
    },
    "outputs": [
      {
        "build": null,
        "name": "delphes",
        "requirements": {
          "build": [
            "cxx_compiler_stub",
            "c_stdlib_stub",
            "cmake",
            "make",
            "grep",
            "sed"
          ],
          "host": [
            "root_base",
