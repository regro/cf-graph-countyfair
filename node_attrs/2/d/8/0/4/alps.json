      "features": [
        "blas_openblas"
      ],
      "number": "5",
      "skip": true
    },
    "extra": {
      "recipe-maintainers": [
        "dolfim"
      ]
    },
    "package": {
      "name": "alps",
      "version": "2.3.0"
    },
    "requirements": {
      "build": [
        "toolchain",
        "cmake",
        "icu 58.*",
        "boost 1.65.1",
        "python",
        "numpy 1.8.*",
        "scipy",
        "hdf5 1.10.1",
        "blas 1.* openblas"
      ],
      "run": [
        "icu 58.*",
        "boost 1.65.1",
        "python",
        "numpy >=1.8",
        "scipy",
        "matplotlib",
        "hdf5 1.10.1",
        "blas 1.* openblas"
      ]
    },
    "source": {
      "patches": [
        "remove_unit_testing.patch",
        "skip_boost_numpy.patch",
        "fix_skeleton.patch",
        "no_python_extra_vars.patch",
        "numpy_array_cdecl.patch",
        "fix_xsl_padding.patch",
        "remove_padding_from_prefix.patch"
      ],
      "sha256": "e64208d1e5acdd6f569277413c4867e1fa366cf4a224570eacbf1e9939fca2d2",
      "url": "http://alps.comp-phys.org/static/software/releases/alps-2.3.0-src.tar.gz"
    },
    "test": {
      "commands": [
        "test -f $PREFIX/lib/libalps.so",
        "python -c \"import pyalps\"",
        "python -c \"import pyalps; print(pyalps.tools.xslPath())\"",
        "python -c \"import pyalps; pyalps.copyStylesheet('.')\""
      ]
    }
  },
