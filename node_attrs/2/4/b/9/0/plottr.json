  "linux_64_meta_yaml": {
    "about": {
      "description": "A framework for inspecting data, based on flowcharts from pyqtgraph.\nplottr's main aim is to allow the user to define custom data processing flows and plotting.\nA particular use case is data filtering and plotting.\n",
      "dev_url": "https://github.com/toolsforexperiments/plottr",
      "doc_url": "https://plottr.readthedocs.io/en/latest/",
      "home": "https://github.com/toolsforexperiments/plottr",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "A tool for live plotting and processing data"
    },
    "build": {
      "entry_points": [
        "plottr-monitr = plottr.apps.monitr:script",
        "plottr-inspectr = plottr.apps.inspectr:script",
        "plottr-autoplot-ddh5 = plottr.apps.autoplot:script"
      ],
      "number": "0",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "trevormorgan",
        "astafan8",
        "jenshnielsen",
        "wpfff"
      ]
    },
