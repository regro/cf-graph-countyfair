  "linux_64_meta_yaml": {
    "about": {
      "description": "\"TeNPy is a package of Python code for the simulation of quantum many-body systems with tensor networks.\nAmongst others, it includes codes for the density matrix renormalization group (DMRG) algorithm\nand the time evolving block decimation (TEBD) algorithm based on matrix product states (MPS).\nThe minimal dependency is Python, Numpy and Scipy, but it ships with an optional Cython extension to\nreduce the Python overhead in tensor contractions.\"\n",
      "dev_url": "https://github.com/tenpy/tenpy",
      "doc_url": "https://tenpy.readthedocs.io/",
      "home": "https://github.com/tenpy/tenpy",
      "license": "Apache-2.0",
      "license_file": "LICENSE",
      "license_url": "https://www.apache.org/licenses/LICENSE-2.0",
      "summary": "Simulation of quantum many-body systems with tensor networks in Python"
    },
    "build": {
      "number": "0",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "jhauschild",
        "Jakob-Unfried"
      ]
    },
    "package": {
      "name": "physics-tenpy",
      "version": "1.0.6"
    },
    "requirements": {
      "build": [
        "cxx_compiler_stub",
        "c_stdlib_stub"
      ],
      "host": [
        "pip",
        "setuptools",
        "python",
        "cython",
        "numpy",
        "scipy"
      ],
      "run": [
        "python",
        "scipy",
        "bottleneck",
        "pyyaml",
        "h5py"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "5a382a10e4f91ba6a5afd1fb06f065b01a84e590d9ea96057cd0aa1b2b2be119",
      "url": "https://pypi.org/packages/source/p/physics-tenpy/physics_tenpy-1.0.6.tar.gz"
    },
    "test": {
      "commands": [
        "pytest -m \"not slow\" --ignore=tests/test_examples.py tests"
      ],
