  "linux_64_meta_yaml": {
    "about": {
      "description": "Great Expectations helps teams save time and promote analytic integrity\nby offering a unique approach to automated testing: pipeline tests.\nPipeline tests are applied to data (instead of code) and at batch time\n(instead of compile or deploy time). Pipeline tests are like unit tests\nfor datasets: they help you guard against upstream data changes and monitor\ndata quality.\n\nSoftware developers have long known that automated testing is\nessential for managing complex codebases. Great Expectations brings\nthe same discipline, confidence, and acceleration to data science\nand engineering teams.\n",
      "dev_url": "https://github.com/great-expectations/great_expectations",
      "doc_url": "https://docs.greatexpectations.io",
      "home": "https://github.com/great-expectations/great_expectations",
      "license": "Apache-2.0",
      "license_family": "Apache",
      "license_file": "LICENSE",
      "summary": "Always know what to expect from your data."
    },
    "build": {
      "entry_points": [
        "great_expectations = great_expectations.cli:main"
      ],
      "noarch": "python",
      "number": "0",
      "script": "PYTHON -m pip install . --no-deps -vv"
    },
