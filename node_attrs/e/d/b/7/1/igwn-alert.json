    "about": {
      "description": "The IGWN Alert System (LVAlert) is a prototype notification service\nbuilt on Apache Kafka, using the publish-subscribe (pubsub) protocol.\nIt is a higher-level modification of [SCIMMA](https://scimma.org/)’s\n[hop-client](https://hop-client.readthedocs.io/) to streamline\nreceiving and responding to alerts from\n[GraceDB](https://gracedb.ligo.org).\n",
      "dev_url": "https://git.ligo.org/computing/igwn-alert/client.git",
      "doc_url": "https://igwn-alert.readthedocs.io/",
      "home": "https://igwn-alert.readthedocs.io/",
      "license": "GPL-3.0-or-later",
      "license_family": "GPL",
      "license_file": "COPYING",
      "summary": "IGWN Alert Network"
    },
    "build": {
      "entry_points": [
        "igwn-alert=igwn_alert.tool:main"
      ],
      "noarch": "python",
      "number": "0",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "aepace",
        "duncanmmacleod"
      ]
    },
    "package": {
      "name": "igwn-alert",
      "version": "0.7.0"
    },
    "requirements": {
      "host": [
        "pip",
        "python 3.9",
        "setuptools"
      ],
      "run": [
        "adc-streaming >=2.3.0",
        "hop-client >=0.7.0",
        "pyopenssl >=23.0.0",
        "python >=3.9",
        "python-confluent-kafka >=2.0.0",
        "safe-netrc"
      ]
    },
    "schema_version": 0,
