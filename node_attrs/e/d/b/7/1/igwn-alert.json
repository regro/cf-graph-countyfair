      "url": "https://pypi.org/packages/source/i/igwn-alert/igwn_alert-0.7.0.tar.gz"
    },
    "test": {
      "commands": [
        "python -m pip check igwn-alert",
        "python -m pip show igwn-alert",
        "python -m pytest --pyargs igwn_alert.tests -ra -v",
        "igwn-alert --help"
      ],
      "requires": [
        "pip",
        "pytest",
        "python 3.9"
      ]
    }
