  "linux_64_meta_yaml": {
    "about": {
      "description": "atool is a script for managing file archives of various types (tar, tar+gzip, zip etc).\n",
      "home": "https://github.com/solsticedhiver/atool2",
      "license": "GPL-3.0-only",
      "license_file": "COPYING",
      "summary": "atool with support for zstd"
    },
    "build": {
      "number": "0",
      "script": [
        "ln -fs $BUILD_PREFIX/share/automake-*/install-sh build-aux/install-sh",
        "./configure --prefix=$PREFIX",
        "sed -i\"\" -e \"1s@/usr/bin/perl -w@/usr/bin/perl@g\" atool",
        "make install",
        "echo \"#!/usr/bin/env perl\" > tmp && cat $PREFIX/bin/atool >> tmp && mv tmp $PREFIX/bin/atool"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "jonashaag"
      ]
    },
    "package": {
      "name": "atool2",
      "version": "0.2"
    },
    "requirements": {
      "build": [
        "automake",
        "make",
        "perl >=5.004"
      ],
      "run": [
        "perl >=5.004"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "87fc55cc21e2ac74c356797fc4e1d73b37dd37a6b91811cea778c68787079239",
      "url": "https://github.com/solsticedhiver/atool2/archive/0.2.tar.gz"
    },
    "test": {
      "commands": [
        "atool --help",
        "aunpack --help",
        "apack --help"
      ]
    }
  },
