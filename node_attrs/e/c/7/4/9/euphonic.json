      "number": "1"
    },
    "extra": {
      "recipe-maintainers": [
        "MialLewis",
        "DBJochym",
        "ajjackson",
        "rebeccafair"
      ]
    },
    "package": {
      "name": "euphonic",
      "version": "1.4.5"
