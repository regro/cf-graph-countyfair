  "raw_meta_yaml": "schema_version: 1\n\ncontext:\n  component_name: launch\n  repo_name: ${{ \"gz-\" ~ component_name }}\n  version: 9.0.0\n  major_version: \"9\"\n  name: ${{ repo_name }}\n  component_version: ${{ component_name ~ major_version }}\n  cxx_name: ${{ \"lib\" ~ name }}\n\nrecipe:\n  name: ${{ name }}\n  version: ${{ version }}\n\nsource:\n  - url: https://github.com/gazebosim/${{ repo_name }}/archive/${{ repo_name }}${{ major_version }}_${{ version }}.tar.gz\n    sha256: 4f1bd0ac59cf9da897307ceaf781eefe498afb7d3175dc72b9689e6fca727ad6\n    patches:\n      - unvendor_backward.patch\n\nbuild:\n  skip:\n    - ppc64le\n  number: 0\n\noutputs:\n  - package:\n      name: ${{ cxx_name }}\n    build:\n      script:\n        - if: unix\n          then: build_cxx.sh\n        - if: win\n          then: bld_cxx.bat\n    requirements:\n      build:\n        - ${{ compiler('cxx') }}\n        - ${{ compiler('c') }}\n        - ${{ stdlib('c') }}\n        - ninja\n        - cmake\n        - pkg-config\n      host:\n        - libgz-cmake\n        - libgz-common\n        - libgz-plugin\n        - libgz-tools\n        - libgz-transport\n        - libgz-msgs\n        - libgz-math\n        - libgz-gui\n        - libgz-sim\n        - backward-cpp\n        - tinyxml2\n        - qt6-main\n        - if: linux\n          then:\n            - libwebsockets\n            - xorg-libxfixes\n            - libgl-devel\n            - elfutils\n        - libabseil\n        - libprotobuf\n        - libsdformat\n      run_exports:\n        - ${{ pin_subpackage(cxx_name, upper_bound='x') }}\n    tests:\n      - package_contents:\n          include:\n            - gz/${{ component_version }}/gz/${{ component_name }}.hh\n          lib:\n            - gz-${{ component_name }}\n      # Commented as a workaround for https://github.com/conda-forge/gz-sim-feedstock/issues/109\n      #- script:\n      #    - cmake-package-check gz-${{ component_name }} --targets gz-${{ component_name }}::gz-${{ component_name }}\n      #  requirements:\n      #    run:\n      #      - cmake-package-check\n      #      - ${{ compiler('c') }}\n      #      - ${{ compiler('cxx') }}\n\n  - package:\n      name: ${{ name }}\n    requirements:\n      run:\n        - ${{ pin_subpackage(cxx_name, exact=True) }}\n      run_exports:\n        - ${{ pin_subpackage(cxx_name, upper_bound='x') }}\n    tests:\n      - script:\n           - echo \"Metapackage, no test needed\"\n      # comment out for https://github.com/conda-forge/gz-sim-feedstock/issues/109\n      #    - cmake-package-check gz-${{ component_name }} --targets gz-${{ component_name }}::gz-${{ component_name }}\n      #  requirements:\n      #    run:\n      #      - cmake-package-check\n      #      - ${{ compiler('c') }}\n      #      - ${{ compiler('cxx') }}\n\nabout:\n  homepage: https://github.com/gazebosim/${{ repo_name }}\n  license: Apache-2.0\n  license_file: LICENSE\n  summary: Run and manage programs and plugins.\n\nextra:\n  feedstock-name: ${{ repo_name }}\n  recipe-maintainers:\n    - wolfv\n    - traversaro\n    - Tobias-Fischer\n    - j-rivero\n",
