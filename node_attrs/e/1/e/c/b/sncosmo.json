  "linux_64_meta_yaml": {
    "about": {
      "description": "SNCosmo synthesizes supernova spectra and photometry from SN models,\nand has functions for fitting and sampling SN model parameters given\nphotometric light curve data. It includes supernova models such as SALT2,\nMLCS2k2, Hsiao, Nugent, PSNID, SNANA and Whalen models, as well as a\nvariety of built-in bandpasses and magnitude systems, and provides\nconvenience functions for reading and writing commonly used data formats.\nThe library is extensible, allowing user-defined models, bandpasses,\nand magnitude systems.\n",
      "dev_url": "https://github.com/sncosmo/sncosmo",
      "doc_url": "http://sncosmo.readthedocs.io/",
      "home": "https://github.com/sncosmo/sncosmo",
      "license": "BSD-3-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE.rst",
      "summary": "Python library for supernova cosmology"
    },
    "build": {
      "number": "1",
      "script": "PYTHON -m pip install . --no-deps -vv"
    },
