  "linux_64_meta_yaml": {
    "about": {
      "description": "Python interface for OSQP, the Operator Splitting QP Solver.\n\nTo use the `codegen` interface, install `cmake` and an appropriate\ncompiler version. (We don't install this by default.)\n",
      "dev_url": "http://github.com/osqp/osqp-python",
      "doc_url": "https://osqp.org/",
      "home": "https://osqp.org/",
      "license": "Apache-2.0",
      "license_family": "Apache",
      "license_file": "LICENSE",
      "summary": "Python interface for OSQP, the Operator Splitting QP Solver"
    },
    "build": {
      "number": "0",
      "script": [
        "export SETUPTOOLS_SCM_PRETEND_VERSION=1.0.5",
        "export CMAKE_GENERATOR=Ninja",
        "export CMAKE_INCLUDE_PATH=$PREFIX/include",
        "PYTHON -m pip install . --no-deps -vv"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "sebp",
        "djsutherland",
        "h-vetinari",
        "bstellato",
        "gbanjac",
        "jayfurmanek"
      ]
    },
    "package": {
      "name": "osqp",
      "version": "1.0.5"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub",
        "cxx_compiler_stub",
        "cmake",
        "ninja",
        "pybind11"
      ],
      "host": [
        "python",
        "scikit-build-core",
        "setuptools_scm",
        "pip",
        "numpy",
        "libosqp 1.0.0",
        "setuptools"
      ],
      "run": [
        "python",
        "qdldl-python",
        "jinja2",
        "joblib",
        "scipy"
      ],
      "run_constrained": null
    },
    "schema_version": 0,
    "source": {
      "patches": [
        "patches/0001-Link-against-shared-osqp-library.patch",
        "patches/0003-disable-codegen-ABI-is-not-compatible.patch"
      ],
      "sha256": "fe555180a1ad7e25bb3e3df787a1539de664a9fd0b1639271f8d5553bc22049e",
      "url": "https://github.com/osqp/osqp-python/archive/refs/tags/v1.0.5.tar.gz"
    },
    "test": {
      "commands": [
        "python -m pytest --pyargs osqp -k \"not (codegen or test_multithread or nn_test.py)\""
      ],
      "imports": [
        "osqp",
        "osqp.codegen"
      ],
      "requires": [
        "blas * mkl",
        "pytest",
        "pytorch"
      ]
    }
  },
