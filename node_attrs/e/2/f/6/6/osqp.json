{
  "archived": false,
  "branch": "main",
  "conda-forge.yml": {
    "build_platform": {
      "linux_aarch64": "linux_64",
      "linux_ppc64le": "linux_64",
      "osx_arm64": "osx_64"
    },
    "conda_build": {
      "pkg_format": "2"
    },
    "conda_forge_output_validation": true,
    "github": {
      "branch_name": "main",
      "tooling_branch_name": "main"
    },
    "test": "native_and_emulated"
  },
  "feedstock_name": "osqp",
  "hash_type": "sha256",
  "linux_64_meta_yaml": {
    "about": {
      "description": "Python interface for OSQP, the Operator Splitting QP Solver.\n\nTo use the `codegen` interface, install `cmake` and an appropriate\ncompiler version. (We don't install this by default.)\n",
      "dev_url": "http://github.com/osqp/osqp-python",
      "doc_url": "https://osqp.org/",
      "home": "https://osqp.org/",
      "license": "Apache-2.0",
      "license_family": "Apache",
      "license_file": "LICENSE",
      "summary": "Python interface for OSQP, the Operator Splitting QP Solver"
    },
    "build": {
      "number": "1",
      "script": [
        "export SETUPTOOLS_SCM_PRETEND_VERSION=0.6.7.post3",
        "PYTHON -m pip install . --no-deps -vv"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "sebp",
        "djsutherland",
        "h-vetinari",
        "bstellato",
        "gbanjac",
        "jayfurmanek"
      ]
    },
    "package": {
      "name": "osqp",
      "version": "0.6.7.post3"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub",
        "cxx_compiler_stub",
        "cmake",
        "ninja"
      ],
      "host": [
        "python",
        "setuptools_scm",
        "pip",
        "numpy",
        "libosqp 0.6.3",
        "setuptools"
      ],
      "run": [
        "python",
        "qdldl-python",
        "scipy"
      ],
      "run_constrained": null
    },
    "schema_version": 0,
    "source": {
      "patches": [
        "patches/0002-use-ninja-universally.patch",
        "patches/0003-point-to-our-own-builds.patch",
        "patches/0004-do-not-rebuild-libosqp-here.patch",
        "patches/0005-disable-codegen-ABI-is-not-compatible.patch"
      ],
      "sha256": "033692f64877e7a8ec6bb1bbef321e2da467e75cf346ef5e48f601dae498a9b9",
      "url": "https://github.com/osqp/osqp-python/archive/refs/tags/v0.6.7.post3.tar.gz"
    },
    "test": {
      "commands": [
        "python -m pytest --pyargs osqp -k \"not (codegen or test_multithread)\""
      ],
      "imports": [
        "osqp",
        "osqp.codegen",
        "osqppurepy"
      ],
      "requires": [
        "blas=*=mkl",
        "cvxopt",
        "pytest",
        "setuptools"
      ]
    }
  },
