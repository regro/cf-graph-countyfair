      "license_file": "COPYING",
      "summary": "Kinematic Likelihood Fitter for top quark physics"
    },
    "build": {
      "number": "0",
      "script": {
        "content": "export BATINSTALLDIR=\"${PREFIX}\"\ncmake ${CMAKE_ARGS} \\\n  -DBUILTIN_BAT=FALSE \\\n  -DINSTALL_TESTS=OFF \\\n  -DINSTALL_EXAMPLES=OFF \\\n  -DCMAKE_CXX_STANDARD=$(root-config --cxxstandard) \\\n  -DBUILD_STATIC_LIBS=OFF \\\n  -S . \\\n  -B build\ncmake build -LH\ncmake --build build --parallel \"${CPU_COUNT}\"\ncmake --install build\n"
      }
    },
