  "feedstock_name": "pyshtools",
  "hash_type": "sha256",
  "linux_64_meta_yaml": {
    "about": {
      "description": "SHTOOLS/pysthools is a Fortran-95/Python library that can be used to\nperform spherical harmonic transforms, multitaper spectral analyses on the\nsphere, expansions of functions into Slepian bases, and standard operations\non global gravitational and magnetic field data.\n",
      "dev_url": "https://github.com/SHTOOLS/SHTOOLS",
      "doc_url": "https://shtools.github.io/SHTOOLS/",
      "home": "https://github.com/SHTOOLS/SHTOOLS",
      "license": "GPL-2.0-or-later",
      "license_family": "GPL",
      "license_file": "LICENSE",
      "summary": "Tools for working with spherical harmonics"
    },
    "build": {
      "error_overlinking": true,
      "number": "2"
    },
    "extra": {
      "recipe-maintainers": [
        "megies",
        "moorepants",
        "markwieczorek",
        "jan-janssen"
      ]
    },
    "package": {
      "name": "pyshtools",
      "version": "4.13.1"
    },
    "requirements": {
      "build": [
        "cmake",
        "c_compiler_stub",
        "c_stdlib_stub",
        "fortran_compiler_stub"
      ],
      "host": [
        "python",
        "pip",
        "setuptools",
        "meson-python >=0.14",
        "setuptools_scm >=8",
        "pkg-config",
        "jupyter",
        "numpy",
        "fftw",
        "libblas",
        "liblapack"
      ],
      "run": [
        "python",
        "fftw",
        "scipy",
        "matplotlib-base",
        "astropy",
        "xarray",
        "requests",
        "pooch",
        "ducc0"
      ],
      "run_constrained": [
        "cartopy >=0.18",
        "gmt >=6.3.0",
        "pygmt >=0.7.0"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "cc4a323e9cbc905c04ae9e2e9fedeea6d76f3315a6863ede353a4dec87b8c018",
      "url": "https://pypi.org/packages/source/p/pyshtools/pyshtools-4.13.1.tar.gz"
    },
    "test": {
      "commands": [
        "make -C examples/python -f Makefile no-timing"
      ],
      "imports": [
        "pyshtools"
      ],
      "requires": [
        "make",
        "cartopy",
        "pygmt"
      ],
      "source_files": [
        "examples/*"
      ]
    }
  },
