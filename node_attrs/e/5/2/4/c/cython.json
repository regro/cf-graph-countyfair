    "about": {
      "description": "Cython is an optimising static compiler for both the Python programming\nlanguage and the extended Cython programming language. It makes writing C\nextensions for Python as easy as Python itself.\n",
      "dev_url": "https://github.com/cython/cython",
      "doc_url": "https://docs.cython.org",
      "home": "http://www.cython.org/",
      "license": "Apache-2.0",
      "license_file": "LICENSE.txt",
      "summary": "The Cython compiler for writing C extensions for the Python language"
    },
    "build": {
      "entry_points": [
        "cython = Cython.Compiler.Main:setuptools_main",
        "cythonize = Cython.Build.Cythonize:main",
        "cygdb = Cython.Debugger.Cygdb:main"
      ],
      "noarch": "python",
      "number": "2",
      "script": [
        "PYTHON -m pip install . --no-deps -vv",
        "export NO_CYTHON_COMPILE=true"
      ],
      "skip": true,
      "track_features": [
        "cython_no_compile"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "jakirkham",
        "msarahan",
        "ocefpaf",
        "synapticarbors",
        "isuruf",
        "scopatz",
