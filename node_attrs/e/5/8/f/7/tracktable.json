  "linux_64_meta_yaml": {
    "about": {
      "description": "Tracktable is for analyzing trajectories from moving objects.  It provides modules to read in points/trajectories, assemble trajectories from points, compute features and feature vectors for trajectories, run DBSCAN clustering, search for similar trajectories, and render the results in static or dynamic maps.\n",
      "dev_url": "https://github.com/sandialabs/tracktable",
      "doc_url": "https://tracktable.readthedocs.io/en/latest/",
      "home": "https://tracktable.sandia.gov/",
      "license": "BSD-2-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE.txt",
      "license_url": "https://opensource.org/licenses/BSD-2-Clause",
      "summary": "Trajectory Analysis and Rendering"
    },
    "build": {
      "number": "2"
    },
    "extra": {
      "recipe-maintainers": [
        "pabaxley",
        "atwilso"
      ]
    },
    "package": {
      "name": "tracktable",
      "version": "1.7.3"
    },
    "requirements": {
      "build": [
        "cxx_compiler_stub",
        "c_stdlib_stub",
        "git",
        "git-lfs",
        "cmake >=3.19.5",
        "make"
      ],
      "host": [
        "python",
        "git",
        "git-lfs",
        "setuptools",
        "numpy",
        "cartopy >=0.18.0",
        "jupyter_core",
        "nbconvert",
        "pytz",
        "shapely",
        "tqdm",
        "libboost-devel",
        "libboost-python-devel",
        "pip"
      ],
      "run": [
        "python",
        "setuptools",
        "cartopy >=0.18.0",
        "folium >=0.11.0",
        "matplotlib-base",
        "numpy",
        "pyshp",
        "pytz >=2020.1",
        "shapely",
        "six",
        "tqdm",
        "tracktable-data >=1.7.2"
      ]
    },
    "schema_version": 0,
    "source": {
      "git_rev": "v1.7.3-rc1",
      "git_url": "https://github.com/sandialabs/tracktable"
    },
    "test": {
      "imports": [
        "tracktable",
        "tracktable.algorithms.dbscan",
        "tracktable.algorithms.distance_geometry",
        "tracktable.applications.assemble_trajectories",
        "tracktable.applications.trajectory_splitter",
