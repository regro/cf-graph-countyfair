  "linux_64_meta_yaml": {
    "about": {
      "description": "View time series data of channels from LIGO network data servers\n(NDS) with an intuitive GUI interface.  Features include:\n* online and offline data viewing\n* intuitive mouse gestures for pan and zoom\n* auto-retrieval of new data as the time range expands\n* auto-switching from raw data to second/minute trends depending\n  on time scale\n* supports both NDS1 and NDS2 protocols, for viewing both in\n  LIGO/Kagra control rooms as well as anywhere else in the world\n",
      "dev_url": "https://git.ligo.org/cds/ndscope",
      "home": "https://git.ligo.org/cds/ndscope",
      "license": "GPL-3.0-or-later",
      "license_family": "GPL",
      "license_file": "COPYING",
      "summary": "Next-generation NDS oscilloscope"
    },
    "build": {
      "entry_points": [
        "ndscope = ndscope.__main__:main",
        "ndschans = ndscope.channel_select:main"
      ],
      "noarch": "python",
      "number": "1",
      "script": "PYTHON -m pip install . --no-deps --ignore-installed --no-cache-dir -vvv"
    },
    "extra": {
      "recipe-maintainers": [
        "duncanmmacleod",
        "jrollins"
      ]
    },
    "package": {
      "name": "ndscope",
      "version": "0.19.3"
    },
    "requirements": {
      "host": [
        "pip",
        "python 3.9",
        "setuptools",
        "setuptools-scm"
      ],
      "run": [
        "cairosvg",
        "gpstime",
        "h5py",
        "matplotlib-base",
        "numpy",
        "pyqtgraph",
        "python >=3.9",
        "python-dateutil",
        "python-nds2-client",
        "pyyaml",
        "pyqt >=5",
        "qtpy",
        "setproctitle"
      ]
    },
