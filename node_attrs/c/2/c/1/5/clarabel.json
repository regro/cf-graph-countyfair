  "linux_64_meta_yaml": {
    "about": {
      "description": "Clarabel is an interior point numerical solver for convex optimization problems implemented\nin Rust.  It solves linear programs (LPs), quadratic programs (QPs), second-order cone programs\n(SOCPs) and semidefinite programs (SDPs). It also solves problems with exponential, power\ncone and generalized power cone constraints.\n",
      "dev_url": "https://github.com/oxfordcontrol/Clarabel.rs",
      "doc_url": "https://oxfordcontrol.github.io/ClarabelDocs/",
      "home": "https://oxfordcontrol.github.io/ClarabelDocs/",
      "license": "Apache-2.0",
      "license_family": "Apache",
      "license_file": [
        "LICENSE.md",
        "THIRDPARTY.yml"
      ],
      "summary": "Python interface for Clarabel: Interior Point Conic Optimization Solver"
    },
    "build": {
      "number": "0",
      "script": [
        "PYTHON -m pip install . --no-deps --ignore-installed -vv",
        "cargo-bundle-licenses --format yaml --output THIRDPARTY.yml"
      ]
    },
    "extra": {
      "recipe-maintainers": [
