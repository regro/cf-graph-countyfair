      ]
    },
    "package": {
      "name": "xtensor-zarr",
      "version": "0.0.7"
    },
    "requirements": {
      "build": [
        "make",
        "cmake",
        "cxx_compiler_stub"
      ],
      "host": [
        "zarray >=0.1,<0.2",
        "xtensor >=0.23.8,<0.24",
        "xtensor-io >=0.12.7,<0.13",
        "nlohmann_json >=3.9.1,<3.10",
        "cpp-filesystem >=1.3.8,<1.4",
        "gdal >=3.0.4,<3.1",
        "zlib >=1.2.11,<1.3",
        "blosc >=1.21.0,<1.22"
      ],
      "run": [
        "gdal",
        "zlib",
        "blosc",
        "zarray",
        "xtensor",
        "xtensor-io",
        "nlohmann_json",
        "cpp-filesystem"
      ]
    },
