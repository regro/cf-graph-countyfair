    "about": {
      "description": "Jupyter Translate provides functionality to extract localizable strings\nfrom Jupyterlab extensions. Extensions can update the `jupyterlab-\nlanguage-packs` repository or provide localization files in the extension\npackage.\n",
      "dev_url": "https://github.com/jupyterlab/jupyterlab-translate",
      "doc_url": "https://github.com/jupyterlab/jupyterlab-translate",
      "home": "https://github.com/jupyterlab/jupyterlab-translate",
      "license": "BSD-3-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE.txt",
      "summary": "Jupyterlab Language Pack Translations Helper"
    },
    "build": {
      "entry_points": [
        "jupyterlab-translate = jupyterlab_translate.cli:main",
        "gettext-extract = jupyterlab_translate.gettext_extract:main"
      ],
      "noarch": "python",
      "number": "1",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "fcollonval",
        "goanpeca"
      ]
    },
    "package": {
      "name": "jupyterlab-translate",
      "version": "1.3.7"
    },
    "requirements": {
