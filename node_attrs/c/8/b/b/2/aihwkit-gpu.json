  "raw_meta_yaml": "{% set pname = \"aihwkit\" if cuda_compiler_version == \"None\" else \"aihwkit-gpu\" %}\n{% set name = \"aihwkit\" %}\n{% set version = \"1.0.0\" %}\n{% set number = 0 %}\n\npackage:\n  name: {{ pname|lower }}\n  version: {{ version }}\n\nsource:\n  url: https://pypi.org/packages/source/{{ name[0] }}/{{ name }}/{{ name }}-{{ version }}.tar.gz\n  sha256: 129799705db6031dda5dfae9f0fea92db944fa7f8d92ff518ae287cfb66b306b\n\nbuild:\n  skip: true  # [win or osx]\n  # not yet ready for python 3.12\n  skip: true  # [py>=312]\n  ignore_run_exports_from:\n    - libcusolver-dev  # [cuda_compiler_version != \"None\"]\n    - libcusparse-dev  # [cuda_compiler_version != \"None\"]\n    - pytorch\n    - python\n  number: {{ number }}\n\nrequirements:\n  build:\n    - {{ compiler('c') }}\n    - {{ stdlib(\"c\") }}\n    - {{ compiler('cxx') }}\n    - {{ compiler('cuda') }}  # [cuda_compiler_version != \"None\"]\n    - cmake\n    - numpy\n    - pybind11\n    - make\n    - mypy\n    - libgomp  # [linux]\n    - openblas\n  host:\n    - cuda-version {{ cuda_compiler_version }}  # [cuda_compiler_version != \"None\"]\n    - libcublas-dev                             # [cuda_compiler_version != \"None\"]\n    - libcurand-dev                             # [cuda_compiler_version != \"None\"]\n    - python\n    - openblas\n    - blas\n    - pip ==23.0.1\n    - scikit-build\n    - setuptools                                # Added setuptools for pip build backend\n    - pytorch * cuda*                           # [cuda_compiler_version != \"None\"]\n    - pytorch                                   # [cuda_compiler_version == \"None\"]\n    ########################################################################################################################################\n    # Needed because PyTorch's ATen `#include`s them                                                                                       #\n    # xref: https://github.com/pytorch/pytorch/blob/50efbb9f1e71