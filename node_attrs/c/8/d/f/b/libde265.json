  "linux_64_meta_yaml": {
    "about": {
      "description": "libde265 is an open source implementation of the h.265 video codec.\nIt is written from scratch and has a plain C API to enable a simple\nintegration into other software. libde265 supports WPP and tile-based\nmultithreading and includes SSE optimizations. The decoder includes all\nfeatures of the Main profile and correctly decodes almost all conformance\nstreams.\n",
      "dev_url": "https://github.com/strukturag/libde265",
      "home": "http://www.libde265.org/",
      "license": "LGPL-3.0-or-later",
      "license_family": "LGPL",
      "license_file": "COPYING",
      "summary": "Open h.265 video codec implementation"
    },
