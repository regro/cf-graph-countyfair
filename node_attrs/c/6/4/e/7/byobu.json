  "linux_64_meta_yaml": {
    "about": {
      "description": "Byobu is a GPLv3 open source text-based window manager and terminal\nmultiplexer. It was originally designed to provide elegant enhancements to\nthe otherwise functional, plain, practical GNU Screen, for the Ubuntu server\ndistribution. Byobu now includes an enhanced profiles, convenient\nkeybindings, configuration utilities, and toggle-able system status\nnotifications for both the GNU Screen window manager and the more modern\nTmux terminal multiplexer, and works on most Linux, BSD, and Mac\ndistributions.",
      "dev_url": "https://github.com/dustinkirkland/byobu",
      "doc_url": "https://www.byobu.org/documentation",
      "home": "https://www.byobu.org/",
      "license": "GPL-3.0-only",
      "license_family": "GPL-3.0-only",
      "license_file": "COPYING",
      "summary": "Text-based window manager and terminal multiplexer"
    },
    "build": {
      "number": "0"
    },
    "extra": {
      "recipe-maintainers": [
        "sodre",
        "matthiasdiener",
        "aywi"
      ]
    },
    "outputs": [
      {
        "build": null,
        "name": "byobu",
        "requirements": {
          "build": [
            "autoconf",
            "automake",
            "make"
          ],
          "host": [],
          "run": [
            "newt",
            "tmux >=1.5"
          ]
        },
        "tests": [
          {
            "script": [
              "set -ex",
              "byobu -c 'echo \"Hello conda-forge!\"'",
              "test \"$(command -v python)\" -ef \"$(env -u BYOBU_PYTHON byobu -c 'printenv BYOBU_PYTHON')\"",
              "test -x \"${CONDA_PREFIX}/bin/byobu-conda\"",
              "test -x \"${CONDA_PREFIX}/bin/byobu-disable-conda\"",
              "test -x \"${CONDA_PREFIX}/bin/byobu-enable-conda\"",
              "test \"$CONDA_PREFIX\" -ef \"$(env -i HOME=\"$HOME\" PATH=\"$PATH\" QEMU_LD_PREFIX=\"$QEMU_LD_PREFIX\" USER=\"$USER\" byobu -c 'printenv CONDA_PREFIX')\""
            ]
          }
        ],
        "version": "6.14"
      }
    ],
