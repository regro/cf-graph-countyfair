  "linux_64_meta_yaml": {
    "about": {
      "description": "Gyptis is a package to solve problems relevant in Electromagnetism and Photonics.\nIt is built on top of Gmsh <https://gmsh.info> for defining and meshing geometries and\nFEniCS <https://fenicsproject.org> to solve Maxwell's equations with the Finite Element Method.\n",
      "dev_url": "https://gitlab.com/gyptis/gyptis",
      "doc_url": "https://gyptis.gitlab.io",
      "home": "https://gitlab.com/gyptis/gyptis",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE.txt",
      "summary": "Computational Photonics in Python"
    },
    "build": {
      "number": "0",
      "script": "PYTHON -m pip install . -vv --no-deps"
    },
    "extra": {
      "recipe-maintainers": [
        "benvial"
      ]
    },
    "package": {
      "name": "gyptis",
      "version": "1.1.2"
    },
    "requirements": {
      "build": [
        "cxx_compiler_stub",
        "c_stdlib_stub",
        "cdt_stub"
      ],
      "host": [
        "python",
        "pip",
        "setuptools",
        "fenics",
        "numpy",
        "mpich",
        "xorg-libxfixes",
        "xorg-libxext",
        "xorg-libxrender",
        "xorg-libx11",
        "xorg-libxmu"
      ],
      "run": [
        "python",
        "fenics",
        "numpy",
        "dolfin-adjoint",
        "gmsh",
        "python-gmsh",
        "meshio",
        "scipy",
        "matplotlib-base",
        "nlopt",
        "psutil",
        "ipython",
        "joblib",
        "loguru",
        "mpich",
        "h5py * mpi_mpich_*"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "5dcdc627d40569fe9ee7c655532d6733f8a4c548ccf2fa114baad8c2eb93b7b9",
