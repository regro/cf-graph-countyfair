    "about": {
      "description": "Hypercorn is an ASGI web server based on the sans-io hyper, h11, h2,\nand wsproto libraries and inspired by Gunicorn. Hypercorn supports HTTP/1, HTTP/2,\nand websockets and the ASGI 2 specification. Hypercorn can utilise asyncio,\nuvloop, or trio worker types.\n",
      "doc_url": "https://hypercorn.readthedocs.io",
      "home": "https://github.com/pgjones/hypercorn",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "dist/LICENSE",
      "summary": "A ASGI Server based on Hyper libraries and inspired by Gunicorn."
    },
    "build": {
      "entry_points": [
        "hypercorn = hypercorn.__main__:main"
      ],
      "noarch": "python",
