    "about": {
      "description": "GridTest is a small python library that will read in one or more python\nscripts or modules, and generate a testing file that can be used to run grid\ntests. A \"gridtest\" is one that is run over a grid of parameter\nsettings. For each gridtest, you can also measure one or more metrics.\n",
      "dev_url": "https://github.com/vsoch/gridtest",
      "doc_url": "https://vsoch.github.io/gridtest",
      "home": "https://github.com/vsoch/gridtest",
      "license": "MPL-2.0",
      "license_family": "OTHER",
      "license_file": "LICENSE",
      "summary": "generate grid testing for Python modules and functions"
    },
    "build": {
      "entry_points": [
        "gridtest=gridtest.client:main"
      ],
      "noarch": "python",
      "number": "1",
      "script": "PYTHON -m pip install . --no-deps --ignore-installed -vv "
    },
    "extra": {
      "recipe-maintainers": [
        "vsoch"
      ]
    },
    "package": {
      "name": "gridtest",
      "version": "0.0.15"
    },
    "requirements": {
      "host": [
