    "about": {
      "description": "pip-system-certs patches pip and requests at runtime to use certificates\nfrom the default system store (rather than the bundled certs ca).\n\nThis will allow pip to verify tls/ssl connections to servers whose cert is\ntrusted by your system install.\n",
      "dev_url": "https://gitlab.com/alelec/pip-system-certs",
      "home": "https://gitlab.com/alelec/pip-system-certs",
      "license": "BSD-2-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE",
      "summary": "Live patches pip and requests to use system certs by default"
    },
    "build": {
      "noarch": "python",
      "number": "0",
      "script": "PYTHON -m pip install . -vv"
    },
