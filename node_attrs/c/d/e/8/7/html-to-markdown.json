  "raw_meta_yaml": "# yaml-language-server: $schema=https://raw.githubusercontent.com/prefix-dev/recipe-format/main/schema.json\nschema_version: 1\n\ncontext:\n  name: html-to-markdown\n  version: \"2.24.3\"\n  python_min: '3.10'\n\npackage:\n  name: ${{ name|lower }}\n  version: ${{ version }}\n\nsource:\n  url: https://pypi.org/packages/source/${{ name[0] }}/${{ name }}/html_to_markdown-${{ version }}.tar.gz\n  sha256: 24b18b27a63c6b378d93aca28183ecec4a56308f8c52b4f2e71d9e6c0a307d54\n\nbuild:\n  number: 0\n  script:\n    - if: unix\n      then:\n        # Unix recipe\n        - export OPENSSL_DIR=\"$PREFIX\"\n      else:\n        # Win recipe  # work around long path problem,\n        # hack: path too long for pixi_config subpackage, https://github.com/prefix-dev/pixi/issues/3691\n        - set CARGO_HOME=C:\\.cargo\n        - md %CARGO_HOME%\n        - if errorlevel 1 exit 1\n    - cargo-bundle-licenses --format yaml --output THIRDPARTY.yml\n    - ${{ PYTHON }} -m pip install . -vv --no-deps --no-build-isolation\n  python:\n    version_independent: true\n\nrequirements:\n  build:\n    - if: build_platform != target_platform\n      then:\n        - python\n        - cross-python_${{ target_platform }}\n        - crossenv\n        - maturin >=1.9,<2\n\n    - ${{ compiler(\"c\") }}\n    - ${{ compiler(\"rust\") }}\n    - ${{ stdlib('c') }}\n    - cargo-bundle-licenses\n  host:\n    - python\n    - maturin >=1.9,<2\n    - tomli >=2\n    - pip\n  run:\n    - python\n\ntests:\n  - python:\n      imports:\n        - html_to_markdown\n      pip_check: true\n  - requirements:\n      run:\n        - pip\n        - python\n    script:\n      - pip check\n      - \"python -c \\\"import html_to_markdown\\\"\"\n\nabout:\n  summary: High-performance HTML to Markdown converter powered by Rust with a clean Python API\n  license: MIT\n  license_file:\n    - LICENSE\n    - THIRDPARTY.yml\n  homepage: https://github.com/kreuzberg-dev/html-to-markdown\n\nextra:\n  recipe-maintainers:\n    - rxm7706\n",
