  "linux_64_meta_yaml": {
    "about": {
      "description": "Software to calculate crystal microscopic anharmonic properties\nfrom molecular dynamics (MD) using the normal-mode-decomposition\ntechnique. These properties include the phonon frequency shifts and\nlinewidths, as well as the renormalized force constanst and thermal\nproperties by using quasiparticle theory. This code includes\ninterfaces for MD outputs from VASP and LAMMPS. PHONOPY code is used\nto obtain harmonic phonon modes.\n",
      "dev_url": "https://github.com/abelcarreras/DynaPhoPy",
      "doc_url": "http://abelcarreras.github.io/DynaPhoPy/",
      "home": "http://abelcarreras.github.io/DynaPhoPy/",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "LICENSE",
      "summary": "Phonon Analysis from Molecular Dynamics Data"
    },
    "build": {
      "number": "2",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "jan-janssen"
      ]
    },
    "package": {
      "name": "dynaphopy",
      "version": "1.17.16"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub"
      ],
      "host": [
        "python",
        "pip",
        "numpy",
        "setuptools"
      ],
      "run": [
        "python",
        "pip",
        "phonopy",
        "scipy",
        "matplotlib-base"
      ]
    },
    "schema_version": 0,
    "source": {
      "patches": [
        "setup.patch"
      ],
