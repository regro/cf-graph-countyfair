      "doc_url": "https://kikuchipy.org",
      "home": "https://kikuchipy.org",
      "license": "GPL-3.0-or-later",
      "license_file": "LICENSE",
      "summary": "Processing, simulating, and indexing of electron backscatter diffraction (EBSD)\npatterns\n"
    },
    "build": {
      "noarch": "python",
      "number": "1"
    },
    "extra": {
      "recipe-maintainers": [
        "hakonanes"
      ]
    },
    "outputs": [
      {
        "build": {
          "noarch": "python",
          "script": "PYTHON -m pip install . --no-deps -vv"
        },
        "name": "kikuchipy-base",
        "requirements": {
          "host": [
            "python 3.10",
            "hatchling",
            "pip"
          ],
          "run": [
            "python >=3.10",
            "dask-core >=2021.8.1",
            "diffpy.structure >=3",
