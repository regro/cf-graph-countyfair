            "hyperspy-base >=2.2",
            "h5py >=2.10",
            "imageio",
            "matplotlib-base >=3.5",
            "numpy >=1.23.0",
            "numba >=0.57",
            "orix-base >=0.12.1",
            "pooch >=1.3.0",
            "pyyaml",
            "rosettasciio-base >=0.3.0",
            "scikit-image >=0.16.2",
            "scikit-learn",
            "scipy >=1.7",
            "tqdm >=0.5.2"
          ]
        },
        "test": {
          "commands": [
            "pip check"
          ],
          "requires": [
            "python 3.10",
            "pip"
          ],
          "source_files": [
            "tests",
            "conftest.py"
          ]
        }
      },
      {
        "build": {
          "noarch": "python"
        },
        "name": "kikuchipy",
        "requirements": {
          "host": [
