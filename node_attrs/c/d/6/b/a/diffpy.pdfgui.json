  "linux_64_meta_yaml": {
    "about": {
      "description": "The diffpy.pdfgui package provides a graphical user interface to pdffit2 refinement engine,\nwith many powerful extensions. It is a GUI program for structure refinements to atomic pair\ndistribution function. It is capable of full-profile fitting of the atomic pair distribution\nfunction derived from x-ray or neutron diffraction data and comes with built in graphical and\nstructure visualization capabilities.\n",
      "dev_url": "https://github.com/diffpy/diffpy.pdfgui",
      "doc_url": "https://www.diffpy.org/products/pdfgui.html",
      "home": "https://github.com/diffpy/diffpy.pdfgui",
      "license": "Free To Use But Restricted",
      "license_file": "LICENSE.rst",
      "summary": "GUI for PDF simulation and structure refinement"
    },
    "app": {
      "entry": "pdfgui",
      "icon": "pdfgui_64x64.png",
      "summary": "materials structure analysis from Pair Distribution Function"
    },
    "build": {
      "entry_points": [
        "pdfgui = diffpy.pdfgui.applications.pdfgui:main"
      ],
      "number": "1",
      "osx_is_app": true,
      "script": "PYTHON -m pip install . --no-deps -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "bobleesj",
        "dragonyanglong",
        "sbillinge"
      ]
    },
    "package": {
      "name": "diffpy.pdfgui",
      "version": "3.1.0"
    },
    "requirements": {
      "host": [
        "python",
        "setuptools",
        "setuptools-git-versioning >=2.0",
        "pip"
      ],
      "run": [
        "python",
