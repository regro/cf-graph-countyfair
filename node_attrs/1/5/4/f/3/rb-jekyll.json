      "license_file": "jekyll-4.0.0/LICENSE",
      "summary": "Jekyll is a simple, blog aware, static site generator"
    },
    "build": {
      "noarch": "generic",
      "number": "0",
      "script": [
        "gem install --norc -N -l -V --ignore-dependencies jekyll-4.0.0.gem",
