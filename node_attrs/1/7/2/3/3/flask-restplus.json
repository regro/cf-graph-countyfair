  "linux_64_meta_yaml": {
    "about": {
      "description": "Flask-RESTPlus is an extension for Flask that adds support for quickly\nbuilding REST APIs. Flask-RESTPlus encourages best practices with minimal\nsetup. If you are familiar with Flask, Flask-RESTPlus should be easy to\npick up. It provides a coherent collection of decorators and tools to\ndescribe your API and expose its documentation properly using Swagger.\n",
      "dev_url": "https://github.com/noirbizarre/flask-restplus",
      "doc_url": "http://flask-restplus.readthedocs.org/",
      "home": "https://github.com/noirbizarre/flask-restplus",
      "license": "MIT",
      "license_family": "MIT",
      "license_file": "RECIPE_DIR/LICENSE",
      "summary": "Fully featured framework for fast, easy and documented API development with Flask"
    },
    "build": {
      "number": "3",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "frol"
      ]
    },
    "package": {
      "name": "flask-restplus",
      "version": "0.13.0"
    },
    "requirements": {
      "host": [
        "python",
        "pip"
      ],
      "run": [
        "python",
        "flask >=0.8",
        "six >=1.3.0",
        "jsonschema",
        "pytz",
        "aniso8601 >=0.82",
        "werkzeug =0.16.0"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "a66e442d0bca08f389fc3d07b4d808fc89961285d12fb8013f7cf15516fa9f5c",
      "url": "https://pypi.io/packages/source/f/flask-restplus/flask-restplus-0.13.0.tar.gz"
    },
    "test": {
      "commands": [
        "test -f \"${SP_DIR}/flask_restplus/static/swagger-ui.js\"",
        "test -f \"${SP_DIR}/flask_restplus/static/swagger-ui.css\""
      ],
      "imports": [
        "flask_restplus"
      ]
    }
  },
