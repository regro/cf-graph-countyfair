    "about": {
      "description": "LOOS is a C++ library for developing new tools to analyze molecular\ndynamics simulations. It is designed to provide good performance while\ngreatly simplifying the process of creating new methods, particularly when\nusing its python interface. This package contains roughly 100 useful tools,\nas well as the python bindings needed to create new tools. If you wish to\nwrite a new C++ tool or modify the library itself, we recommend you instead\nclone LOOS from GitHub.\n",
      "dev_url": "https://github.com/GrossfieldLab/loos",
      "doc_url": "https://grossfieldlab.github.io/loos",
      "home": "https://github.com/GrossfieldLab/loos",
      "license": "GPL-3.0-only",
      "license_family": "GPL3",
      "license_file": "COPYING",
      "summary": "The Lightweight Object Oriented Structure analysis Library"
    },
    "build": {
      "number": "3"
    },
    "extra": {
      "recipe-maintainers": [
        "agrossfield",
        "tromo"
      ]
    },
    "package": {
      "name": "loos",
      "version": "4.2.0.1"
    },
    "requirements": {
      "build": [
        "cxx_compiler_stub",
        "c_stdlib_stub",
        "cmake",
        "make",
        "swig",
        "python"
      ],
      "host": [
        "python",
        "pip",
        "numpy",
        "libboost-devel",
        "openblas",
        "libnetcdf",
        "liblapack",
        "eigen",
        "gemmi",
        "zlib",
