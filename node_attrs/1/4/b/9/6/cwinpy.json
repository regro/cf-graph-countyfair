      "branch_name": "main",
      "tooling_branch_name": "main"
    },
    "test": "native_and_emulated"
  },
  "feedstock_name": "cwinpy",
  "hash_type": "sha256",
  "linux_64_meta_yaml": {
    "about": {
      "description": "A Python module for performing Bayesian inference for\ncontinuous gravitational-wave signals from pulsars.\n",
      "dev_url": "https://git.ligo.org/cwinpy/cwinpy",
      "doc_url": "https://cwinpy.readthedocs.io/",
      "home": "https://github.com/cwinpy/cwinpy/",
      "license": "MIT",
      "license_file": "LICENSE",
      "summary": "CW Inference in Python"
    },
    "build": {
      "entry_points": [
        "cwinpy_pe = cwinpy.pe.pe:pe_cli",
        "cwinpy_pe_pipeline = cwinpy.pe.pe:pe_pipeline_cli",
        "cwinpy_pe_generate_pp_plots = cwinpy.pe.testing:generate_pp_plots"
      ],
      "number": "3",
      "script": "PYTHON -m pip install . -vv"
    },
    "extra": {
      "recipe-maintainers": [
        "mattpitkin",
        "duncanmmacleod"
      ]
    },
    "package": {
      "name": "cwinpy",
      "version": "1.4.0"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub"
      ],
      "host": [
        "cython >=3",
        "numpy",
        "pip",
        "python",
        "setuptools >=61",
        "setuptools_scm >=6.2"
      ],
      "run": [
        "appdirs",
        "arby",
        "astropy-base",
        "bilby >=2.0.1",
        "configargparse",
        "corner >=2.2.1",
        "gwosc >=0.5.4",
        "gwpy >=2.1.1",
        "healpy",
        "lintegrate >=0.1.7",
        "matplotlib-base >=3.6.0",
        "numba >=0.5.0",
        "numpy >=1.17.0",
        "pesummary >=1.2.0",
        "psrqpy >=1.1.7",
        "python",
        "python-htcondor >=10.0.0",
        "python-lal >=7.2.2",
        "python-lalframe >=2.0.3",
        "python-lalpulsar >=5.0.2",
        "requests",
        "scipy",
        "simpleeval",
        "solar_system_ephemerides >=1.0.0"
      ]
    },
    "schema_version": 0,
    "source": {
      "patches": [
        "patches/0001-Remove-lalsuite-Python-requirement.patch",
        "patches/0002-Handle-no-internet-connection-in-test.patch"
      ],
      "sha256": "f047c6ec0c82a57a68d232c57e7cd29e297c925a6565d1ed9536a0e4645f137d",
      "url": "https://pypi.org/packages/source/c/cwinpy/cwinpy-1.4.0.tar.gz"
    },
    "test": {
      "commands": [
        "python -m pip check",
        "python -m pytest --pyargs cwinpy.test -ra",
        "cwinpy_pe --help",
        "cwinpy_pe_pipeline --help",
        "cwinpy_pe_generate_pp_plots --help"
      ],
      "requires": [
        "arby",
        "cweqgen >=0.4.4",
        "healpy",
        "httpretty",
        "libstempo >=2.4.2",
        "lalapps",
        "pesummary >=1.3.2",
        "pip",
        "pytest >=4.6",
        "pytest-astropy",
        "pytest-socket",
        "seaborn"
      ]
    }
  },
