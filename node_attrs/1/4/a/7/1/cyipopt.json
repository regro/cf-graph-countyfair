  "linux_64_meta_yaml": {
    "about": {
      "description": "Ipopt (Interior Point OPTimizer, pronounced eye-pea-Opt) is a software\npackage for large-scale nonlinear optimization. cyipopt is a python wrapper\naround Ipopt. It enables using Ipopt from the comfort of the great Python\nscripting language. Ipopt is available from the COIN-OR initiative, under\nthe Eclipse Public License (EPL).\n",
      "dev_url": "https://github.com/mechmotum/cyipopt",
      "doc_url": "https://cyipopt.readthedocs.io",
      "home": "https://github.com/mechmotum/cyipopt",
      "license": "EPL-2.0",
      "license_file": "LICENSE",
      "summary": "A Cython wrapper to the IPOPT optimization package"
    },
    "build": {
      "number": "3",
      "script": "/bin/rm -f pyproject.toml && PYTHON -m pip install . -vv --no-deps --no-build-isolation"
    },
    "extra": {
      "recipe-maintainers": [
        "moorepants"
      ]
    },
    "package": {
      "name": "cyipopt",
      "version": "1.5.0"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub",
        "pkg-config"
      ],
      "host": [
        "cython >=0.29.28",
        "ipopt",
        "libblas",
        "liblapack",
        "numpy >=1.25",
        "pip",
        "python",
        "setuptools >=41.1.1"
      ],
      "run": [
        "ipopt",
        "numpy >=1.21.5",
        "python"
      ],
      "run_constrained": [
        "scipy >=1.8.0"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "8c6f9c1081aa754c6f84d9f2842bd0d4e4ad466a567f0d4675b124d681eec1dc",
      "url": "https://pypi.org/packages/source/c/cyipopt/cyipopt-1.5.0.tar.gz"
    },
    "test": {
