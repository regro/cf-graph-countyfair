      "license_file": "src/LICENSE",
      "summary": "Pin GitHub Actions versions"
    },
    "build": {
      "number": "0",
      "script": [
        "cd src",
        "go-licenses save ./cmd/pinact --save_path ../library_licenses",
        "go build -v -o $PREFIX/bin/pinact -ldflags=\"-s -w\" ./cmd/pinact"
      ]
    },
    "extra": {
