      "summary": "M2Crypto: A Python crypto and SSL toolkit"
    },
    "build": {
      "number": "0",
      "script": [
        "sed -i.bak \"/sys\\/types.h/d\" $PREFIX/include/openssl/e_os2.h",
        "OPENSSL_PATH=$PREFIX PYTHON -m pip install . -vv",
        "mv $PREFIX/include/openssl/e_os2.h{.bak,}"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "chrisburr"
      ]
    },
    "package": {
      "name": "m2crypto",
      "version": "0.43.0"
    },
    "requirements": {
