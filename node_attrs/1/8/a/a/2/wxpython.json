  "linux_64_meta_yaml": {
    "about": {
      "description": "Welcome to wxPython's Project Phoenix! Phoenix is the improved next-generation\nwxPython, \"better, stronger, faster than he was before.\" This new\nimplementation is focused on improving speed, maintainability and\nextensibility. Just like \"Classic\" wxPython, Phoenix wraps the wxWidgets C++\ntoolkit and provides access to the user interface portions of the wxWidgets\nAPI, enabling Python applications to have a native GUI on Windows, Macs or\nnot win systems, with a native look and feel and requiring very little (if any)\nplatform specific code.\n",
      "dev_url": "https://github.com/wxWidgets/Phoenix",
      "doc_url": "https://docs.wxpython.org/",
      "home": "https://wxpython.org/",
      "license": "LGPL-2.0-or-later WITH WxWindows-exception-3.1",
      "license_family": "LGPL",
      "license_file": "LICENSE.txt",
      "summary": "Cross platform GUI toolkit for Python, \"Phoenix\" version"
    },
    "build": {
      "number": "3"
    },
    "extra": {
      "recipe-maintainers": [
        "ocefpaf",
        "constantinpape",
        "timsnyder",
        "pauldmccarthy"
      ]
    },
    "package": {
      "name": "wxpython",
      "version": "4.2.3"
    },
    "requirements": {
      "build": [
        "gnuconfig",
        "make",
        "pkg-config",
        "sip 6.10.0",
        "setuptools",
        "c_compiler_stub",
        "cxx_compiler_stub",
        "c_stdlib_stub"
      ],
      "host": [
        "python",
        "pip",
        "attrdict3",
        "cython",
        "expat",
        "gtk3",
        "libgl-devel",
        "libglx-devel",
        "requests",
        "setuptools",
        "wxwidgets 3.2",
        "xorg-xineramaproto",
