    },
    "outputs": [
      {
        "build": null,
        "name": "zensical",
        "requirements": {
          "build": [
            "c_stdlib_stub",
            "c_compiler_stub",
            "rust_compiler_stub",
            "cargo-bundle-licenses",
            "cargo-auditable"
          ],
          "host": [
            "python",
            "python-abi3",
            "pip",
            "maturin >=1.8,<2.0"
          ],
          "run": [
            "python",
            "click >=8.1.8",
            "deepmerge >=2.0",
            "markdown >=3.7",
            "pygments >=2.16",
            "pymdown-extensions >=10.15",
            "pyyaml >=6.0.2",
            "tomli >=2.0"
          ]
        },
        "tests": [
          {
            "python": {
              "imports": [
                "zensical"
              ],
              "python_version": "3.10.*"
            }
          },
          {
            "package_contents": {
              "site_packages": [
                "zensical/bootstrap/.github/workflows/docs.yml",
                "zensical-0.0.16.dist-info/*"
              ]
            }
          },
          {
            "requirements": {
              "run": [
                "abi3audit"
              ]
            },
            "script": [
              "export SP_DIR=$(python -c \"import site; print(site.getsitepackages()[0])\")",
