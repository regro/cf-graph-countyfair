  "linux_64_meta_yaml": {
    "about": {
      "description": "ReaDDy (standing for [Rea]ction [D]iffusion [Dy]namics) is a particle based\nreaction-diffusion simulator that can be configured and run with Python 3.6+.\nThe Python interface wraps around a C++ implementation for good performance.\nTrajectories are written using the flexible HDF5 format.\n",
      "dev_url": "https://github.com/readdy/readdy",
      "doc_url": "https://readdy.github.io/",
      "home": "https://github.com/readdy/readdy",
      "license": "BSD-3-Clause",
      "license_family": "BSD",
      "license_file": "LICENSE",
      "summary": "Python / C++ based particle reaction-diffusion simulator"
    },
    "build": {
      "number": "0",
      "rpaths": [
        "lib/"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "clonker",
        "chrisfroe"
      ]
    },
    "package": {
      "name": "readdy",
      "version": "2.0.13"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub",
        "cxx_compiler_stub",
        "cmake",
        "ninja",
        "spdlog >=1.15.0",
        "nlohmann_json >=3.12.0",
        "fmt >=11.2.0",
        "pybind11 >=2.13.6"
      ],
      "host": [
        "python",
        "h5py",
        "hdf5",
        "zlib 1.2.*",
        "blosc 1.21.1",
        "numpy >=1.14"
      ],
      "run": [
        "python",
        "numpy",
        "h5py",
        "pint",
        "tqdm",
