  "linux_64_meta_yaml": {
    "about": {
      "description": "ASAP is a tool for doing atomic-scale computer simulations\n(mainly molecular dynamics) using classical potentials\n(mainly Effective Medium Theory). It is intended to work\nas an energy calculator in connection with the Atomic Simulation\nEnvironment (ASE). However, some parts of the ASE are either\nextended or reimplemented for reasons of efficiency and\nease-of-use on parallel computers.\n",
      "dev_url": "https://gitlab.com/asap/asap",
      "doc_url": "https://wiki.fysik.dtu.dk/asap/Manual",
      "home": "https://wiki.fysik.dtu.dk/asap",
      "license": "LGPL-3.0-only",
      "license_family": "LGPL",
      "license_file": "LICENSE.txt",
      "summary": "ASAP is a calculator for doing large-scale classical molecular dynamics within ASE."
    },
    "build": {
      "number": "0",
      "script": [
        "echo \"remove_compile_args += ['-march=native']\" >> customize.py",
        "PYTHON -m pip install . -vv",
        "echo \"libraries += ['mpi']\" >> customize.py"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "jochym"
      ]
    },
    "package": {
      "name": "asap3",
      "version": "3.13.6"
    },
    "requirements": {
      "build": [
        "c_compiler_stub",
        "c_stdlib_stub",
        "cxx_compiler_stub"
      ],
      "host": [
        "python",
        "pip",
        "numpy",
        "openblas",
        "kim-api",
        "setuptools",
        "openmpi"
      ],
      "run": [
        "python",
        "openblas",
        "ase >=3.24",
        "kim-api",
        "openmpi"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "28576d42311e721c2af13cd677ffb40ce05c07b49c9d8d925fae62d19db7fdee",
      "url": "https://pypi.org/packages/source/a/asap3/asap3-3.13.6.tar.gz"
    },
    "test": {
      "commands": [
        "cp Verlet*.pickle hostfile Test",
        "cd Test",
        "sed -i'.bak' -e 's/np.float)/float)/g' *.py parallel/*.py",
        "python TestAll.py",
        "mpirun -np 2 --hostfile hostfile python -W error::FutureWarning TestAll.py --parallel"
      ],
      "files": [
        "Verlet*.pickle",
        "hostfile"
      ],
      "imports": [
        "asap3"
      ],
      "requires": [
        "openmpi"
      ],
      "source_files": [
        "Test"
      ]
    }
  },
