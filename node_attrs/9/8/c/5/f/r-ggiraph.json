  "raw_meta_yaml": "{% set version = \"0.8.12\" %}\n{% set posix = 'm2-' if win else '' %}\n\npackage:\n  name: r-ggiraph\n  version: {{ version|replace(\"-\", \"_\") }}\n\nsource:\n  url:\n    - {{ cran_mirror }}/src/contrib/ggiraph_{{ version }}.tar.gz\n    - {{ cran_mirror }}/src/contrib/Archive/ggiraph/ggiraph_{{ version }}.tar.gz\n  sha256: a8544fad40e68174bd0e753ecb3fc0afd840adf0fefa45231d6f5326a8a3b557\n\nbuild:\n  number: 1\n  # Checking windows to see if it passes. Uncomment the line if it fails.\n  # skip: true  # [win]\n  rpaths:\n    - lib/R/lib/\n    - lib/\n\nrequirements:\n  build:\n    - {{ compiler('c') }}              # [not win]\n    - {{ stdlib(\"c\") }}                # [not win]\n    - {{ compiler('m2w64_c') }}        # [win]\n    - {{ stdlib(\"m2w64_c\") }}          # [win]\n    - {{ compiler('cxx') }}            # [not win]\n    - {{ compiler('m2w64_cxx') }}      # [win]\n    - {{ posix }}filesystem        # [win]\n    - {{ posix }}sed               # [win]\n    - {{ posix }}grep              # [win]\n    - {{ posix }}autoconf\n    - {{ posix }}automake          # [not win]\n    - {{ posix }}automake-wrapper  # [win]\n    - {{ posix }}pkg-config\n    - {{ posix }}make\n    - {{ posix }}coreutils         # [win]\n    - {{ posix }}zip               # [win]\n    - cross-r-base {{ r_base }}    # [build_platform != target_platform]\n    - r-rcpp >=1.0                 # [build_platform != target_platform]\n    - r-cli                        # [build_platform != target_platform]\n    - r-ggplot2 >=3.5.0            # [build_platform != target_platform]\n    - r-htmltools                  # [build_platform != target_platform]\n    - r-htmlwidgets >=1.5          # [build_platform != target_platform]\n    - r-purrr                      # [build_platform != target_platform]\n    - r-rlang                      # [build_platform != target_platform]\n    - r-systemfonts                # [build_platform != target_platform]\n    - r-uuid                      