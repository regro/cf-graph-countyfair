    "about": {
      "description": "HoloViews is an open-source Python library designed to make data analysis\nand visualization seamless and simple. With HoloViews, you can usually\nexpress what you want to do in very few lines of code, letting you focus on\nwhat you are trying to explore and convey, not on the process of plotting.\n",
      "dev_url": "https://github.com/holoviz/holoviews",
      "doc_url": "http://holoviews.org/getting_started/index.html",
      "home": "https://holoviews.org",
      "license": "BSD-3-Clause",
      "license_file": "LICENSE.txt",
      "summary": "Stop plotting your data - annotate your data and let it visualize itself."
    },
    "build": {
      "entry_points": [
        "holoviews = holoviews.util.command:main"
      ],
      "noarch": "python",
      "number": "0",
      "script": "PYTHON -m pip install . --no-deps -vv"
    },
    "extra": {
      "recipe-maintainers": [
