    },
    "outputs": [
      {
        "build": {
          "run_exports": [
            "libpmix"
          ]
        },
        "files": {
          "exclude": [
            "bin/",
            "include/",
            "lib/pkgconfig/",
            "share/doc/",
            "share/man/"
          ],
          "include": [
            "**/*"
          ]
        },
        "name": "libpmix",
        "requirements": {
          "build": [
            "c_compiler_stub",
            "c_stdlib_stub",
            "make",
            "pkg-config",
