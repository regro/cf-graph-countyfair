        "name": "pmix-bin",
        "requirements": {
          "build": [
            "c_compiler_stub",
            "c_stdlib_stub"
          ],
          "host": [
            "libpmix"
          ],
          "run": [
            "libpmix"
          ]
        },
        "test": {
          "commands": [
            "pmix_info"
          ]
        }
      }
    ],
    "package": {
      "name": "openpmix-feedstock",
      "version": "6.0.0"
