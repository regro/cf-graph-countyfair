  "feedstock_name": "photospline",
  "hash_type": "sha256",
  "linux_64_meta_yaml": {
    "about": {
      "description": "Detector response to a high-energy physics process is often estimated by\nMonte Carlo simulation. For purposes of data analysis, the results of this\nsimulation are typically stored in large multi-dimensional histograms,\nwhich can quickly become unwieldy in terms of size or numerically\nproblematic due to unfilled bins or interpolation artifacts. Photospline is\na library that uses the penalized spline technique to efficiently compute,\nstore, and evaluate B-spline representations of such tables.\n",
      "dev_url": "https://github.com/icecube/photospline",
      "home": "https://github.com/icecube/photospline",
      "license": "BSD-3-Clause",
      "license_family": "BSD",
      "license_file": "COPYING.txt",
      "summary": "Tensor product B-spline fitting and evaluation"
    },
    "build": {
      "number": "2",
      "run_exports": [
        "photospline"
      ]
    },
    "extra": {
      "recipe-maintainers": [
        "jvansanten"
      ]
    },
    "package": {
      "name": "photospline",
      "version": "2.4.1"
    },
    "requirements": {
      "build": [
        "cmake >=3.1,<4",
        "make",
        "c_compiler_stub",
        "c_stdlib_stub",
        "cxx_compiler_stub"
      ],
      "host": [
        "python",
        "numpy",
        "suitesparse",
        "cfitsio"
      ],
      "run": [
        "python",
        "suitesparse",
        "cfitsio"
      ]
    },
    "schema_version": 0,
    "source": {
      "sha256": "c8bfd2a087300f3f217cecfe3e4354be4e2a485dfc503420c8ebbffeec5adf03",
      "url": "https://github.com/icecube/photospline/archive/v2.4.1.tar.gz"
    },
    "test": {
      "commands": [
        "timeout 10 python test/test_fit.py || (stat=$?; test $stat -eq 124 || exit $stat)",
        "photospline-config --help",
        "photospline-eval test/test_data/test_spline_3d_nco.fits 0 0 0",
        "photospline-inspect test/test_data/test_spline_3d_nco.fits"
      ],
      "imports": [
        "photospline"
      ],
      "requires": [
        "coreutils"
      ],
      "source_files": [
        "test"
      ]
    }
  },
