  "linux_64_meta_yaml": {
    "about": {
      "description": "A fork of Intel's Hyperscan, modified to run on more platforms. Currently\nARM NEON/ASIMD and Power VSX are 100% functional. ARM SVE2 support is in\nongoing with access to hardware now. More platforms will follow in the\nfuture. Further more, starting 5.4.12 there is now a SIMDe port, which can\nbe either used for platforms without official SIMD support, as SIMDe can\nemulate SIMD instructions, or as an alternative backend for existing\narchitectures, for reference and comparison purposes.",
      "doc_url": "http://intel.github.io/hyperscan/dev-reference/",
      "home": "https://www.vectorcamp.gr/project/vectorscan/",
      "license": "BSD-3-Clause",
      "license_family": "BSD-3-Clause",
      "license_file": "vectorscan/COPYING",
      "summary": "A portable fork of the high-performance regular expression matching library"
    },
    "build": {
      "number": "1",
      "script": "tar xvf pcre-${{ pcre_version }}.tar.gz\nmkdir build/\ncd build/\ncmake $CMAKE_ARGS -GNinja -DPCRE_SOURCE=$SRC_DIR/pcre-8.45 $SRC_DIR/vectorscan\ncmake --build . -- -j${CPU_COUNT}\ncmake --build . --target install\n"
    },
    "extra": {
      "recipe-maintainers": [
        "chrisburr"
      ]
    },
    "outputs": [
